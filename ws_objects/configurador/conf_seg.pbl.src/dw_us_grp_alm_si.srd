 retrieve="SELECT 
	seggrupo.codgrupo, seggrupo.descripcion, usuarios.usuario,
	usuarios.nombre,'0' AS esco,seggrupo_alm.codalmacen
FROM 
	(usuarios INNER JOIN (seggrupo 
	INNER JOIN usugrupo ON seggrupo.codgrupo = usugrupo.codgrupo) ON usuarios.usuario = usugrupo.usuario) 
	INNER JOIN seggrupo_alm ON (usugrupo.usuario = seggrupo_alm.usuario) AND (usugrupo.codgrupo = seggrupo_alm.codgrupo)
WHERE 
	(((usuarios.estado)='1') AND ((seggrupo_alm.codalmacen)=:alm ));
" update="seggrupo_alm" updatewhere=0 updatekeyinplace=no arguments=(("alm", string))  sort="descripcion A nombre A " )
