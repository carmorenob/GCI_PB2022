 retrieve="SELECT 
	capitulos.cd_capitulo,capitulos.cd_seccion,pgrupos.cd_grupo
FROM 
	capitulos INNER JOIN pgrupos ON (capitulos.CD_CAPITULO = pgrupos.CD_CAPITULO) 
	AND (capitulos.COD_VERSION = pgrupos.COD_VERSION)
WHERE 
	(((capitulos.cd_seccion)=:codseccion));
" arguments=(("codseccion", string)) )
