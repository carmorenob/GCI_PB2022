 retrieve="SELECT  plancate.codplan ,plancate.cod_version,plancate.cd_grupo,plancate.cd_subgrupo,plancate.cd_categoria,pcategorias.de_categoria,plancate.todos     
FROM pcategorias inner join plancate  on ( ( pcategorias.cod_version = plancate.cod_version) and  ( pcategorias.cd_grupo = plancate.cd_grupo ) 
and  ( pcategorias.cd_subgrupo = plancate.cd_subgrupo ) and          ( pcategorias.cd_categoria = plancate.cd_categoria )  )
WHERE( ( plancate.codplan = :plan )  and ( plancate.cod_version=:versi)) 
ORDER BY pcategorias.de_categoria          ASC  " update="plancate" updatewhere=0 updatekeyinplace=yes arguments=(("versi", string),("plan", string))  sort="codplan A cod_version A cd_grupo A cd_subgrupo A cd_categoria A " )
