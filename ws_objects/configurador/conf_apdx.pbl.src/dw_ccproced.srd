 retrieve="SELECT  
	ccproced.coduf,ccproced.codcc,ccproced.cod_version,ccproced.cd_grupo,ccproced.cd_subgrupo,ccproced.cd_categoria,
	ccproced.codproced,proced.descripcion,proced.cod_cups
FROM 
	ccproced,proced     
WHERE 
	( ccproced.codproced = proced.codproced ) and     ( ccproced.cod_version = proced.cod_version ) 
	and ( ( ccproced.coduf = :uf ) And ( ccproced.codcc = :cc ) And ( ccproced.cod_version = :versi ) And ( ccproced.cd_grupo = :grup ) 
	And ( ccproced.cd_subgrupo = :subgrp ) And ( ccproced.cd_categoria = :categ ) )  " update="ccproced" updatewhere=0 updatekeyinplace=no arguments=(("versi", string),("uf", string),("cc", string),("grup", string),("subgrp", string),("categ", string)) )
