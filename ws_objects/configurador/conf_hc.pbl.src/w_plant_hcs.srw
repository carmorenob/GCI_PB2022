event clicked;if dw_lista.rowcount()=0 then return
st_xa_elegir st
st.dw_object='dw_ls_plants'
st.titulo='Elegir plantilla para copiar campos'
st.ancho=1800
st.codplantilla=dw_lista.getitemstring(dw_lista.getrow(),'codplantilla')
openwithparm(w_buscar_gral,st)
if message.stringparm='' then return
string cplant
cplant=message.stringparm
if dw_save_campo.RowCount() > 0 then
	if MessageBox('Atención','La plantilla tiene campos, estos se reemplazarán. Desea continuar?',QUESTION!,YESNO!,2) = 2 then Return
end if

long i,j
string temp,nulo
setnull(nulo)
uo_datastore dw_c
dw_c = create uo_datastore
dw_c.DataObject = 'dw_campos_plant'
dw_c.SetTransObject(SQLCA)

if dw_save_campo.RowCount() > 0 then
	do while dw_save_campo.RowCount()>0
		dw_save_campo.DeleteRow(0)
	loop
	dw_l.setfilter('')
	dw_l.filter()
	do while dw_l.rowcount()>0
		dw_l.deleterow(0)
	loop
	if dw_l.update(true,false)=-1 then
		rollback;
		return
	end if
	if dw_save_campo.Update(true,false) = -1 then
		Rollback;
		Return
	end if
	dw_l.resetupdate()
	dw_save_campo.resetupdate()
	tv_1.event reset()
end if

if dw_c.Retrieve(cplant)>0 then
	dw_c.rowscopy(1,dw_c.rowcount(),primary!,dw_save_campo,1,primary!)
	for j=1 to dw_save_campo.rowcount()
		dw_save_campo.setitem(j,'codplantilla',tab_1.tp_1.t_p.p_1.dw_deta.getitemstring(1,'codplantilla'))
	next
	temp=i_cplant
	i_cplant=cplant
	dw_l.retrieve(i_cplant)
	for j=1 to dw_l.rowcount()
		dw_l.setitemstatus(j,0,primary!,new!)
		dw_l.setitem(j,'codplantilla',tab_1.tp_1.t_p.p_1.dw_deta.getitemstring(1,'codplantilla'))
	next
	tv_1.event rec_inserta(0,0)
	i_cplant=temp
end if
i_cambio=true
end event

