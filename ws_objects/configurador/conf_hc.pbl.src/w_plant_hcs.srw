if hand=-1 then return
do while hand > 0
	expandall(hand)
	getitem(hand,tvl)
	donde=dw_save_campo.find('numcampo='+string(tvl.data),1,dw_save_campo.rowcount())
	dw_save_campo.setitem(donde,'orden',orden)
