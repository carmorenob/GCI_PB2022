event constructor;GetChild('fuente',dwc_f)
environment env
String 	Ruta, Fon[] ,type_obj,ee
long rtn, i, j, f

rtn = GetEnvironment(env)
Choose Case Env.OSType
	Case Windows!
	  If Env.OSMajorRevision = 4 Or Env.OSMinorRevision = 95 Then
		Ruta="HKEY_LOCAL_MACHINE\Software\Microsoft\Windows\CurrentVersion\Fonts"
	  Else
		 Ruta="2"
	  End If			
	Case windowsnt!
		Ruta="HKEY_LOCAL_MACHINE\Software\Microsoft\Windows NT\CurrentVersion\Fonts"
End Choose
RegistryValues(Ruta,Fon)
For i=1 To UpperBound(Fon)
	j = Pos(fon[i]," (")
	If j<>0 Then
		f = dwc_f.InsertRow(0)
		dwc_f.SetItem(f,1, Mid(Fon[i],1,j - 1))		
	End If
Next
InsertRow(0)

end event

event itemchanged;string obj,ret
obj = dw_p.GetItemString(1,'nombre')
if obj = '' then Return
if acceptText() = -1 then Return
choose case GetColumnName()
	case 'x'
		ret = dw_base.Modify(obj+'.x='+String(GetItemNumber(1,'x')))
	case 'y'
		dw_base.Modify(obj+'.y='+String(GetItemNumber(1,'y')))
	case 'alto'
		dw_base.Modify(obj+'.height='+String(GetItemNumber(1,'alto')))
	case 'ancho'
		dw_base.Modify(obj+'.width='+String(GetItemNumber(1,'ancho')))
	case 'fuente'
		dw_base.Modify(obj+'.font.face="'+GetItemString(1,'fuente')+'"')
	case 'tamletra'
		dw_base.Modify(obj+'.font.height="-'+string(GetItemNumber(1,'tamletra'))+'"')
	case 'resaltado'
		dw_base.Modify(obj+'.font.weight='+string(GetItemNumber(1,'resaltado')))
	case 'alineacion'
		dw_base.Modify(obj+'.alignment='+string(GetItemNumber(1,'alineacion')))
	case 'texto'
		dw_base.Modify(obj+'.text="'+GetItemString(1,'texto')+'"')
		
end choose

end event

