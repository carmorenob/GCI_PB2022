end if
if  tab_1.tp_1.t_p.p_2.dw_edad.update(true,false)=-1 then
	Rollback;
	i_operacion = ''
	Return -1
end if
i_operacion = ''
if tab_1.tp_1.dw_lista.rowcount()>0 and tab_1.tp_1.t_p.p_1.dw_deta.rowcount()>0 then
	if tab_1.tp_1.dw_lista.getitemstatus(1,0,primary!)=new! then
		tab_1.tp_1.dw_lista.setitem(1,'codplantilla',tab_1.tp_1.t_p.p_1.dw_deta.getitemstring(1,'codplantilla'))
		tab_1.tp_1.dw_lista.setitem(1,'desplantilla',tab_1.tp_1.t_p.p_1.dw_deta.getitemstring(1,'desplantilla'))
		tab_1.tp_1.dw_lista.setitem(1,'estado',tab_1.tp_1.t_p.p_1.dw_deta.getitemstring(1,'estado'))		
		tab_1.tp_1.dw_lista.resetupdate()
	end if
end if
tab_1.tp_1.t_p.p_1.dw_deta.resetupdate()
tab_1.tp_1.dw_save_campo.resetupdate()
tab_1.tp_1.dw_l.resetupdate()
tab_1.tp_1.dw_lista.resetupdate()
i_cambio=false
commit;
tab_1.tp_1.dw_lista.triggerevent(rowfocuschanged!)
