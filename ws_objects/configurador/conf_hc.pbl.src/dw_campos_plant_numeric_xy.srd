 retrieve="  SELECT hclin_plantcampo.codplantilla,   
         hclin_plantcampo.numcampo,   
         hclin_plantcampo.descampo,   
         '' as XY  
FROM hclin_plantcampo INNER JOIN hclin_plant ON hclin_plantcampo.codplantilla = hclin_plant.codplantilla
WHERE 
	( hclin_plantcampo.codplantilla = hclin_plant.codplantilla ) and  
     ( ( hclin_plantcampo.codplantilla = :cplant ) AND  
     ( hclin_plantcampo.tipo = 'C' or  hclin_plantcampo.tipo = 'R' ) )    
" arguments=(("cplant", string))  sort="numcampo A " )
