 column=(type=char(255) updatewhereclause=yes name=tipodoc_ter dbname="pre_docu_cb.tipodoc_ter" )
 column=(type=char(255) updatewhereclause=yes name=documento_ter dbname="pre_docu_cb.documento_ter" )
 column=(type=char(255) updatewhereclause=yes name=razon_social dbname="pre_docu_cb.razon_social" )
 column=(type=char(255) updatewhereclause=yes name=cod_concep dbname="pre_docu_cb.cod_concep" )
 retrieve="SELECT 0 AS esco, v_nom_aportes.DES_CONCEP, v_nom_aportes.NUM_NOMINA, Sum(mod_relacion_origen.valor) AS tbruto, v_nom_aportes.TipoDoc_TER, v_nom_aportes.documento_TER, v_nom_aportes.razon_social, v_nom_aportes.COD_CONCEP
FROM (((SELECT DISTINCT NOM_CPO.NUM_NOMINA, NOM_CPO.COD_CONCEP, NOM_CLASE_TERCERO.TipoDoc_TER, NOM_CLASE_TERCERO.documento_TER, NOM_CONCEP.DES_CONCEP, terceros.razon_social
FROM (NOM_CLASE_TERCERO INNER JOIN terceros ON (NOM_CLASE_TERCERO.documento_TER = terceros.documento) AND (NOM_CLASE_TERCERO.TipoDoc_TER = terceros.TipoDoc)) INNER JOIN (NOM_CONCEP INNER JOIN NOM_CPO ON NOM_CONCEP.COD_CONCEP = NOM_CPO.COD_CONCEP) ON (NOM_CLASE_TERCERO.item = NOM_CPO.item_clase) AND (NOM_CLASE_TERCERO.cod_clase = NOM_CPO.cod_clase) AND (NOM_CLASE_TERCERO.documento = NOM_CPO.documento_clase) AND (NOM_CLASE_TERCERO.TipoDoc = NOM_CPO.TipoDoc_clase)
WHERE (((NOM_CPO.NUM_NOMINA)=:nomi) AND (NOM_CONCEP.TIPO='P' or NOM_CONCEP.TIPO='V'))
) AS v_nom_aportes INNER JOIN (mod_relacion_origen INNER JOIN pre_docu_cp ON (mod_relacion_origen.num_doc1 = pre_docu_cp.numdoc) AND (mod_relacion_origen.char_doc1 = pre_docu_cp.coddoc) AND (mod_relacion_origen.char_doc2 = pre_docu_cp.clugar) AND (mod_relacion_origen.num_doc2 = pre_docu_cp.item)) ON (v_nom_aportes.NUM_NOMINA = mod_relacion_origen.num_orig1) AND (v_nom_aportes.COD_CONCEP = mod_relacion_origen.char_orig3) AND (v_nom_aportes.TipoDoc_TER = mod_relacion_origen.char_orig1) AND (v_nom_aportes.documento_TER = mod_relacion_origen.char_orig2)) INNER JOIN NOM_CONCEP ON v_nom_aportes.COD_CONCEP = NOM_CONCEP.COD_CONCEP) INNER JOIN pre_docu_cb ON (pre_docu_cp.numdoc = pre_docu_cb.numdoc) AND (pre_docu_cp.clugar = pre_docu_cb.clugar) AND (pre_docu_cp.coddoc = pre_docu_cb.coddoc)
WHERE (((pre_docu_cb.estado)<>'3') AND ((pre_docu_cp.monto_interfaz)<pre_docu_cp.monto_vigente))
GROUP BY v_nom_aportes.DES_CONCEP, v_nom_aportes.NUM_NOMINA, v_nom_aportes.TipoDoc_TER, v_nom_aportes.documento_TER, v_nom_aportes.razon_social, v_nom_aportes.COD_CONCEP

UNION ALL

SELECT DISTINCT 0 AS esco, v_nom_aportes.DES_CONCEP, v_nom_aportes.NUM_NOMINA, Sum(mod_relacion_origen.valor) AS tbruto, v_nom_aportes.tipodoc AS tipodoc_ter, v_nom_aportes.documento AS documento_ter, v_nom_aportes.razon_social, v_nom_aportes.COD_CONCEP
FROM ((SELECT DISTINCT NOM_CPO.NUM_NOMINA, NOM_CPO.COD_CONCEP, NOM_CONCEP.DES_CONCEP, TERCEROS.tipodoc, TERCEROS.documento, TERCEROS.razon_social
FROM TERCEROS INNER JOIN (NOM_CONCEP INNER JOIN (EMPLECARGO INNER JOIN NOM_CPO ON (EMPLECARGO.TipoDoc = NOM_CPO.TIPODOC) AND (EMPLECARGO.documento = NOM_CPO.DOCUMENTO) AND (EMPLECARGO.ncargo = NOM_CPO.NCARGO)) ON NOM_CONCEP.COD_CONCEP = NOM_CPO.COD_CONCEP) ON (TERCEROS.tipodoc = NOM_CONCEP.tipodoc) AND (TERCEROS.documento = NOM_CONCEP.documento)
WHERE (((NOM_CPO.NUM_NOMINA)=:nomi) AND (NOM_CONCEP.TIPO='P' or NOM_CONCEP.TIPO='V'))
) AS v_nom_aportes INNER JOIN (pre_docu_cb INNER JOIN (mod_relacion_origen INNER JOIN pre_docu_cp ON (mod_relacion_origen.char_doc1 = pre_docu_cp.coddoc) AND (mod_relacion_origen.char_doc2 = pre_docu_cp.clugar) AND (mod_relacion_origen.num_doc1 = pre_docu_cp.numdoc) AND (mod_relacion_origen.num_doc2 = pre_docu_cp.item)) ON (pre_docu_cp.coddoc = pre_docu_cb.coddoc) AND (pre_docu_cb.numdoc = pre_docu_cp.numdoc) AND (pre_docu_cb.clugar = pre_docu_cp.clugar)) ON (v_nom_aportes.COD_CONCEP = mod_relacion_origen.char_orig3) AND (v_nom_aportes.NUM_NOMINA = mod_relacion_origen.num_orig1)) INNER JOIN NOM_CAB ON v_nom_aportes.NUM_NOMINA = NOM_CAB.NUM_NOMINA
WHERE (((NOM_CAB.ESTADO)='1') AND ((pre_docu_cb.estado)<>'3')  AND ((pre_docu_cp.monto_interfaz)<pre_docu_cp.monto_vigente))
GROUP BY v_nom_aportes.DES_CONCEP, v_nom_aportes.NUM_NOMINA, v_nom_aportes.tipodoc, v_nom_aportes.documento, v_nom_aportes.razon_social, v_nom_aportes.COD_CONCEP
