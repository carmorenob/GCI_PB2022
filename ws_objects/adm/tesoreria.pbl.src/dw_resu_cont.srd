 retrieve="SELECT
	contra.acum_cobro,   
	contra.acum_pago,   
	monto as tbruto,   
	monto as tneto,   
	monto as tbase  
FROM 
	contra  
WHERE 
	( contra.ano = :ano ) AND  
  	( contra.ncontrato = :ncont ) AND  
	( contra.otrosi = :otros )    
" arguments=(("ano", number),("ncont", number),("otros", number)) )
