 retrieve="SELECT 
	tesocajamenor.codcaja, tesocajamenor.clugar, tesocajamenor.descripcion, tesocajamenor.responsable, tesocajamenor.fecha_crea, 
	tesocajamenor.usuario, tesocajamenor.estado, tesocajamenor.fecha_anula, tesocajamenor.base, tesocajamenor.efectivo, 
	tesocajamenor.destino_pres, tesocajamenor.presupuesto, tesocajamenor.cod_empresa, 0 AS saldo, 
	PRE_VIGENCIA.INICIA, PRE_VIGENCIA.TERMINA,tesocajamenor.porce_gasto
FROM 
	(tesocajamenor LEFT JOIN tesocuentasban ON (tesocajamenor.clugar = tesocuentasban.clugarmenor) AND (tesocajamenor.codcaja = tesocuentasban.codcajamenor)) 
	LEFT JOIN PRE_VIGENCIA ON tesocajamenor.COD_VIGENCIA = PRE_VIGENCIA.COD_VIGENCIA
WHERE 
	(((tesocajamenor.codcaja)=:caja) AND ((tesocajamenor.clugar)=:lugar))
" update="tesocajamenor" updatewhere=0 updatekeyinplace=yes arguments=(("lugar", string),("caja", string)) )
