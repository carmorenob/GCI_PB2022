 retrieve="SELECT
	contra_fpago_causa.ano,contra_fpago_causa.ncontrato,   
	contra_fpago_causa.otrosi,contra_fpago_causa.item,   
	contra_fpago_causa.cod_rel, tesorelfact_concep.descripcion as descripcion_rel, contra_fpago_causa.codlugar,   
	contra_fpago_causa.coduf,contra_fpago_causa.codcc,   
	contra_fpago_causa.monto,contra_fpago_causa.monto_vigente,
	contra_fpago_causa.usuario,centrocostos.descripcion,
	0 as nuevo,0 as porcentaje
FROM
	contra_fpago_causa 
	inner join  centrocostos   ON (centrocostos.coduf =contra_fpago_causa.coduf ) and (centrocostos.codcc =contra_fpago_causa.codcc )
	inner join tesorelfact_concep on contra_fpago_causa.cod_rel = tesorelfact_concep.cod_rel
WHERE 
	contra_fpago_causa.ano = :anyo and 
	contra_fpago_causa.ncontrato= :contr and 
	contra_fpago_causa.otrosi = :otrosi " update="contra_fpago_causa" updatewhere=0 updatekeyinplace=no arguments=(("anyo", number),("contr", number),("otrosi", number)) )
