table(column=(type=char(12) update=yes updatewhereclause=yes name=codigo dbname="mod_relacion_origen.codigo" )
 column=(type=number update=yes updatewhereclause=yes name=item dbname="mod_relacion_origen.item" )
 column=(type=number update=yes updatewhereclause=yes name=num_orig1 dbname="mod_relacion_origen.num_orig1" )
 column=(type=char(20) update=yes updatewhereclause=yes name=char_orig1 dbname="mod_relacion_origen.char_orig1" )
 column=(type=char(20) update=yes updatewhereclause=yes name=char_orig2 dbname="mod_relacion_origen.char_orig2" )
 column=(type=decimal(2) update=yes updatewhereclause=yes name=valor dbname="mod_relacion_origen.valor" initial="0" )
 column=(type=number updatewhereclause=yes name=num_doc2 dbname="mod_relacion_origen.num_doc2" )
 column=(type=decimal(2) updatewhereclause=yes name=pagado dbname="pagado" )
 retrieve="  SELECT mod_relacion_origen.codigo,   
         mod_relacion_origen.item,   
         mod_relacion_origen.num_orig1,
         mod_relacion_origen.char_orig1,
         mod_relacion_origen.char_orig2,
         mod_relacion_origen.valor,
         mod_relacion_origen.num_doc2,
         sum(car_cobro_cpo.valor + car_cobro_cpo.a_amortizar) as pagado
    FROM mod_relacion_origen, car_cobro_cpo, car_tipo  
   WHERE ( car_cobro_cpo.cartipo = car_tipo.cartipo ) and  
         ( mod_relacion_origen.char_orig1 = car_cobro_cpo.coddoc ) and  
         ( mod_relacion_origen.char_orig2 = car_cobro_cpo.clugar ) and  
         ( mod_relacion_origen.num_orig1 = car_cobro_cpo.num_cobro ) and  
         ( mod_relacion_origen.codigo = :codigo ) AND  
         ( mod_relacion_origen.char_doc1 = :coddoc ) AND  
         ( mod_relacion_origen.char_doc2 = :clugar ) AND  
         ( mod_relacion_origen.num_doc1 = :numdoc ) AND  
         ( car_tipo.codtipo = '1' ) AND  
         ( car_cobro_cpo.presupuesto = '0' OR car_cobro_cpo.presupuesto is NULL)
         and car_cobro_cpo.estado is null and car_cobro_cpo.num_cobro=:ncobro
GROUP BY mod_relacion_origen.codigo,   
         mod_relacion_origen.item,   
         mod_relacion_origen.num_orig1,   
         mod_relacion_origen.char_orig1,   
         mod_relacion_origen.char_orig2,   
         mod_relacion_origen.valor,
         mod_relacion_origen.num_doc2
" arguments=(("codigo", string),("coddoc", string),("clugar", string),("numdoc", number),("ncobro", number))  sort="codigo A item A " )
