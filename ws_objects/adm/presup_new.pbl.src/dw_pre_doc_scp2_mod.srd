 retrieve="SELECT pre_docu_scp.coddoc, pre_docu_scp.clugar, pre_docu_scp.numdoc, pre_docu_scp.item,
 pre_docu_scp.sitem, pre_docu_scp.cod_vigencia, pre_docu_scp.codtotal, pre_docu_scp.monto,
 pre_docu_scp.estado, pre_docu_scp.coddoc_equ, pre_docu_scp.clugar_equ, pre_docu_scp.numdoc_equ,
 pre_docu_scp.item_equ, pre_docu_scp.consec_equ, pre_equ_monto.monto_gasto, pre_equ_monto.monto_ejecutado,
 pre_plan.descrip, pre_plan.planeado_inicial, v_pre_suma_acum.acum_traslado, v_pre_suma_acum.acum_adicion,
 v_pre_suma_acum.acum_recorte, v_pre_suma_acum.acum_dispon, v_pre_suma_acum.acum_mod_dispo,
 v_pre_suma_acum.acum_reserva, v_pre_suma_acum.acum_mod_reserv, v_pre_suma_acum.acum_girado,
 v_pre_suma_acum.acum_recon, v_pre_suma_acum.acum_recaudo, pre_docu_scp.monto as n_monto
FROM ((pre_docu_scp INNER JOIN pre_docu_cp ON (pre_docu_scp.item = pre_docu_cp.item) 
AND (pre_docu_scp.numdoc = pre_docu_cp.numdoc) AND (pre_docu_scp.clugar = pre_docu_cp.clugar) 
AND (pre_docu_scp.coddoc = pre_docu_cp.coddoc)) INNER JOIN pre_equ_monto 
ON (pre_docu_scp.codtotal = pre_equ_monto.CodTotal) AND (pre_docu_scp.cod_vigencia = pre_equ_monto.cod_vigencia) 
AND (pre_docu_cp.codtotal = pre_equ_monto.CodTotal_gasto) 
AND (pre_docu_cp.cod_vigencia = pre_equ_monto.cod_vigen_gasto)) 
INNER JOIN (pre_plan 
LEFT outer JOIN v_pre_suma_acum ON (pre_plan.CodTotal = v_pre_suma_acum.a_codtotal) 
AND (pre_plan.cod_vigencia = v_pre_suma_acum.a_codvigencia)) ON (pre_docu_scp.codtotal = pre_plan.CodTotal) 
AND (pre_docu_scp.cod_vigencia = pre_plan.cod_vigencia)
WHERE (((pre_docu_scp.coddoc)=:cdoc) AND ((pre_docu_scp.clugar)=:clug) AND ((pre_docu_scp.numdoc)=:ndoc) AND
        (pre_docu_scp.coddoc=:item) )" arguments=(("cdoc", string),("clug", string),("ndoc", number),("item", number)) )
