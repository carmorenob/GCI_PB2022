 retrieve="SELECT 
	pre_docu_cp.coddoc,pre_docu_cp.clugar,pre_docu_cp.numdoc,   
	pre_docu_cp.item,pre_docu_cp.coddoc_orig,pre_docu_cp.clugar_orig,   
	pre_docu_cp.numdoc_orig,pre_docu_cp.item_orig,pre_docu_cp.cod_vigencia,   
	pre_docu_cp.codtotal,pre_docu_cp.monto_vigente,pre_docu_cp.monto_utilizado,   
	pre_docu_cp.monto_interfaz,pre_docu_cp.saldo_plan,   
	pre_docu_cp.periodo,pre_plan.descrip,
	pre_docu_cp.monto_vigente - pre_docu_cp.monto_utilizado as monto,   
	pre_plan.planeado_inicial, v_pre_suma_acum.acum_traslado, v_pre_suma_acum.acum_adicion, 
	v_pre_suma_acum.acum_recorte, v_pre_suma_acum.acum_dispon, v_pre_suma_acum.acum_mod_dispo, 
	v_pre_suma_acum.acum_reserva, v_pre_suma_acum.acum_mod_reserv, v_pre_suma_acum.acum_girado, 
	v_pre_suma_acum.acum_recon, v_pre_suma_acum.acum_recaudo
FROM 
	((pre_docu_cp INNER JOIN pre_docu_cb ON (pre_docu_cp.numdoc = pre_docu_cb.numdoc) AND (pre_docu_cp.clugar = pre_docu_cb.clugar) 
	AND (pre_docu_cp.coddoc = pre_docu_cb.coddoc)) INNER JOIN pre_plan ON (pre_docu_cp.codtotal = pre_plan.codtotal) 
	AND (pre_docu_cp.cod_vigencia = pre_plan.cod_vigencia)) LEFT JOIN v_pre_suma_acum ON (pre_plan.codtotal = v_pre_suma_acum.a_codtotal) 
	AND (pre_plan.cod_vigencia = v_pre_suma_acum.a_codvigencia)
WHERE 
	(((pre_docu_cb.coddoc)=:cdoc) AND ((pre_docu_cb.clugar)=:clug) AND ((pre_docu_cb.estado) In ('1','2')) AND ((pre_docu_cb.cod_vigencia)=:vig) 
	AND ((pre_docu_cb.en_destino)='0') AND ((pre_docu_cb.cod_flujo) Like :dest))
	AND ((pre_docu_cp.monto_vigente - pre_docu_cp.monto_utilizado)<>0 );
" retrieve.asneeded=yes arguments=(("cdoc", string),("clug", string),("vig", number),("dest", string)) )
