 retrieve="SELECT 
	car_cobro_cpo.clugar, car_cobro_cpo.coddoc, car_cobro_cpo.num_cobro, car_cobro_cpo.cartipo, car_cobro_cpo.item, 
	car_cobro_cpo.valor, car_tipo.descripcion, car_cobro_cpo.a_amortizar, car_tipo.cod_vigencia, 
	car_tipo.codtotal_pres, pre_plan.descrip
FROM
	(car_cobro_cpo INNER JOIN car_tipo ON car_cobro_cpo.cartipo = car_tipo.cartipo) 
	LEFT JOIN pre_plan ON (car_tipo.codtotal_pres = pre_plan.codtotal) AND (car_tipo.cod_vigencia = pre_plan.cod_vigencia)
WHERE 
	(((car_cobro_cpo.clugar)=:clugar ) AND ((car_cobro_cpo.coddoc)=:coddoc) 
	AND ((car_cobro_cpo.num_cobro)=:num_cobro) AND ((car_tipo.codtipo)='1') 
	AND ((car_cobro_cpo.estado) Is Null) 
	AND ((car_cobro_cpo.presupuesto)='0'))" arguments=(("coddoc", string),("clugar", string),("num_cobro", number))  sort="num_cobro A cartipo A item A " )
