 retrieve="SELECT 1 as tipo, tesnotabanco_carcbrocpo.codbanco, tesnotabanco_carcbrocpo.tipo_cuenta, tesnotabanco_carcbrocpo.numcuenta, tesnotabanco_carcbrocpo.item, tesnotabanco_carcbrocpo.valor, tesobancos.DesBanco, factcab.nradica, factcab.clugar_rad, regimen_Ccosto.cod_vig_recon, regimen_Ccosto.cta_pres_recon, Sum(car_cobro_rad.valor*factcpo.vproced/factcab.vtproced) AS valor_pagado
FROM (((((mod_relacion_origen INNER JOIN tesnotabanco_carcbrocpo ON (mod_relacion_origen.num_orig1 = tesnotabanco_carcbrocpo.item) AND (mod_relacion_origen.char_orig3 = tesnotabanco_carcbrocpo.numcuenta) AND (mod_relacion_origen.char_orig2 = tesnotabanco_carcbrocpo.tipo_cuenta) AND (mod_relacion_origen.char_orig1 = tesnotabanco_carcbrocpo.codbanco)) INNER JOIN CAR_COBRO_CPO ON (tesnotabanco_carcbrocpo.item_pag = CAR_COBRO_CPO.item) AND (tesnotabanco_carcbrocpo.cartipo = CAR_COBRO_CPO.cartipo) AND (tesnotabanco_carcbrocpo.num_cobro = CAR_COBRO_CPO.num_cobro) AND (tesnotabanco_carcbrocpo.coddoc_pag = CAR_COBRO_CPO.coddoc) AND (tesnotabanco_carcbrocpo.clugar_pag = CAR_COBRO_CPO.clugar)) INNER JOIN tesobancos ON tesnotabanco_carcbrocpo.codbanco = tesobancos.CodBanco) INNER JOIN car_cobro_rad ON (CAR_COBRO_CPO.clugar = car_cobro_rad.clugar) AND (CAR_COBRO_CPO.coddoc = car_cobro_rad.coddoc) AND (CAR_COBRO_CPO.num_cobro = car_cobro_rad.num_cobro) AND (CAR_COBRO_CPO.cartipo = car_cobro_rad.cartipo) AND (CAR_COBRO_CPO.item = car_cobro_rad.item)) INNER JOIN factcab ON (car_cobro_rad.clugar_fact = factcab.clugar) AND (car_cobro_rad.nfact = factcab.nfact)) INNER JOIN ((factcpo INNER JOIN regimen_Ccosto ON (factcpo.cufuncional = regimen_Ccosto.coduf) AND (factcpo.cccosto = regimen_Ccosto.codcc)) INNER JOIN contratos ON (contratos.ctreg = regimen_Ccosto.tipo_reg) AND (factcpo.cemp = contratos.codemp) AND (factcpo.ccontrato = contratos.codcontrato)) ON (factcab.clugar = factcpo.clugar) AND (factcab.nfact = factcpo.nfact)
WHERE (((mod_relacion_origen.codigo)='PIING-') AND ((mod_relacion_origen.char_doc1)=:coddoc) AND ((mod_relacion_origen.char_doc2)=:clugar) AND ((mod_relacion_origen.num_doc1)=:numdoc))
GROUP BY tesnotabanco_carcbrocpo.codbanco, tesnotabanco_carcbrocpo.tipo_cuenta, tesnotabanco_carcbrocpo.numcuenta, tesnotabanco_carcbrocpo.item, tesnotabanco_carcbrocpo.valor, tesobancos.DesBanco, factcab.nradica, factcab.clugar_rad, regimen_Ccosto.cod_vig_recon, regimen_Ccosto.cta_pres_recon

union all

SELECT 2 as tipo, tescajing_carpago.codcaja, tescajing_carpago.clugar, tescajing_carpago.coddoc, tescajing_carpago.ningreso, tescajing_carpago.valor, tescajing_carpago.codcaja, factcab.nradica, factcab.clugar_rad, regimen_Ccosto.cod_vig_recon, regimen_Ccosto.cta_pres_recon, Sum(car_cobro_rad.valor*factcpo.vproced/factcab.vtproced) AS valor_pagado
FROM ((((((mod_relacion_origen INNER JOIN tescajing_carpago ON (mod_relacion_origen.num_orig1 = tescajing_carpago.ningreso) AND (mod_relacion_origen.char_orig3 = tescajing_carpago.coddoc) AND (mod_relacion_origen.char_orig2 = tescajing_carpago.clugar) AND (mod_relacion_origen.char_orig1 = tescajing_carpago.codcaja)) INNER JOIN CAR_COBRO_CPO ON (tescajing_carpago.item_pag = CAR_COBRO_CPO.item) AND (tescajing_carpago.cartipo = CAR_COBRO_CPO.cartipo) AND (tescajing_carpago.num_cobro = CAR_COBRO_CPO.num_cobro) AND (tescajing_carpago.coddoc_pag = CAR_COBRO_CPO.coddoc) AND (tescajing_carpago.clugar_pag = CAR_COBRO_CPO.clugar)) INNER JOIN car_cobro_rad ON (CAR_COBRO_CPO.item = car_cobro_rad.item) AND (CAR_COBRO_CPO.cartipo = car_cobro_rad.cartipo) AND (CAR_COBRO_CPO.num_cobro = car_cobro_rad.num_cobro) AND (CAR_COBRO_CPO.coddoc = car_cobro_rad.coddoc) AND (CAR_COBRO_CPO.clugar = car_cobro_rad.clugar)) INNER JOIN factcab ON (car_cobro_rad.clugar_fact = factcab.clugar) AND (car_cobro_rad.nfact = factcab.nfact)) INNER JOIN factcpo ON (factcab.clugar = factcpo.clugar) AND (factcab.nfact = factcpo.nfact)) INNER JOIN contratos ON (factcpo.ccontrato = contratos.codcontrato) AND (factcpo.cemp = contratos.codemp)) INNER JOIN regimen_Ccosto ON (factcpo.cccosto = regimen_Ccosto.codcc) AND (factcpo.cufuncional = regimen_Ccosto.coduf) AND (contratos.ctreg = regimen_Ccosto.tipo_reg)
WHERE (((mod_relacion_origen.codigo)='PIING-') AND ((mod_relacion_origen.char_doc1)=:coddoc) AND ((mod_relacion_origen.char_doc2)=:clugar) AND ((mod_relacion_origen.num_doc1)=:numdoc))
GROUP BY tescajing_carpago.codcaja, tescajing_carpago.clugar, tescajing_carpago.coddoc, tescajing_carpago.ningreso, tescajing_carpago.valor, factcab.nradica, factcab.clugar_rad, regimen_Ccosto.cod_vig_recon, regimen_Ccosto.cta_pres_recon
