 retrieve="SELECT CAR_COBRO_CAB.clugar, CAR_COBRO_CAB.coddoc, CAR_COBRO_CAB.num_cobro, regimen_ccosto.cod_vig_recon AS cod_vigencia_recon, regimen_ccosto.cta_pres_recon AS cod_total_recon, ripsradicadet.cemp AS codemp, 0 AS selec, Sum( (FactCpo.VEmp-FactCpo.desc_emp)) AS valor, CAR_COBRO_CAB.fecha, CAR_COBRO_CPO.num_radicacion
FROM (((((CAR_COBRO_CAB INNER JOIN CAR_COBRO_CPO ON (CAR_COBRO_CAB.num_cobro = CAR_COBRO_CPO.num_cobro) AND (CAR_COBRO_CAB.coddoc = CAR_COBRO_CPO.coddoc) AND (CAR_COBRO_CAB.clugar = CAR_COBRO_CPO.clugar)) INNER JOIN ripsradicadet ON (CAR_COBRO_CPO.clugar_rad = ripsradicadet.clugar) AND (CAR_COBRO_CPO.num_radicacion = ripsradicadet.num_radicacion)) INNER JOIN Contratos ON (ripsradicadet.ccontrato = Contratos.CodContrato) AND (ripsradicadet.cemp = Contratos.CodEmp)) INNER JOIN FactCab ON (ripsradicadet.ccontrato = FactCab.CContrato) AND (ripsradicadet.cemp = FactCab.CEmp) AND (ripsradicadet.clugar = FactCab.clugar_rad) AND (ripsradicadet.num_radicacion = FactCab.nradica)) INNER JOIN FactCpo ON (FactCab.clugar = FactCpo.clugar) AND (FactCab.NFact = FactCpo.NFact)) INNER JOIN regimen_ccosto ON (Contratos.CTReg = regimen_ccosto.tipo_reg) AND (FactCpo.CCcosto = regimen_ccosto.codcc) AND (FactCpo.CUfuncional = regimen_ccosto.coduf)
WHERE (((ripsradicadet.ctipo)='1') AND ((CAR_COBRO_CAB.presupuesto)='0') AND ((CAR_COBRO_CAB.estado) Is Null) AND ((FactCab.Estado) Is Null))
GROUP BY CAR_COBRO_CAB.clugar, CAR_COBRO_CAB.coddoc, CAR_COBRO_CAB.num_cobro, regimen_ccosto.cod_vig_recon, regimen_ccosto.cta_pres_recon, ripsradicadet.cemp, CAR_COBRO_CAB.fecha, CAR_COBRO_CPO.num_radicacion

UNION ALL

SELECT CAR_COBRO_CAB.clugar, CAR_COBRO_CAB.coddoc, CAR_COBRO_CAB.num_cobro, CAR_COBRO_CAB.cod_vigencia, 
CAR_COBRO_CAB.CodTotal_presu, '' AS CodEmp, 0 AS selec, CAR_COBRO_CAB.vtcobro, CAR_COBRO_CAB.fecha,0 as nradica
FROM CAR_COBRO_CAB
WHERE (((CAR_COBRO_CAB.presupuesto)='0') AND ((CAR_COBRO_CAB.estado) Is Null) AND ((CAR_COBRO_CAB.cxp)='2'))

UNION ALL

SELECT CAR_COBRO_CAB.clugar, CAR_COBRO_CAB.coddoc, CAR_COBRO_CAB.num_cobro, regimen_ccosto.cod_vig_recon AS cod_vigencia_recon, regimen_ccosto.cta_pres_recon AS cod_total_recon, ripsradicadet.cemp, 0 AS selec, Sum(factcpo.VEmp*ripsradicadet.valor_capita/ripsradicadet.valor_evento) AS valor, CAR_COBRO_CAB.fecha, CAR_COBRO_CPO.num_radicacion
FROM (((((CAR_COBRO_CAB INNER JOIN CAR_COBRO_CPO ON (CAR_COBRO_CAB.num_cobro = CAR_COBRO_CPO.num_cobro) AND (CAR_COBRO_CAB.coddoc = CAR_COBRO_CPO.coddoc) AND (CAR_COBRO_CAB.clugar = CAR_COBRO_CPO.clugar)) INNER JOIN ripsradicadet ON (CAR_COBRO_CPO.clugar_rad = ripsradicadet.clugar) AND (CAR_COBRO_CPO.num_radicacion = ripsradicadet.num_radicacion)) INNER JOIN FactCab ON (ripsradicadet.ccontrato = FactCab.CContrato) AND (ripsradicadet.cemp = FactCab.CEmp) AND (ripsradicadet.clugar = FactCab.clugar_rad) AND (ripsradicadet.num_radicacion = FactCab.nradica)) INNER JOIN Contratos ON (ripsradicadet.ccontrato = Contratos.CodContrato) AND (ripsradicadet.cemp = Contratos.CodEmp)) INNER JOIN regimen_ccosto ON Contratos.CTReg = regimen_ccosto.tipo_reg) INNER JOIN FactCpo ON (FactCpo.CCcosto = regimen_ccosto.codcc) AND (FactCpo.CUfuncional = regimen_ccosto.coduf) AND (FactCab.clugar = FactCpo.clugar) AND (FactCab.NFact = FactCpo.NFact)
WHERE (((ripsradicadet.ctipo)='2') AND ((CAR_COBRO_CAB.presupuesto)='0') AND ((CAR_COBRO_CAB.estado) Is Null) AND ((FactCab.Estado) Is Null) AND ((ripsradicadet.valor_evento)>0))
GROUP BY CAR_COBRO_CAB.clugar, CAR_COBRO_CAB.coddoc, CAR_COBRO_CAB.num_cobro, regimen_ccosto.cod_vig_recon, regimen_ccosto.cta_pres_recon, ripsradicadet.cemp, CAR_COBRO_CAB.fecha, CAR_COBRO_CPO.num_radicacion;
