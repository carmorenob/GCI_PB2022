 column=(type=decimal(2) updatewhereclause=yes name=monto_vigente dbname="monto_vigente" )
 column=(type=decimal(2) updatewhereclause=yes name=monto_utilizado dbname="monto_utilizado" )
 column=(type=long updatewhereclause=yes name=cod_vigencia_new dbname="cod_vigencia_new" )
