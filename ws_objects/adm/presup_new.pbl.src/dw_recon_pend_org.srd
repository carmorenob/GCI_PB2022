 retrieve="SELECT pre_docu_cp.cod_vigencia, pre_docu_cp.codtotal, pre_plan.descrip, Sum(pre_docu_cp.monto_vigente) AS monto_vigente, Sum(pre_docu_cp.monto_utilizado) AS monto_utilizado, 0 AS cod_vigencia_new, '' AS codtotal_new, '' AS descrip_new, 1 AS selec
FROM ((pre_docu_cb INNER JOIN pre_docu_cp ON (pre_docu_cb.coddoc = pre_docu_cp.coddoc) AND (pre_docu_cb.clugar = pre_docu_cp.clugar) AND (pre_docu_cb.numdoc = pre_docu_cp.numdoc)) INNER JOIN pre_plan ON (pre_docu_cp.cod_vigencia = pre_plan.cod_vigencia) AND (pre_docu_cp.codtotal = pre_plan.CodTotal)) INNER JOIN mod_relacion_origen ON (pre_docu_cp.coddoc = mod_relacion_origen.char_doc1) AND (pre_docu_cp.clugar = mod_relacion_origen.char_doc2) AND (pre_docu_cp.numdoc = mod_relacion_origen.num_doc1)
WHERE ((pre_docu_cp.monto_vigente > pre_docu_cp.monto_utilizado) AND ((pre_docu_cb.coddoc)='PRC') AND ((pre_docu_cb.estado)='1') AND ((mod_relacion_origen.char_orig1)='CXC'))
GROUP BY pre_docu_cp.cod_vigencia, pre_docu_cp.codtotal, pre_plan.descrip
HAVING (((pre_docu_cp.cod_vigencia)=:cod_vigencia))
" arguments=(("cod_vigencia", number)) )
