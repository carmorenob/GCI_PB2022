 retrieve="SELECT pre_docu_scp.coddoc, pre_docu_scp.clugar, pre_docu_scp.numdoc, pre_docu_scp.item, pre_docu_scp.sitem,
 pre_docu_scp.cod_vigencia, pre_docu_scp.codtotal, pre_docu_scp.monto, pre_docu_scp.estado ,
 pre_docu_scp.coddoc_equ, pre_docu_scp.clugar_equ, pre_docu_scp.numdoc_equ, pre_docu_scp.item_equ,
 pre_docu_scp.consec_equ, pre_plan.descrip, pre_plan.planeado_inicial, v_pre_suma_acum.acum_traslado,
 v_pre_suma_acum.acum_adicion, v_pre_suma_acum.acum_recorte, v_pre_suma_acum.acum_dispon,
 v_pre_suma_acum.acum_mod_dispo, v_pre_suma_acum.acum_reserva, v_pre_suma_acum.acum_mod_reserv,
 v_pre_suma_acum.acum_girado, v_pre_suma_acum.acum_recon, v_pre_suma_acum.acum_recaudo,pre_docu_cb.estado as estado_cb,
 pre_docu_scp.monto as monto_ant, pre_docu_scp.monto as n_monto

FROM (pre_docu_scp INNER JOIN pre_docu_cb ON (pre_docu_scp.numdoc = pre_docu_cb.numdoc) 
AND (pre_docu_scp.clugar = pre_docu_cb.clugar) AND (pre_docu_scp.coddoc = pre_docu_cb.coddoc)) 
INNER JOIN (pre_plan LEFT outer JOIN v_pre_suma_acum ON (pre_plan.CodTotal = v_pre_suma_acum.a_codtotal) 
AND (pre_plan.cod_vigencia = v_pre_suma_acum.a_codvigencia)) ON (pre_docu_scp.codtotal = pre_plan.CodTotal) 
AND (pre_docu_scp.cod_vigencia = pre_plan.cod_vigencia)
WHERE ((pre_docu_scp.coddoc=:cdoc) AND (pre_docu_scp.clugar=:clug) AND (pre_docu_scp.numdoc=:ndoc) AND
       (pre_docu_scp.item = :item))
" update="pre_docu_scp" updatewhere=0 updatekeyinplace=yes arguments=(("cdoc", string),("clug", string),("ndoc", number),("item", number)) )
