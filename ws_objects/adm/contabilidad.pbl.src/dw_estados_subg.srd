 retrieve="SELECT 
	cont_estados_grupo_cuenta.cod_empresa,cont_estados_grupo_cuenta.codigo, cont_estados_grupo_cuenta.item, 
	cont_estados_grupo_cuenta.subitem,cont_estados_grupo_cuenta.mayor, cont_estados_grupo_cuenta.grupo, 
	cont_estados_grupo_cuenta.cuenta, cont_estados_grupo_cuenta.factor, cont_estados_grupo_cuenta.corriente, 
	MAYOR.DESCRIP, GRUPO.DESCRIP, CUENTAS.DESCRIP
FROM 
	((cont_estados_grupo_cuenta 
	LEFT JOIN CONT_PLAN AS MAYOR ON (cont_estados_grupo_cuenta.COD_EMPRESA = MAYOR.COD_EMPRESA) 
	AND (cont_estados_grupo_cuenta.MAYOR = MAYOR.MAYOR)) 
	LEFT JOIN CONT_PLAN AS GRUPO ON (cont_estados_grupo_cuenta.COD_EMPRESA = GRUPO.COD_EMPRESA) 
	AND (cont_estados_grupo_cuenta.MAYOR = GRUPO.MAYOR) AND (cont_estados_grupo_cuenta.GRUPO = GRUPO.GRUPO)) 
	LEFT JOIN CONT_PLAN AS CUENTAS ON (cont_estados_grupo_cuenta.COD_EMPRESA = CUENTAS.COD_EMPRESA) 
	AND (cont_estados_grupo_cuenta.MAYOR = CUENTAS.MAYOR) AND (cont_estados_grupo_cuenta.GRUPO = CUENTAS.GRUPO) 
	AND (cont_estados_grupo_cuenta.CUENTA = CUENTAS.CUENTA)
WHERE 
	(((cont_estados_grupo_cuenta.codigo)=:grp) AND ((cont_estados_grupo_cuenta.item)=:item) 
	AND ((cont_estados_grupo_cuenta.cod_empresa)=:emp) AND ((MAYOR.GRUPO) Is Null) 
	AND ((GRUPO.CUENTA) Is Null) AND ((CUENTAS.SUBCUENTA) Is Null));
" update="cont_estados_grupo_cuenta" updatewhere=0 updatekeyinplace=no arguments=(("emp", string),("grp", number),("item", number))  sort="cod_empresa A codigo A item A subitem A " )
