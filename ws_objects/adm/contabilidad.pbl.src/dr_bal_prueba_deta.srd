 retrieve="SELECT 
CONT_SALDOS.COD_EMPRESA, CONT_SALDOS.ano, CONT_SALDOS.mes, CONT_SALDOS.Cuenta, 
CONT_PLAN.movimiento, CONT_PLAN.descrip, CONT_SALDOS.tipodoc, 
CONT_SALDOS.nit, CONT_SALDOS.Dverificacion, 
TERCEROS.nombre1, TERCEROS.nombre2, TERCEROS.apellido1, 
TERCEROS.APELLIDO2, TERCEROS.razon_social, CONT_SALDOS.saldodebito, 
CONT_SALDOS.saldocredito, CONT_SALDOS.Debito, CONT_SALDOS.Credito, 
CONT_SALDOS.cierredebito, CONT_SALDOS.cierrecredito
FROM 
(CONT_SALDOS LEFT outer JOIN TERCEROS ON (CONT_SALDOS.tipodoc = TERCEROS.TipoDoc) AND 
(CONT_SALDOS.nit = TERCEROS.documento)) LEFT outer JOIN CONT_PLAN ON 
(CONT_SALDOS.COD_EMPRESA = CONT_PLAN.COD_EMPRESA) AND 
(CONT_SALDOS.Cuenta = CONT_PLAN.CodTotal)
WHERE 
CONT_SALDOS.COD_EMPRESA=:empresa and CONT_SALDOS.ano=:ano AND CONT_SALDOS.mes=:mes1;
" arguments=(("empresa", string),("ano", number),("mes1", number)) )
