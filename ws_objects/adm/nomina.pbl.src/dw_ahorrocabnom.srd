 retrieve="SELECT
	nom_ahorro.num_ahorro,nom_ahorro.tipo,nom_ahorro.fecha,nom_ahorro.doc_tercero,nom_ahorro.nit,
	nom_ahorro.porcensal,nom_ahorro.vfijo,nom_ahorro.estado,nom_ahorro.fecha_anulada,
	nom_ahorro.usuario,nom_ahorro.tipo_doc,nom_ahorro.empleado,terceros.persona,
	terceros.nombre1,terceros.nombre2,terceros.apellido1,terceros.apellido2,terceros.razon_social,
	0 as nuevo,nom_ahorro.cod_concep     
FROM 
	nom_ahorro inner join terceros on (nom_ahorro.doc_tercero = terceros.tipodoc and  nom_ahorro.nit = terceros.documento )
WHERE
	((nom_ahorro.tipo_doc = :tipodoc) and (nom_ahorro.empleado = :documento) 
	and (nom_ahorro.estado = '1' ))  " update="nom_ahorro" updatewhere=0 updatekeyinplace=yes arguments=(("tipodoc", string),("documento", string))  sort="cod_concep A num_ahorro A " )
