global function decimal f_aproxima_a (decimal numero, double multiplo);double residuo,centena1
decimal rs

residuo=mod(numero,multiplo)
if residuo=0 then 
	rs=numero
else
	rs=numero + (multiplo - residuo)
end if

return rs
end function

