 retrieve="SELECT  
	nom_coope_plan_cp.num_prescoopera,nom_coope_plan_cp.num_plan,nom_coope_plan_cp.ncuota,nom_coope_plan_cp.fecha,
	nom_coope_plan_cp.valor,nom_coope_plan_cp.estado,nom_coope_plan_cp.num_nomina,nom_coope_plan_cp.tipodoc,
	nom_coope_plan_cp.documento,nom_coope_plan_cp.cod_concep,nom_coope_plan_cp.item,nom_concep.sigla,nom_coope.estado as estado_cb
FROM 
	(nom_coope INNER JOIN nom_concep ON nom_coope.COD_CONCEP = nom_concep.COD_CONCEP) 
	INNER JOIN nom_coope_plan_cp ON nom_coope.NUM_PRESCOOPERA = nom_coope_plan_cp.NUM_PRESCOOPERA
WHERE
	 (((nom_coope_plan_cp.num_prescoopera)=:num_presta) AND ((nom_coope_plan_cp.num_plan)=:num_plan));" update="nom_coope_plan_cp" updatewhere=0 updatekeyinplace=yes arguments=(("num_presta", number),("num_plan", number))  sort="num_prescoopera A num_plan A ncuota A " )
