 retrieve="SELECT 
	nom_concep_cargo.codrela,   
	nom_concep_cargo.tipoemple,   
	nom_concep_cargo.cargo,   
	nom_concep_cargo.cod_concep,   
	nom_concep_cargo.cod_vigencia,   
	nom_concep_cargo.codtotal,   
	nom_concep.sigla,   
	nom_concep.des_concep,  
	nom_concep.estado
FROM 
	nom_concep_cargo INNER JOIN nom_concep ON nom_concep_cargo.COD_CONCEP = nom_concep.COD_CONCEP
WHERE 
	( ( nom_concep_cargo.codrela = :codrela ) AND  
	( nom_concep_cargo.tipoemple = :tipoemple ) AND  
	( nom_concep_cargo.cargo = :cargo ) )    
