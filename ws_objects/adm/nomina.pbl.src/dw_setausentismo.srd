 retrieve="SELECT 
	nom_ausentismo.TIPODOC, nom_ausentismo.DOCUMENTO, nom_ausentismo.COD_AUSEN, nom_ausentismo.ITEM, 
	nom_ausentismo.FECHAINI, nom_ausentismo.FECHAFIN, nom_ausentismo.ESTADO, nom_ausentismo.tipo, 
	nom_tipo_ausentismo.DESCRIPCION, nom_tipo_ausentismo.DIAS_BASE, 
	nom_tipo_ausentismo.PORCENTAJE, nom_tipo_ausentismo.COD_CONCEP, nom_concep.SIGLA,nom_ausentismo.nro_incapacidad,
	 nom_tipo_ausentismo.INCAP,	nom_ausentismo.fecha_anula, nom_ausentismo.motiv_anula, 
	 nom_ausentismo.usu_anula, nom_ausentismo.cod_anula,
	nom_ausentismo.TIPODOC_pro, nom_ausentismo.DOCUMENTO_pro, nom_ausentismo.CODAUSEN_pro, nom_ausentismo.ITEM_pro,
	nom_ausentismo.fechai_cau, nom_ausentismo.fechaf_cau,nom_ausentismo.usuario,nom_ausentismo.fecha_audita
FROM 
	(nom_ausentismo INNER JOIN nom_tipo_ausentismo ON nom_ausentismo.COD_AUSEN = nom_tipo_ausentismo.COD_AUSEN)
 	LEFT JOIN nom_concep ON nom_tipo_ausentismo.COD_CONCEP = nom_concep.COD_CONCEP
WHERE 
	((nom_ausentismo.TIPODOC=:tipodoc) AND (nom_ausentismo.DOCUMENTO=:documento) AND
 	(nom_ausentismo.COD_AUSEN Like :tipo))" update="nom_ausentismo" updatewhere=0 updatekeyinplace=yes arguments=(("tipodoc", string),("documento", string),("tipo", string))  sort="fechafin D fechaini A estado A cod_ausen A item A " )
