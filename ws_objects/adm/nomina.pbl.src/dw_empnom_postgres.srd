 retrieve="SELECT DISTINCT 
 	NOM_CPO.TIPODOC, NOM_CPO.DOCUMENTO, '1' AS estado, terceros.nombre1, terceros.nombre2, terceros.apellido1, 
	terceros.apellido2, 1 AS sel, EMPLECARGO.ncargo, emplecargo.codrela||emplecargo.tipoemple AS tipoemple, 
	lugar.descripcion AS clugar_des, ufuncional.desufuncional AS ufun_des, centrocostos.descripcion AS cc_des
FROM 
	((((( (select num_nomina,tipodoc,documento, max(ntraslado) as traslado from nom_cpo where num_nomina=:num_nomina group by num_nomina,tipodoc,documento) AS NOM_tras 
	INNER JOIN NOM_CPO ON (NOM_tras.traslado = NOM_CPO.ntraslado) AND (NOM_tras.documento = NOM_CPO.DOCUMENTO) 
	AND (NOM_tras.tipodoc = NOM_CPO.TIPODOC) AND (NOM_tras.num_nomina = NOM_CPO.NUM_NOMINA)) 
	INNER JOIN EMPLECARGO ON (NOM_CPO.NCARGO = EMPLECARGO.ncargo) AND (NOM_CPO.DOCUMENTO = EMPLECARGO.documento) 
	AND (NOM_CPO.TIPODOC = EMPLECARGO.TipoDoc)) 
	INNER JOIN (lugar INNER JOIN EMPLEUBICA ON lugar.codlugar = EMPLEUBICA.CodLugar) ON (NOM_CPO.DOCUMENTO = EMPLEUBICA.documento) 
	AND (NOM_CPO.TIPODOC = EMPLEUBICA.TipoDoc) AND (NOM_CPO.ntraslado = EMPLEUBICA.NTRASLADO) AND (NOM_CPO.NCARGO = EMPLEUBICA.ncargo)) 
	INNER JOIN ufuncional ON EMPLEUBICA.ufuncional = ufuncional.coduf) INNER JOIN terceros ON (NOM_tras.tipodoc = terceros.tipodoc) 
	AND (NOM_tras.documento = terceros.documento)) 
	INNER JOIN centrocostos ON (EMPLEUBICA.CCosto = centrocostos.codcc) AND (EMPLEUBICA.ufuncional = centrocostos.coduf)" arguments=(("num_nomina", number))  sort="apellido1 A apellido2 A nombre1 A nombre2 A documento A " )
