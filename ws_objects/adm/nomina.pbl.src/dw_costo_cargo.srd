 retrieve="SELECT 
	case when nom_qc.cod_concep is null then NOM_CONCEP.cod_concep else nom_qc.cod_concep end as cod_concep,
	nom_qc.cargo, nom_qc.lugar, nom_qc.cod_empresa, nom_qc.codtotal, nom_qc.descrip, 
	case when nom_qc.factor is null then 1 else nom_qc.factor end as factor, 
	case when nom_qc.publico is null then '0' else nom_qc.publico end as publico, 
	nom_qc.coduf, nom_qc.codcc,
	NOM_CONCEP.COD_TIPO_CONCEP, NOM_CONCEP.TIPO
FROM 
	NOM_CONCEP LEFT JOIN 
	(
		SELECT 
			nom_costo_cargo.cod_concep, nom_costo_cargo.cargo, nom_costo_cargo.lugar, nom_costo_cargo.cod_empresa, 
			nom_costo_cargo.codtotal, CONT_PLAN.descrip, nom_costo_cargo.factor, nom_costo_cargo.publico, 
			nom_costo_cargo.coduf, nom_costo_cargo.codcc
		FROM 
			nom_costo_cargo LEFT JOIN CONT_PLAN ON (nom_costo_cargo.codtotal = CONT_PLAN.CodTotal) 
			AND (nom_costo_cargo.cod_empresa = CONT_PLAN.COD_EMPRESA)
		WHERE 
			(((nom_costo_cargo.cargo)=:cargo) 
			AND ((nom_costo_cargo.lugar)=:lugar) 
			AND ((nom_costo_cargo.coduf)=:uf) 
			AND ((nom_costo_cargo.codcc)=:cc))
	) as nom_qc ON NOM_CONCEP.cod_concep = nom_qc.cod_concep
order  by 
NOM_CONCEP.des_concep

