 retrieve="  SELECT nom_programacion.tipodoc,   
         nom_programacion.documento,   
         nom_programacion.fecha_dom,   
         nom_programacion.item,   
         nom_programacion.ncargo,   
         nom_programacion.clugar,   
         nom_programacion.ufuncional,   
         nom_programacion.ccosto,   
         nom_programacion.inicio,   
         nom_programacion.fin,   
         nom_programacion.horas,   
         nom_programacion.estado,   
         nom_programacion.agendaprof,   
         nom_programacion.calculado,   
         nom_programacion.hextra,   
         nom_programacion.compensa,   
         lugar.descripcion as des_lu,   
         ufuncional.desufuncional as des_uf,   
         centrocostos.descripcion as des_cc,   
         nom_programacion.usuario,   
         nom_programacion.fecha_modi,   
         nom_programacion.turno,   
         '               ' as objeto,   
         inicio as h_ini,   
         fin as h_fin,   
         0 as obj , 
		centrocostos.COLOR AS COLOR
FROM 
	((nom_programacion INNER JOIN lugar ON nom_programacion.CLUGAR = lugar.CODLUGAR) 
	INNER JOIN centrocostos ON (nom_programacion.CCOSTO = centrocostos.CODCC) AND (nom_programacion.UFUNCIONAL = centrocostos.CODUF)) 
	INNER JOIN ufuncional ON centrocostos.CODUF = ufuncional.CODUF
WHERE 
        ( ( nom_programacion.tipodoc = :tipodoc ) AND  
         ( nom_programacion.documento = :documento ) AND
		( nom_programacion.calculado='0') ) AND
		((nom_programacion.inicio >= :fechaI and  nom_programacion.inicio <=:fechaf) 
		OR (nom_programacion.fin >= :fechaI and nom_programacion.fin<= :fechaf))
" update="nom_programacion" updatewhere=0 updatekeyinplace=no arguments=(("tipodoc", string),("documento", string),("FechaI", datetime),("FechaF", datetime))  sort="fecha_dom A inicio A fin A " )
