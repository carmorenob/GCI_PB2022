 retrieve="SELECT 
		car_glosa_cab.coddoc,   
         car_glosa_cab.clugar,   
         car_glosa_cab.num_glosa,   
         car_glosa_cab.codemp,   
         car_glosa_cab.fecha,   
         car_glosa_cab.valor,   
         car_glosa_cab.estado,   
         car_glosa_cab.fecha_anula,   
         car_glosa_cab.usuario,   
         car_glosa_cab.valor_aceptado,   
         empresa.desemp,   
         car_glosa_cab.fecha_envia,   
         car_glosa_cab.fecha_radica ,
		0 as reen
FROM 
	  	car_glosa_cab inner join empresa on (car_glosa_cab.codemp = empresa.codemp)
WHERE 
	 ((car_glosa_cab.anyo=:anyo) AND (car_glosa_cab.estado in ('2','3')))
" update=" car_glosa_cab" updatewhere=0 updatekeyinplace=yes arguments=(("anyo", number))  sort="num_glosa D " )
