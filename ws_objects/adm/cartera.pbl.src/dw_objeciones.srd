 column=(type=char(4) updatewhereclause=yes name=tipo_auditoria dbname="glosa_objeciones.tipo_auditoria" dbalias=".tipo_auditoria" )
 retrieve="SELECT 
	glosa_objeciones.cod_objecion, glosa_objeciones.descripcion, glosa_objeciones.subsanable, 
	glosa_objeciones.tiempo_resp, glosa_objeciones.pertinente, glosa_objeciones.estado, 
	glosa_objeciones.con_gral, glosa_objeciones.con_espe, glosa_objeciones.tipo_auditoria
FROM 
	glosa_objeciones  INNER JOIN pm_glosa_version ON glosa_objeciones.cod_version = pm_glosa_version.cod_version
WHERE 
	(((glosa_objeciones.con_gral)=:gral) AND ((glosa_objeciones.con_espe)=:espe) 
	AND ((glosa_objeciones.tipo_auditoria) Is Not Null)
	AND (getdate() between pm_glosa_version.valido_inicio and pm_glosa_version.valido_hasta));

" arguments=(("gral", string),("espe", string))  sort="glosa_objeciones_con_gral A glosa_objeciones_con_espe A tipo_auditoria A cod_objecion A descripcion A " )
