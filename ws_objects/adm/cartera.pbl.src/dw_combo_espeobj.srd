	glosa_objeciones.con_espe, glosa_objeciones.descripcion,
	glosa_objeciones.cod_objecion
FROM 
	glosa_objeciones 
	INNER JOIN pm_glosa_version ON glosa_objeciones.cod_version = pm_glosa_version.cod_version
WHERE 
	 (((glosa_objeciones.con_espe) Is Not Null) 
	AND ((glosa_objeciones.con_gral)=:gral) 
	AND ((glosa_objeciones.estado)='1') 
	AND ((glosa_objeciones.tipo_auditoria) Is Null ) 
