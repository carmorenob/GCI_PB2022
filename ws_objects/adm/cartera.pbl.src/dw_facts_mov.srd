 retrieve="SELECT 
	FactCab.NFact, FactCab.clugar, FactCab.Fecha, FactCab.CEmp, FactCab.CContrato, FactCab.VTProced, 
	FactCab.VTEmp,  FactCab.VTAbono, car_mov_rad.monto,car_mov_rad.descuentos, FactCab.VTEmp-FactCab.VTEmp AS valor_pago, 1 AS selec,
	 case when valor_aceptado is null then 0 else valor_aceptado end AS glosa,FactCab.tipo,DOCUMENTOS_AUTFACT.PREFIJO
FROM ((
	FactCab LEFT JOIN
	(	SELECT 
			car_cobro_rad.clugar, car_cobro_rad.coddoc, car_cobro_rad.num_cobro, car_cobro_rad.nfact, car_cobro_rad.clugar_fact, car_cobro_rad.TIPO_FAC, 
			Sum(car_cobro_rad.valor) AS monto, sum(case when car_cobro_rad.des_ajus is null then 0 else car_cobro_rad.des_ajus end )  AS descuentos, 
			car_cobro_rad.TIPO_RAD
	FROM car_cobro_rad
	WHERE 
		(((car_cobro_rad.num_radicacion)=:nradica) AND ((car_cobro_rad.clugar_rad)=:lugar) AND ((car_cobro_rad.TIPO_RAD)=:tipo_rad))
	GROUP BY 
		car_cobro_rad.clugar, car_cobro_rad.coddoc, car_cobro_rad.num_cobro, car_cobro_rad.nfact, car_cobro_rad.clugar_fact, 
		car_cobro_rad.TIPO_FAC,car_cobro_rad.TIPO_RAD
	)  AS car_mov_rad ON (FactCab.TIPO = car_mov_rad.TIPO_FAC) AND (FactCab.NFact = car_mov_rad.nfact) AND (FactCab.clugar = car_mov_rad.clugar_fact)) 
	LEFT JOIN
	 (	SELECT 
			car_glosa_cpo.nfact, car_glosa_cpo.clugar_fact, car_glosa_cpo.TIPO, car_glosa_cpo.valor_aceptado
		FROM 
			(((((CAR_COBRO_CAB INNER JOIN CAR_COBRO_CPO ON (CAR_COBRO_CAB.num_cobro = CAR_COBRO_CPO.num_cobro) AND 
			(CAR_COBRO_CAB.coddoc = CAR_COBRO_CPO.coddoc) AND (CAR_COBRO_CAB.clugar = CAR_COBRO_CPO.clugar)) 
			INNER JOIN car_glosa_cab ON (CAR_COBRO_CPO.NUM_GLOSA = car_glosa_cab.num_glosa) AND (CAR_COBRO_CPO.clugar_glosa = car_glosa_cab.clugar) 
			AND (CAR_COBRO_CPO.CodDoc_glosa = car_glosa_cab.coddoc)) 
			INNER JOIN car_glosa_cpo ON (car_glosa_cab.num_glosa = car_glosa_cpo.num_glosa) AND (car_glosa_cab.clugar = car_glosa_cpo.clugar) AND 
			(car_glosa_cab.coddoc = car_glosa_cpo.coddoc)) 
			INNER JOIN car_tipo ON CAR_COBRO_CPO.cartipo = car_tipo.cartipo) 
			INNER JOIN CAR_COBRO_CPO AS CAR_COBRO_CPO_1 ON (CAR_COBRO_CPO.num_cobro = CAR_COBRO_CPO_1.num_cobro) AND 
			(CAR_COBRO_CPO.coddoc = CAR_COBRO_CPO_1.coddoc) AND (CAR_COBRO_CPO.clugar = CAR_COBRO_CPO_1.clugar)) 
			INNER JOIN car_tipo AS car_tipo_1 ON CAR_COBRO_CPO_1.cartipo = car_tipo_1.cartipo
		WHERE 
			(((CAR_COBRO_CPO.estado) Is Null) AND ((CAR_COBRO_CAB.estado) Is Null) AND ((CAR_COBRO_CAB.cxp)<>'1') AND 
			((car_tipo.codtipo)<>'0') AND ((car_tipo.operacion)=-1) AND ((car_tipo_1.codtipo)='0') AND 
			((CAR_COBRO_CPO_1.num_radicacion)=:nradica) AND ((CAR_COBRO_CPO_1.clugar_rad)=:lugar) AND ((CAR_COBRO_CPO_1.TIPO_RAD)=:tipo_rad))
		)  AS glosas ON (FactCab.TIPO = glosas.TIPO) AND (FactCab.NFact = glosas.nfact) AND (FactCab.clugar = glosas.clugar_fact))
		 LEFT JOIN DOCUMENTOS_AUTFACT ON FactCab.C_AUT = DOCUMENTOS_AUTFACT.C_AUT
WHERE (((FactCab.nradica)=:nradica) AND ((FactCab.clugar_rad)=:lugar) AND ((FactCab.TIPO_RAD)=:tipo_rad));
" arguments=(("nradica", number),("lugar", string),("tipo_rad", string),("cartipo", string),("item", number)) )
