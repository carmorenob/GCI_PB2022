table(column=(type=number updatewhereclause=yes name=num_radicacion dbname="ripsradica.num_radicacion" )
 column=(type=char(2) updatewhereclause=yes name=clugar dbname="ripsradica.clugar" )
 column=(type=datetime updatewhereclause=yes name=fecha_radica dbname="ripsradica.fecha_radica" )
 column=(type=char(120) updatewhereclause=yes name=desemp dbname="empresa.desemp" )
 column=(type=decimal(2) updatewhereclause=yes name=valor_capita dbname="ripsradicadet.valor_capita" )
 column=(type=decimal(2) updatewhereclause=yes name=valor_evento dbname="ripsradicadet.valor_evento" )
 column=(type=char(1) updatewhereclause=yes name=ctipo dbname="ripsradicadet.ctipo" values="Evento	1/Capitado	2/" )
 column=(type=long updatewhereclause=yes name=esco dbname="esco" values="	1/	0" )
 column=(type=char(4) updatewhereclause=yes name=contratos_cod_empre_pxc dbname="contratos.cod_empre_pxc" )
 column=(type=char(50) updatewhereclause=yes name=contratos_codtotal_pxc dbname="contratos.codtotal_pxc" )
 column=(type=char(1) updatewhereclause=yes name=tipo_rad dbname="ripsradica.tipo_rad" values="Relación	R/Factura	F/" )
 column=(type=decimal(2) updatewhereclause=yes name=valor_ndb dbname="contratos.valor_ndb" )
 column=(type=decimal(2) updatewhereclause=yes name=valor_ncr dbname="contratos.valor_ncr" )
 retrieve="SELECT 
 	ripsradica.num_radicacion, ripsradica.clugar, ripsradica.fecha_radica, empresa.desemp, 
	ripsradicadet.valor_capita, ripsradicadet.valor_evento, ripsradicadet.ctipo, 0 AS esco, 
	contratos.cod_empre_pxc, contratos.codtotal_pxc, ripsradica.tipo,ripsradicadet.valor_ndb,ripsradicadet.valor_ncr
FROM 
	((ripsradica INNER JOIN ripsradicadet ON (ripsradica.TIPO = ripsradicadet.TIPO) AND (ripsradica.CLUGAR = ripsradicadet.CLUGAR) 
	AND (ripsradica.NUM_RADICACION = ripsradicadet.NUM_RADICACION)) INNER JOIN empresa ON ripsradica.CEMP = empresa.CODEMP) 
		INNER JOIN contratos ON (ripsradicadet.CCONTRATO = contratos.CODCONTRATO) AND (ripsradicadet.CEMP = contratos.CODEMP)
WHERE
	 (((ripsradica.estado) Is Null) AND ((empresa.tipodoc)=:tdoc) AND ((empresa.nit)=:docu) 
	AND ((ripsradica.en_destino)='0') )

" arguments=(("tdoc", string),("docu", string))  sort="num_radicacion D tipo_rad D fecha_radica D " )
