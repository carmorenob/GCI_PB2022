 retrieve="SELECT 
	contra.ano,contra.ncontrato,contra.otrosi, contra.objeto,   
	contra.inicia,contra.termina,contra.monto,contra.estado,   
	contra.archivo,contra.juridico,contra.indobje,contra.indmonto,   
	contra.indfecha,contra.indfpago  
FROM 
	contra  
WHERE 
	(((contra.ano)=:ano) AND ((contra.ncontrato)=:ncontrato) AND ((contra.otrosi)>0 And (contra.otrosi)<100)) 
	OR 
	(((contra.ano)=:ano) AND ((contra.ncontrato)=:ncontrato) AND ((contra.otrosi)>0 And (contra.otrosi)>100 And (contra.otrosi)<1000)) 
	OR (((contra.ano)=:ano) AND ((contra.ncontrato)=:ncontrato) AND ((contra.otrosi)>0 And (contra.otrosi)>1000));
" update="contra" updatewhere=1 updatekeyinplace=yes arguments=(("ano", number),("ncontrato", number)) )
