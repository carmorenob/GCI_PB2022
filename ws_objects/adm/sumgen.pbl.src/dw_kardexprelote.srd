 retrieve="  SELECT sum_kardex.codalmacen,   
         sum_kardex.codarticulo,   
         sum_kardex.costoprom,   
         sum_kardex.saldoactual,   
         sumarticulo.descripcion,   
         sumarticulo.act_fijo,   
         sumarticulo.vence,   
         sum_lote.fecha_venc,   
         sum_lote.lote,   
         sum_lote.convenio,   
         sum_lote.cantidad,0 as selec ,
(select  n_color 
from medica_triage
where 
(datediff(dd,now(),sum_lote.fecha_venc))
between inicial and final) as n_color,    sumarticulo.codgenerico, sumarticulo.tra_articulo
FROM (sum_kardex LEFT OUTER JOIN sum_lote ON (sum_kardex.CODARTICULO = sum_lote.CODARTICULO) AND
 (sum_kardex.CODALMACEN = sum_lote.CODALMACEN)) 
INNER JOIN sumarticulo ON sum_kardex.CODARTICULO = sumarticulo.codArticulo
WHERE sum_kardex.CODALMACEN=:codalmacen and sum_kardex.saldoactual > 0 and 
(sum_lote.cantidad > 0 or sum_lote.cantidad is NULL)
" arguments=(("codalmacen", string))  sort="descripcion A " )
