 retrieve="SELECT 
	contra_articulo.ano, contra_articulo.ncontrato, contra_articulo.otrosi, contra_articulo.item, 
	contra_articulo.codarticulo, contra_articulo.valor_cont,
	contra_articulo.usuario, usuarios.nombre, sumarticulo.descripcion,contra_articulo.cod_impu
FROM 
	(contra_articulo INNER JOIN sumarticulo ON contra_articulo.codarticulo = sumarticulo.codarticulo) 
	INNER JOIN usuarios ON contra_articulo.usuario = usuarios.usuario
WHERE 
	(((contra_articulo.ano)=:anyo) AND ((contra_articulo.ncontrato)=:ncon) 
	AND ((contra_articulo.otrosi)=:otros));" update="contra_articulo" updatewhere=1 updatekeyinplace=no arguments=(("anyo", number),("ncon", number),("otros", number)) )
