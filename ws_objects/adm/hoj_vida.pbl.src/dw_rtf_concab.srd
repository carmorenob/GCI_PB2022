 retrieve="SELECT contra.ano, contra.ncontrato, contra.otrosi, contra.OBJETO, contra.INICIA, contra.TERMINA, contra.MONTO, 
contra.ACUM_PAGO, contra.INTERVENTOR, contra.estado, contra.archivo, contra.CodDoc, contra.clugar, 
contra.TipoDoc, contra.formapago, contra.documento, terceros.Persona, terceros.nombre1, terceros.nombre2, terceros.apellido1, 
terceros.APELLIDO2, terceros.razon_social, terceros.Direccion, terceros.Tel1, terceros.email, terceros.CDpto, terceros.contacto,
terceros.CCiudad, departamentos.Nombre, ciudades.DesCiudad, contra.juridico, contra.indobje, contra.indmonto, 
contra.indfecha, contra.indfpago, contra.continua, contra.cod_flujo, contra.en_destino, contra.tipo, 
contra.codcontrato, contra.dias, contra.meses, CONTRA_TIPO.descripcion, Documentos_gral.DesDoc, terceros.dverificacion
FROM ((contra LEFT JOIN ((terceros LEFT JOIN departamentos ON terceros.CDpto = departamentos.CodDepar) LEFT JOIN ciudades ON (terceros.CCiudad = ciudades.CodCiudad) AND (terceros.CDpto = ciudades.CDepto)) ON (contra.documento = terceros.documento) AND (contra.TipoDoc = terceros.TipoDoc)) LEFT JOIN Documentos_gral ON contra.CodDoc = Documentos_gral.CodDoc) LEFT JOIN CONTRA_TIPO ON contra.tipo = CONTRA_TIPO.tipo
WHERE (((contra.ano)=:ano) AND ((contra.ncontrato)=:ncontrato) AND ((contra.otrosi)=:otrosi))
" update="CONTRA_TIPO" updatewhere=0 updatekeyinplace=no arguments=(("ano", number),("ncontrato", number),("otrosi", number)) )
