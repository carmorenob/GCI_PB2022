 retrieve="SELECT 
	cos_deprecia_mes.ano, cos_deprecia_mes.mes, cos_deprecia_mes.coduf, cos_deprecia_mes.codcc, 
	cos_deprecia_mes.valor, cos_deprecia_mes.estado, ufuncional.desufuncional, 
	centrocostos.descripcion, lugar.Descripcion AS ldes,cos_deprecia_mes.clugar
FROM 
	((cos_deprecia_mes INNER JOIN centrocostos ON (cos_deprecia_mes.codcc = centrocostos.CodCC) AND (cos_deprecia_mes.coduf = centrocostos.CodUF)) 
	INNER JOIN ufuncional ON centrocostos.CodUF = ufuncional.CodUF) INNER JOIN lugar ON cos_deprecia_mes.clugar = lugar.codlugar
WHERE 
	(((cos_deprecia_mes.ano)=:ano) AND ((cos_deprecia_mes.mes between :mes_i and :mes_f)))

" update="cos_deprecia_mes" updatewhere=0 updatekeyinplace=no arguments=(("ano", number),("mes_i", number),("mes_f", number))  sort="clugar A coduf A codcc A " )
