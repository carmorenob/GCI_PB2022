global function integer f_copy_filas_sel (datawindow dw, long pos);long j
for j=1 to dw.rowcount()
	if dw.isselected(j) then
		dw.RowsCopy(j, j, Primary!, dw, pos +1, Primary!)
	end if
next
return 1
end function

