 retrieve="SELECT DISTINCT
	documentos_autfact.prefijo ||
	ltrim(to_char( 990000000+ripsradicadet.num_radicacion,'99999999999'))|| 
	to_char(ripsradica.fecha_radica,'YYYY-MM-DDHH24:MI:SS-05:00')||
	ltrim(to_char( ripsradica.valor_capita,'99999999999.99')) ||  '01'||
	ltrim(to_char( 0.00,'99999999999.99')) ||
	'040.00'||'030.00' ||
	ltrim(to_char( ripsradica.valor_capita,'99999999999.99')) ||
	ips.documento||
	empresa.nit as cufe,
	
	documentos_autfact.prefijo ||
	ltrim(to_char( 990000000+ripsradicadet.num_radicacion,'99999999999'))|| 
	to_char(ripsradica.fecha_radica,'YYYY-MM-DDHH24:MI:SS-05:00')||
	ltrim(to_char( ripsradica.valor_capita,'99999999999.99')) || 
	ltrim(to_char( 0.00,'99999999999.99')) ||
	'040.00'||'030.00' ||
	ltrim(to_char( ripsradica.valor_capita,'99999999999.99')) ||
	ips.documento||
	empresa.nit as small_cufe,

    documentos_autfact.dian, documentos_autfact.f_ini, 
	documentos_autfact.f_fin, documentos_autfact.n_ini, documentos_autfact.n_fin, documentos_autfact.prefijo, 
	documentos_autfact.provider_id, documentos_autfact.dv_provider, documentos_autfact.software_id, 
	documentos_autfact.clave_tecnica, documentos_autfact.testp,documentos_autfact.pin,
	replicate('',255) as huella,

	documentos_autfact.tipo_ambiente, 
	 990000000+ripsradicadet.num_radicacion as nfact, 
	ips.nombre as ips_descripcion, ips.direccion as ipsdireccion, ips.telefono as ipstelefono, 
	ips.tipo_doc as ipstd, ips.documento as documento,ips.dverificacion as ipsdv , 

	empresa.tipodoc, empresa.nit, terceros.dverificacion, terceros.persona, 
	terceros.nombre1, terceros.nombre2, 
	terceros.apellido1, terceros.apellido2, terceros.razon_social, 
	terceros_direccion.direccion as direccion_cliente, terceros_direccion.tel1 as telefono_cliente,

	ripsradica.fecha_radica as fecha_factura,
	ripsradica.fecha_radica as hora_factura,
	'999' as cd_prod,
	ripsradica.objeto  as ds_prod,
	
	1 as cantidad, 0 as vproced, 
	ripsradica.valor_capita as vemp, 0 as vcopago, 
	0 as vcm, 0 as vcr, 0 as viva, 
	0 as iva_cm, 0 as iva_cr,0 as iva_cp, 

	ripsradica.valor_capita as vtproced, ripsradica.valor_capita as vtemp, 
	0 as vtdescuentos, 0 as vtcopago, 0 as vtcmod,0 as vtcrec, 
	0 as vtpaciente,0 as vtiva,

	ips.correo,  ips.depto_ips as cod_depto, ips.ciudad_ips as cod_ciudad, 
	departamentos.nombre as depto_facturador , ciudades.desciudad as  ciudad_facturador,
 	ips.tipo_reg as ipsregimen_fiscal, 
	ips.tipo_ret as ipsresponsabilidad_fiscal,
	terceros_direccion.cdpto as id_depto_cliente, terceros_direccion.cciudad as id_ciudad_cliente, 	
	departamentos_1.nombre as depto_cliente , ciudades_1.desciudad as  ciudad_cliente, 
	ips.cod_postal as codigo_postal, terceros_direccion.cod_postal as codigo_postal_cliente, 
	terceros.contacto as nombre_responsable, terceros.tipo_ret as obligacion, terceros.tipo_reg as respon_iva, terceros.contacto, 
	0 as percent_iva, '' as nd_med_conce,
	
	'94' as cumd, 'UNIDAD'  as nom_umd, 
	ripsradica.objeto as observaciones, terceros_direccion.email as email_cliente, ripsradica.estado as estado, 
	'' as singnig_time, '' as issuername, '' as serialnumber, '' as cert_digest, 
	ripsradica.estado_dian,	ripsradica.file_name_fact, ripsradica.file_name_zip, ripsradica.track_id, 
	ripsradica.clugar, ripsradica.tipo, 
	ips.c_supersalud ,
	
	'' as tdpac,'' as ti, '' as docpac, '' as a1pac, '' as a2pac, 
	'' as n1pac, ''as n2pac, '' as codta, contratos.mp_fems, pm_modalidapago_fems.descripcion as mpnm_fems, 
	tiporegimen.pl_fems, pm_planbene_fems.descripcion as plnm_fems, factcpo.autorizacion, null as mipres, null as idmipres, 
	contratos.nro_contrato, null as poliza,ips.nom1_rpl,ips.nom2_rpl,ips.ape1_rpl,ips.ape2_rpl,
	documentos.ruta_certificado, documentos.clave_certificado,

	(select tipousuario from tipousuario where tipousuario=pacientes.tu and (codta =emppac.codta  or codta is null)) as cod_tipousuario,
	(select descripcion from tipousuario where tipousuario=pacientes.tu and (codta =emppac.codta  or codta is null)) as nom_tipousuario,
	'' as cumdp, '' as nom_umdp,
	 ripsradica.fecha_inicial as ini_per, ripsradica.fecha_final as fin_per,
	pm_bancos.nombre as nombanco, contratos.tipocuenta, contratos.cuenta,
	ripsradica.cufe_dian,ripsradica.motivo as motivo_anula,
	1 as id_nota,ripsradica.fecha_anula
FROM 
	(((((((((((((((((((((ripsradica INNER JOIN ripsradicadet ON (ripsradica.num_radicacion = ripsradicadet.num_radicacion) 
	AND (ripsradica.clugar = ripsradicadet.clugar) AND (ripsradica.tipo = ripsradicadet.tipo)) 
	INNER JOIN contratos ON (ripsradicadet.cemp = contratos.codemp) AND (ripsradicadet.ccontrato = contratos.codcontrato)) 
	INNER JOIN empresa ON contratos.codemp = empresa.codemp) 
	INNER JOIN terceros ON (empresa.nit = terceros.documento) AND (empresa.tipodoc = terceros.tipodoc)) 
	INNER JOIN documentos_autfact ON ripsradica.c_aut = documentos_autfact.c_aut) 
	INNER JOIN ips ON ripsradica.c_ips = ips.c_ips) 
	INNER JOIN terceros_direccion ON (terceros.tipodoc = terceros_direccion.tipodoc) 
	AND (terceros.documento = terceros_direccion.documento)) 
	INNER JOIN ciudades ON (ips.depto_ips = ciudades.cdepto) AND (ips.ciudad_ips = ciudades.codciudad)) 
	INNER JOIN departamentos ON ciudades.cdepto = departamentos.coddepar) 
	INNER JOIN tiporegimen ON contratos.ctreg = tiporegimen.tipo) 
	INNER JOIN pm_modalidapago_fems ON contratos.mp_fems = pm_modalidapago_fems.codigo) 
	INNER JOIN pm_planbene_fems ON tiporegimen.pl_fems = pm_planbene_fems.codigo) 
	INNER JOIN ciudades AS ciudades_1 ON (terceros_direccion.cdpto = ciudades_1.cdepto) 
	AND (terceros_direccion.cciudad = ciudades_1.codciudad)) 
	INNER JOIN departamentos AS departamentos_1 ON ciudades_1.cdepto = departamentos_1.coddepar) 
	LEFT JOIN factcab ON (ripsradica.tipo = factcab.tipo_rad) AND (ripsradica.clugar = factcab.clugar_rad) 
	AND (ripsradica.num_radicacion = factcab.nradica)) 
	LEFT JOIN factcpo ON (factcab.tipo = factcpo.tipo) AND (factcab.clugar = factcpo.clugar) AND (factcab.nfact = factcpo.nfact)) 
	LEFT JOIN pacientes ON (factcpo.documento = pacientes.documento) AND (factcpo.tipodoc = pacientes.tipodoc)) 
	LEFT JOIN tipoidentificacion ON pacientes.tipodoc = tipoidentificacion.codtipoident) 
	LEFT JOIN pm_tipoidentificacion_fems ON tipoidentificacion.ti_fems = pm_tipoidentificacion_fems.codigo) 
	LEFT JOIN emppac ON (factcpo.tipodoc = emppac.tipodoc) AND (factcpo.documento = emppac.documento) 
	AND (factcpo.cemp = emppac.codemp) AND (factcpo.ccontrato = emppac.codcontrato)) 
	INNER JOIN documentos ON documentos_autfact.coddoc = documentos.coddoc) 
	LEFT JOIN pm_bancos ON contratos.banco = pm_bancos.codigo
WHERE 
	(((documentos_autfact.coddoc)='RV') AND ((ripsradica.num_radicacion)=:nfact) 
