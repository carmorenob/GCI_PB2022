 column=(type=char(50) updatewhereclause=yes name=descripcion_cons dbname="citasasig.descripcion_cons" dbalias=".descripcion_cons" )
 column=(type=datetime updatewhereclause=yes name=serciosturno_fecha dbname="citasasig.serciosturno_fecha" dbalias=".serciosturno_fecha" )
 column=(type=datetime updatewhereclause=yes name=serciosturno_hora dbname="citasasig.serciosturno_hora" dbalias=".serciosturno_hora" )
 column=(type=char(10) updatewhereclause=yes name=codprof dbname="citasasig.codprof" dbalias=".codprof" )
 retrieve="SELECT 
	SERCIOSTURNO.nfact, SERCIOSTURNO.clugar_fac, SERCIOSTURNO.nitem, 
	profe.nombre1|| case when profe.nombre2  is null then ' ' else  '  '||profe.nombre2 end ||' '|| profe.apellido1|| case when profe.apellido2 is null then ' ' else  '  '||profe.apellido2 end as desprof, 
	CONSULTORIO.descripcion AS descripcion_cons, SERCIOSTURNO.fecha AS SerciosTurno_Fecha, 
	SERCIOSTURNO.hora AS SerciosTurno_Hora,profe.codprof
FROM 
	((SERCIOSTURNO LEFT JOIN PROFE ON SERCIOSTURNO.Prof = PROFE.CodProf) LEFT JOIN CONSULTORIO ON 
	SERCIOSTURNO.Consult = CONSULTORIO.CodConsultorio) LEFT JOIN citasasig ON (SERCIOSTURNO.clugar = citasasig.clugar) AND 
	(SERCIOSTURNO.NCita = citasasig.NCita)
WHERE
	 (((SERCIOSTURNO.nfact)=:nfacts) AND ((SERCIOSTURNO.TIPO_FAC)=:tipo) AND ((SERCIOSTURNO.nitem)=:item) AND 
	((SERCIOSTURNO.clugar_fac)=:clugars) AND ((SERCIOSTURNO.control)='1' Or (SERCIOSTURNO.control)='2' Or (SERCIOSTURNO.control)='3'));" arguments=(("nfacts", number),("tipo", string),("clugars", string),("item", number))  sort="serciosturno_fecha A serciosturno_hora A " )
