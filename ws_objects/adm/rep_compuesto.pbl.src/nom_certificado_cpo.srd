 column=(type=char(15) updatewhereclause=yes name=documento dbname="NOM_CONCEP.DOCUMENTO" )
 column=(type=char(6) updatewhereclause=yes key=yes name=cod_concep dbname="NOM_CONCEP.COD_CONCEP" )
