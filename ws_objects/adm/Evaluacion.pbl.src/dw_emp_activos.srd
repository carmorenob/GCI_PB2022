terceros.nombre1, terceros.nombre2, terceros.apellido1, terceros.APELLIDO2, 
emplecargo.NCARGO, emplecargo.cargo, emplecargo.ESTADO as estado_cargo, CARGOS.nombre_cargo, 1 AS sel, 
emplecargo.tipodoc_jefe, emplecargo.documento_jefe
FROM (terceros INNER JOIN (empleados 
INNER JOIN emplecargo ON (empleados.documento = emplecargo.documento) AND (empleados.TipoDoc = emplecargo.TipoDoc)) ON (terceros.documento = empleados.documento) AND (terceros.TipoDoc = empleados.TipoDoc)) 
INNER JOIN CARGOS ON emplecargo.cargo = CARGOS.cargo
" )
