 retrieve="SELECT AF_REGISTRO_CPO.Placa, AF_REGISTRO_CPO.Carticulo AS codarticulo, SUMARTICULO.DESCRIPCION, af_respon.codalmacen, 
SUMALMACEN.DESCRIPCION AS des_almacen, af_respon_usu.tipodoc, af_respon_usu.documento, 
terceros.nombre1, terceros.nombre2, terceros.apellido1, terceros.apellido2, terceros.razon_social, AF_REGISTRO_CPO.Asignado, 0 as selec
FROM ((((AF_REGISTRO_CPO INNER JOIN af_respon ON AF_REGISTRO_CPO.Placa = af_respon.placa) INNER JOIN af_respon_usu ON af_respon.placa = af_respon_usu.placa) INNER JOIN SUMARTICULO ON AF_REGISTRO_CPO.Carticulo = SUMARTICULO.codArticulo) INNER JOIN SUMALMACEN ON af_respon.codalmacen = SUMALMACEN.CODALMACEN) INNER JOIN terceros ON (af_respon_usu.documento = terceros.documento) AND (af_respon_usu.tipodoc = terceros.tipodoc)
WHERE (((AF_REGISTRO_CPO.Carticulo)=:articulo) AND ((AF_REGISTRO_CPO.estado)='1') AND ((af_respon.estado)='1') AND ((af_respon_usu.estado)='1'))
and af_respon.codalmacen=:alma

UNION ALL SELECT AF_REGISTRO_CPO.Placa, AF_REGISTRO_CPO.Carticulo AS codarticulo, SUMARTICULO.DESCRIPCION, AF_REGISTRO_CPO.CODALMACEN_KAR,
SUMALMACEN.DESCRIPCION AS des_almacen, '' AS tipodoc, '' as documento, 
'' as nombre1,'' as nombre2, '' as apellido1,'' as apellido2,'' as razon_social , AF_REGISTRO_CPO.Asignado, 0 as selec
FROM (AF_REGISTRO_CPO INNER JOIN SUMALMACEN ON AF_REGISTRO_CPO.CODALMACEN_KAR = SUMALMACEN.CODALMACEN) INNER JOIN SUMARTICULO ON AF_REGISTRO_CPO.Carticulo = SUMARTICULO.codArticulo
WHERE (((AF_REGISTRO_CPO.Carticulo)=:articulo) AND ((AF_REGISTRO_CPO.Asignado)='0') AND ((AF_REGISTRO_CPO.estado)='1'))
and  AF_REGISTRO_CPO.CODALMACEN_KAR =:alma" update="terceros" updatewhere=1 updatekeyinplace=no arguments=(("alma", string),("articulo", string)) )
