event clicked;if not enabled then return
if not isvalid(w_asig_cita) then
	this.enabled=false
	return
end if

if messagebox("Atención","Está seguro que desea confirmar el turno??",question!,yesno!,1)=2 then return

long ncita,nserv,j,l_n_citas,l_n_turno, sec_cant,donde,n_citas
string proc_cita,clug_cita, cod_fina,des_fina,cod_fina_cm

j=w_asig_cita.dw_1.getrow()
ncita=w_asig_cita.dw_1.getitemnumber(j,"ncita"+right(w_asig_cita.dw_1.getcolumnname(),1))
proc_cita=w_asig_cita.dw_1.getitemstring(j,"proc"+right(w_asig_cita.dw_1.getcolumnname(),1))
nserv=w_asig_cita.dw_1.getitemnumber(j,"nserv"+right(w_asig_cita.dw_1.getcolumnname(),1))
sec_cant=w_asig_cita.dw_1.getitemnumber(j,"sec_cant"+right(w_asig_cita.dw_1.getcolumnname(),1))
clug_cita=w_asig_cita.i_clug

SELECT citasasig.cod_fina, finconsulta.descripcion into :cod_fina,:des_fina
FROM citasasig LEFT JOIN finconsulta ON citasasig.cod_fina = finconsulta.codfin
WHERE (((citasasig.ncita)=:ncita) AND ((citasasig.clugar)=:clug_cita));
if sqlca.sqlcode=-1 then
	messagebox("Error leyendo Citas finalidad",sqlca.sqlerrtext)
	return 
end if
if not isnull(cod_fina) then 
	if messagebox("Atención","Está seguro que desea confirmar la cita para la finalidad ("+cod_fina+ ") "+des_fina ,question!,yesno!,1)=2 then return
	//cod_fina_cm=w_asig_cita.tab_2.tp2_2.dw_citas_ante.getitemstring(w_asig_cita.tab_2.tp2_2.dw_citas_ante.getrow(),'cod_fina')
	donde=w_asig_cita.tab_2.tp2_2.dw_citas_ante.find("(ncita="+string(ncita)+" and clugar='"+clug_cita+"')",1,w_asig_cita.tab_2.tp2_2.dw_citas_ante.rowcount())
	cod_fina_cm=w_asig_cita.tab_2.tp2_2.dw_citas_ante.getitemstring(donde,'cod_fina')
	if cod_fina_cm<>cod_fina then
		select descripcion into :des_fina  from finconsulta where  codfin=:cod_fina_cm;
		if messagebox("Atención","Está seguro que desea confirmar la cita con la finalidad ("+cod_fina_cm+ ") "+des_fina+' que ajusto' ,question!,yesno!,1)=2 then return
			if w_asig_cita.tab_2.tp2_2.dw_citas_ante.update() =-1 then
				rollback;
				return
			end if
	end if
else
	cod_fina=w_asig_cita.tab_2.tp2_2.dw_citas_ante.getitemstring(w_asig_cita.tab_2.tp2_2.dw_citas_ante.getrow(),'cod_fina')
	if isnull(cod_fina) then 
		messagebox("Atención",'Hay cita sin asignar finalidad asignela')
		return
	else
		if w_asig_cita.tab_2.tp2_2.dw_citas_ante.update() =-1 then
			rollback;
			return
		end if
	end if
end if

SELECT  count(SerciosCita.NServicio) into :n_citas
FROM SerciosCita 
WHERE (((SerciosCita.NCita)=:ncita) AND ((SerciosCita.clugar)=:clug_cita));
if sqlca.sqlcode=-1 then
	messagebox("Error leyendo numero prouctos citas ",sqlca.sqlerrtext)
	return 
end if


SELECT  SerciosCita.cantidad, SerciosCita.NTurnos into:l_n_citas,:l_n_turno 
FROM SerciosCita 
WHERE (((SerciosCita.NCita)=:ncita) AND ((SerciosCita.NServicio)=:nserv) AND ((SerciosCita.clugar)=:clug_cita));
if sqlca.sqlcode=-1 then
	messagebox("Error leyendo cupos de las citas ",sqlca.sqlerrtext)
	return 
end if
if l_n_turno > 2 or n_citas>1 then
	UPDATE serciosturno 	SET control = '5'
	WHERE (serciosturno.ncita=:ncita) AND (serciosturno.clugar=:clug_cita) and (serciosturno.control='1');
else
	update serciosturno set control='5' where ncita=:ncita and clugar=:clug_cita and nservicio=:nserv 
	and sec_cant=:sec_cant;
end if
if sqlca.sqlcode=-1 then
	Messagebox("Error",sqlca.sqlerrtext)
	rollback;
end if
update citasasig set estado='5' where ncita=:ncita and clugar=:clug_cita;
if sqlca.sqlcode=-1 then
	Messagebox("Error",sqlca.sqlerrtext)
	rollback;
end if

commit;
this.enabled=false
end event

