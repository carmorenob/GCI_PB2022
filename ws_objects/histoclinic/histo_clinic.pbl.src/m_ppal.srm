long i,ncit,nserv,sec_cant
string asistio,clug_cita
asistio="no"
i=w_citas_asig.dw_citas.getrow()
ncit=w_citas_asig.dw_citas.getitemnumber(i,"ncita")
nserv=w_citas_asig.dw_citas.getitemnumber(i,"nservicio")
