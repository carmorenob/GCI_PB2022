if nturnos>=2 then
	UPDATE serciosturno 	SET serciosturno.control = '2'
	WHERE (serciosturno.ncita=:ncit) AND (serciosturno.clugar=:clug_cita) and (serciosturno.control='1');
else
	update serciosturno set control='2' where ncita=:ncit and clugar=:clug_cita and nservicio=:nserv 
	and sec_cant=:sec_cant;
end if

if sqlca.sqlcode=-1 then
	Messagebox("Error",sqlca.sqlerrtext)
	rollback;
else
	w_citas_asig.dw_Revisa.retrieve(w_citas_asig.dw_citas.getitemnumber(w_citas_asig.dw_citas.getrow(),"ncita"),w_citas_asig.dw_citas.getitemstring(w_citas_asig.dw_citas.getrow(),"clugar"))
	if w_citas_asig.dw_revisa.find("control<>'2'",1,w_citas_asig.dw_revisa.rowcount())>0 then 
		w_citas_asig.dw_citas.setitem(w_citas_asig.dw_citas.getrow(),"estado","3")
	else
		w_citas_asig.dw_citas.setitem(w_citas_asig.dw_citas.getrow(),"estado","1")
	end if
	if w_citas_asig.dw_citas.update(true,false)=-1 then
		rollback;
		return
