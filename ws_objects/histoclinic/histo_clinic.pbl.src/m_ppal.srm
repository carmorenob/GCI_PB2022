if w_hist_gral.i_contador=-1 then return
string edad ,pedazo
long cuanto
edad=w_principal.dw_1.getitemstring(1,"edad")
pedazo = right(edad,len(edad)-pos(edad," ",1))
cuanto = long(left(edad,pos(edad," ",1) -1))
if w_principal.dw_1.getitemstring(1,"sexo") <> "F" then
	messagebox("Error","No es una mujer")
	return
end if
if pedazo="Meses" or pedazo="Días" then
	messagebox("Error","No tiene edad válida")
	return
end if
if cuanto < 11 or cuanto > 60 then
	messagebox("Error","No tiene edad de gestante")
	return
end if
open(w_embarazo)
	
end event

