	for i=1 to w_citas_asig.dw_Revisa.rowcount()
		if w_citas_asig.dw_Revisa.getitemstring(i,"control")<>"3" then 
			asistio="si"
			exit
		end if
	next
	if asistio="no" then
		w_citas_asig.dw_citas.setitem(w_citas_asig.dw_citas.getrow(),"estado","2")
		if w_citas_asig.dw_citas.update()=-1 then
			rollback;
		else
			commit;
