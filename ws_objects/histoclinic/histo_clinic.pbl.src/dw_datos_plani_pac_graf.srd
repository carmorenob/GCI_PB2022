table(column=(type=decimal(2) updatewhereclause=yes name=edad dbname="edad" )
 column=(type=decimal(2) updatewhereclause=yes name=numero dbname="pacientes.numero" )
 column=(type=datetime updatewhereclause=yes name=fecha_reg dbname="pacientes.fecha_reg" )
 retrieve="SELECT 
	DATEDIFF(DAY, Pacientes.FNacimiento,hclin_registro.fecha_reg) - 0.0 AS EDAD,
	HCLIN_REG_VALOR.numero,
	hclin_registro.fecha_reg
FROM 
	((hclin_registro INNER JOIN Historial ON (hclin_registro.clugar = Historial.clugar) AND 
	(hclin_registro.contador = Historial.Contador)) INNER JOIN HCLIN_REG_VALOR ON 
	(hclin_registro.nregistro = HCLIN_REG_VALOR.nregistro) AND (hclin_registro.clugar = HCLIN_REG_VALOR.clugar) AND 
	(hclin_registro.contador = HCLIN_REG_VALOR.CONTADOR)) INNER JOIN Pacientes ON 
	(Historial.Documento = Pacientes.Documento) AND (Historial.TipoDoc = Pacientes.TipoDoc)
WHERE 
	(((Historial.TipoDoc)='cc') AND ((Historial.Documento)='123') AND ((hclin_registro.codplantilla)='CE06') AND ((HCLIN_REG_VALOR.item)='8'))"  sort="fecha_reg A edad A " )
