table(column=(type=char(10) updatewhereclause=yes name=fecha dbname="fecha" )
 column=(type=char(3) updatewhereclause=yes name=tipodoc dbname="tipodoc" )
 column=(type=char(15) updatewhereclause=yes name=documento dbname="documento" )
 column=(type=long updatewhereclause=yes name=peso dbname="peso" )
 column=(type=long updatewhereclause=yes name=talla dbname="talla" )
 retrieve="SELECT 
	Max( peso_talla.fecha) as fecha ,  peso_talla.TipoDoc, peso_talla. Documento,
    convert(int,case when Avg( peso_talla.peso) is null then 9999 else Avg( peso_talla.peso) end)  as peso, 
    convert(int,case when Avg( peso_talla.talla) is null then 9999 else  Avg( peso_talla.talla) end) as talla
FROM (
SELECT DISTINCT  
	convert(varchar(10),hclin_registro.fecha ,111) as fecha,  Historial.TipoDoc, Historial.Documento,  
	case when  HCLIN_REG_VALOR.varia_salud='30' then case when HCLIN_REG_VALOR.numero=0 then 999 else HCLIN_REG_VALOR.numero end end as PESO, 
	case when  HCLIN_REG_VALOR.varia_salud='32' then case when HCLIN_REG_VALOR.numero=0 then 999 else HCLIN_REG_VALOR.numero end end as TALLA
FROM ((((((Historial INNER JOIN hclin_registro ON (Historial.clugar = hclin_registro.clugar) AND (Historial.Contador = hclin_registro.contador)) INNER JOIN HCLIN_REG_VALOR ON (hclin_registro.nregistro = HCLIN_REG_VALOR.nregistro) AND (hclin_registro.clugar = HCLIN_REG_VALOR.clugar) AND (hclin_registro.contador = HCLIN_REG_VALOR.CONTADOR)) INNER JOIN hclin_plantcampo ON (HCLIN_REG_VALOR.item = hclin_plantcampo.numcampo) AND (hclin_registro.codplantilla = hclin_plantcampo.codplantilla)) INNER JOIN ServiciosIngreso ON (Historial.Contador = ServiciosIngreso.Contador) AND (Historial.clugar = ServiciosIngreso.CLugar)) INNER JOIN FactCpo ON (ServiciosIngreso.Contador = FactCpo.Contador) AND (ServiciosIngreso.CLugar = FactCpo.clugar_ser) AND (ServiciosIngreso.Nservicio = FactCpo.NServicio)) INNER JOIN FactCab ON (FactCpo.NFact = FactCab.NFact) AND (FactCpo.clugar = FactCab.clugar)) INNER JOIN hclin_plant ON hclin_registro.codplantilla = hclin_plant.codplantilla
WHERE (((hclin_plantcampo.varia_salud)='30' Or (hclin_plantcampo.varia_salud)='32') AND ((FactCab.nradica)=11705) AND ((hclin_plant.pedt)='1'))
) as peso_talla
GROUP BY  peso_talla.TipoDoc,  peso_talla.Documento;" )
