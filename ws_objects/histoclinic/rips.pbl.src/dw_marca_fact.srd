 retrieve="SELECT FactCab.CodTIngre, FactCab.NFact, FactCab.Fecha, FactCab.VTProced, FactCab.VTEmp, 
FactCab.clugar, Max(ServiciosIngreso.EstRIA) AS MaxDeEstRIA, v_totales_x_rips.SumaDeVProced, 
FactCab.radicar, FactCab.estado_revisa,FactCab.folios
FROM ((FactCab LEFT outer JOIN v_totales_x_rips ON (FactCab.clugar = v_totales_x_rips.clugar) 
AND (FactCab.NFact = v_totales_x_rips.NFact)) INNER JOIN FactCpo 
ON (FactCab.clugar = FactCpo.clugar) AND (FactCab.NFact = FactCpo.NFact)) 
LEFT outer JOIN ServiciosIngreso ON (FactCpo.NItem = ServiciosIngreso.nitem_fac) 
AND (FactCpo.clugar = ServiciosIngreso.clugar_fac) 
AND (FactCpo.NFact = ServiciosIngreso.nfactura)
WHERE (((FactCab.CEmp)=:emp) AND ((FactCab.CContrato)=:cont) AND ((FactCab.Estado) 
Is Null) AND ((FactCab.nradica) Is Null)) and factcab.clugar=:clug
GROUP BY FactCab.CodTIngre, FactCab.NFact, FactCab.Fecha, FactCab.VTProced, FactCab.VTEmp, 
FactCab.clugar, v_totales_x_rips.SumaDeVProced, FactCab.radicar, FactCab.estado_revisa,FactCab.folios
HAVING (((FactCab.Fecha)>=:fecha1 And (FactCab.Fecha)<=:fecha2) 
AND ((Max(ServiciosIngreso.EstRIA))<>'3' Or (Max(ServiciosIngreso.EstRIA)) Is Null))" update="factcab" updatewhere=0 updatekeyinplace=no arguments=(("emp", string),("cont", string),("fecha1", datetime),("fecha2", datetime),("clug", string)) )
