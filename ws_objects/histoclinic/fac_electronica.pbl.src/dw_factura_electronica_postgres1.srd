 retrieve="SELECT DISTINCT
	:paracita AS paracita, FACTCAB.NFact, FACTCAB.clugar, FACTCAB.TIPO,FACTCAB.Fecha AS FactCab_Fecha, FACTCAB.Hora AS FactCab_Hora, 
	FACTCAB.Estado, EMPRESA.CodSuper, EMPRESA.DesEmp, EMPRESA.Nit, CONTRATOS.DesContrato, IPS.c_supersalud as ipscsuper, IPS.nombre AS ips_nombre, 
	IPS.tipo_doc AS ips_tipodoc, IPS.documento AS ips_nit, IPS.direccion AS ips_dir, FACTCAB.Muestra, usuarios.nombre, 
	EMPRESA.CodEmp, EMPRESA.dverificacion, Lugar.Descripcion AS ld, Lugar.DIRECCION, Lugar.TELEFONO, 
	Lugar.CodLugar, IPS.DVERIFICACION AS ipsdv, CIUDADES.DESCIUDAD AS ipsciu, DEPARTAMENTOS.NOMBRE AS dnom, DOCUMENTOS_AUTFACT.DIAN, 
	DOCUMENTOS_AUTFACT.FECHA_RES,DOCUMENTOS_AUTFACT.N_INI, DOCUMENTOS_AUTFACT.N_FIN, 
	IPS.TELEFONO AS ipst, IPS.INDIC AS ipsind, Lugar.regimen AS lregim, DEPARTAMENTOS.nombre AS dpto, CIUDADES.desciudad, EMPRESA.telefono1,
   	pacientes.TipoDoc, pacientes.Documento, pacientes.Nombre1, pacientes.Nombre2, pacientes.Apellido1, pacientes.Apellido2, 
	pacientes.Direccion as dirpac, pacientes.Tel, Ciudades_1.DesCiudad AS desciudadpac,EMPRESA.direccion AS dir_emp, Ciudades_2.desciudad,
	TIPOREGIMEN.DESCRIPCION AS des_reg,lugar.c_supersalud,DOCUMENTOS_AUTFACT.prefijo, ips.celular,factcab.cufe_dian as cufe,
	terceros_direccion.email,factcab.vtproced, factcab.vtemp, factcab.vtdescuentos, factcab.vtcopago, factcab.vtcmod, factcab.vtcrec, 
	factcab.vtpaciente,factcab.vtiva,contratos.descontrato, contratos.nro_contrato,factcab.cufe_dian_anul as cufe_anul,
	factcab.fecha_anula
FROM 
	((((((((((((((FACTCAB 
	INNER JOIN factcpo ON (FACTCAB.NFACT = FACTCPO.NFACT) AND (FACTCAB.CLUGAR = FACTCPO.CLUGAR) AND (FACTCAB.TIPO = FACTCPO.TIPO)) 
	INNER JOIN pacientes ON (FACTCPO.TIPODOC = pacientes.TIPODOC) AND (FACTCPO.DOCUMENTO = pacientes.DOCUMENTO)) 
	INNER JOIN Ciudades AS Ciudades_1 ON (pacientes.CDEPTO = Ciudades_1.CDEPTO) AND (pacientes.CCIUDAD = Ciudades_1.CODCIUDAD)) 
	INNER JOIN IPS ON FACTCAB.IPS = IPS.C_IPS) 
	INNER JOIN CIUDADES ON (IPS.DEPTO_IPS = CIUDADES.CDEPTO) AND (IPS.CIUDAD_IPS = CIUDADES.CODCIUDAD)) 
	INNER JOIN DEPARTAMENTOS ON CIUDADES.CDEPTO = DEPARTAMENTOS.CODDEPAR) 
	INNER JOIN usuarios ON FACTCAB.USUARIO = usuarios.USUARIO) 
	INNER JOIN Lugar ON FACTCAB.CLUGAR = Lugar.CODLUGAR) 
	INNER JOIN CONTRATOS ON (FACTCAB.CEMP = CONTRATOS.CODEMP) AND (FACTCAB.CCONTRATO = CONTRATOS.CODCONTRATO)) 
	INNER JOIN EMPRESA ON CONTRATOS.CODEMP = EMPRESA.CODEMP) 
	INNER JOIN TIPOREGIMEN ON CONTRATOS.CTREG = TIPOREGIMEN.TIPO) 
	LEFT JOIN ciudades AS Ciudades_2 ON (EMPRESA.CDEPTO = Ciudades_2.CDEPTO) AND (EMPRESA.CCIUDAD = Ciudades_2.CODCIUDAD)) 
	LEFT JOIN DOCUMENTOS_AUTFACT ON (FACTCAB.C_AUT = DOCUMENTOS_AUTFACT.C_AUT)) 
	LEFT JOIN terceros ON (EMPRESA.tipodoc = terceros.tipodoc) AND (EMPRESA.nit = terceros.documento)) 
	LEFT JOIN terceros_direccion ON (terceros.documento = terceros_direccion.documento) AND (terceros.tipodoc = terceros_direccion.tipodoc)
WHERE 
	(((FACTCAB.NFact)=:fact) AND ((FACTCAB.clugar)=:lugar) AND ((FACTCAB.TIPO)=:tipo) AND ((terceros_direccion.ctdir)='D01' or (terceros_direccion.ctdir) is null))" arguments=(("fact", number),("paracita", string),("lugar", string),("tipo", string)) )
