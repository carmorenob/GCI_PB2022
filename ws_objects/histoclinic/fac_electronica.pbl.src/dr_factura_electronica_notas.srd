 retrieve="
SELECT 
	encode(digest(
		case when ma_factura_notas.tipo_nota ='c' then 'NC' else 'ND' end 										||ma_factura.prefijo			|| ma_factura_notas.nro_nota	||					
		to_char(ma_factura_notas.fecha_nota,'YYYY-MM-DDHH24:MI:SS-05:00') || 
		ltrim(to_char(ma_factura_notas.vt_base,'99999999999.99'))|| '01'										|| ltrim(to_char(ma_factura_notas.vt_iva,'99999999999.99'))||
		'040.00'														||'030.00'												|| ltrim(to_char(ma_factura_notas.vt_bruto,'99999999999.99'))||
		pm_gral_lugar.documento 								||ma_factura.documento_ter 						|| '83007' ||
		coalesce(pm_gral_documentos_opciones.tipo_ambiente,''),
	'sha384'),'hex') AS CUFE,

	encode(digest(
		pm_gral_documentos_opciones.prefijo 				|| ma_factura.nro_factura_x_tipo 					|| to_char(fecha_factura,'YYYY-MM-DDHH24:MI:SS-05:00') || 
		ltrim(to_char(ma_factura.vt_base,'99999999999.99'))			|| '01'									|| ltrim(to_char(ma_factura.vt_iva,'99999999999.99'))||
		'040.00'														||'030.00'												|| ltrim(to_char(ma_factura.vt_bruto,'99999999999.99'))||
		pm_gral_lugar.documento 								||ma_factura.documento_ter 						|| coalesce(pm_gral_documentos_opciones.clave_tecnica,'') ||
		coalesce(pm_gral_documentos_opciones.tipo_ambiente,''),
	'sha384'),'hex') AS CUFE_previo,
	
	encode(digest(digest(
		case when ma_factura_notas.tipo_nota ='c' then 'NC' else 'ND' end 										||ma_factura.prefijo			|| ma_factura_notas.nro_nota	||				
		to_char(ma_factura_notas.fecha_nota,'YYYY-MM-DDHH24:MI:SS-05:00') || 
		ltrim(to_char(ma_factura_notas.vt_base,'99999999999.99'))			|| '01'							|| ltrim(to_char(ma_factura_notas.vt_iva,'99999999999.99'))||
		'040.00'														||'030.00'												|| ltrim(to_char(ma_factura_notas.vt_bruto,'99999999999.99'))||
		pm_gral_lugar.documento 								||ma_factura.documento_ter 						|| '83007' ||
		coalesce(pm_gral_documentos_opciones.tipo_ambiente,''),
	'sha384'),'md5'),'hex') AS small_CUFE, 
	
	ma_factura.nro_resolucion,
	pm_gral_documentos_opciones.resolucion_dian, 
	pm_gral_documentos_opciones.fecha_inicio, 
	pm_gral_documentos_opciones.fecha_fin_validez, 
	pm_gral_documentos_opciones.nro_inicial, 
	pm_gral_documentos_opciones.nro_final, 
	pm_gral_documentos_opciones.prefijo,
	pm_gral_documentos_opciones.provider_id, 
	pm_gral_documentos_opciones.dv_provider, 
	pm_gral_documentos_opciones.software_id, 
	pm_gral_documentos_opciones.clave_tecnica,
	encode(digest( pm_gral_documentos_opciones.software_id || '83007' ||case when ma_factura_notas.tipo_nota ='c' then 'NC' else 'ND' end 	||ma_factura.prefijo|| ma_factura_notas.nro_nota,'sha384'),'hex') AS huella, 
	case when ma_factura_notas.tipo_nota ='c' then 'NC' else 'ND' end 										||ma_factura.prefijo			|| ma_factura_notas.nro_nota as ID_nota,
	pm_gral_documentos_opciones.tipo_ambiente,
	ma_factura.nro_factura_x_tipo as nro_factura,
	pm_gral_lugar.descrip_lugar, 
	pm_gral_lugar.direccion, 
	pm_gral_lugar.telefono, 
	pm_gral_lugar.tdoc , 
	pm_gral_lugar.documento , 
	ma_factura.tipo_doc_ter, 
	ma_factura.documento_ter, 
	ma_tercero.dverificacion, 
	ma_tercero.persona, 
	ma_tercero.nombre1, 
	ma_tercero.nombre2, 
	ma_tercero.apellido1, 
	ma_tercero.apellido2, 
	ma_tercero.razon_social, 
	ma_tercero.direccion as direccion_cliente, 
	ma_tercero.telefono as telefono_cliente, 
	ma_factura.fecha_factura, 
	ma_factura_deta.cod_producto, 
	ma_factura_deta.cod_concep_cobro, 
	ma_producto.descrip_producto, 
	car_pm_concep_cobro.descripcion, 
	ma_factura_notas_deta.cantidad, 
	ma_factura_notas_deta.v_base, 
	ma_factura_notas_deta.v_bruto, 
	ma_factura_notas_deta.v_descuento, 
	ma_factura_notas_deta.v_neto_pagar, 
	ma_factura_notas_deta.porcent_iva, 
	ma_factura_notas_deta.v_iva, 
	ma_factura_notas.vt_base, 
	ma_factura_notas.vt_bruto, 
	ma_factura_notas.vt_descuento, 
	ma_factura_notas.vt_neto_pagar, 
	ma_factura_notas.vt_pagado, 
	ma_factura_notas.vt_amortizar, 
	ma_factura_notas.vt_abono_previo, 
	ma_factura_notas.vt_iva, 
	pm_gral_lugar.email ,
	pm_gral_lugar.cod_depto,
	pm_gral_lugar.cod_ciudad ,
	depto_facturador.descrip_depto as depto_facturador, 
	ciudad_facturador.descrip_ciudad as ciudad_facturador,
	ma_tercero.cod_depto as id_depto_cliente, 
	ma_tercero.cod_ciudad as id_ciudad_cliente, 
	depto_cliente.descrip_depto as depto_cliente, 
	ciudad_cliente.descrip_ciudad as ciudad_cliente,
	pm_gral_lugar.codigo_postal,
	ma_tercero.codigo_postal as codigo_postal_cliente,
	pm_gral_lugar.responsabilidad_fiscal,
	pm_gral_lugar.gerente,
	ma_tercero.obligacion ,
	ma_tercero.respon_iva,
	ma_tercero.nombre_responsable,
	round (ma_factura_notas.vt_iva/ ma_factura_notas.vt_base ,2) *100 as percent_iva,
	ma_producto.id_unidad_medida as und_med_prod,
	car_pm_concep_cobro.id_unidad_medida as und_med_concep,
	(select descripcion_unidad from pm_unidad_cantidad where id_unidad=ma_producto.id_unidad_medida) as descrip_und_medida_prod ,
	(select descripcion_unidad from pm_unidad_cantidad where id_unidad=car_pm_concep_cobro.id_unidad_medida) as descrip_und_medida_concep,
	ma_factura_notas.observaciones,
	ma_tercero.email as email_cliente,
	ma_factura_notas.estado,
	'' as singnig_time,
	'' as IssuerName,
	'' as serialNumber,
	'' as cert_digest,
	ma_factura_notas.nro_nota as nro_nota,
	ma_factura_notas.tipo_nota,
	ma_factura_notas.estado_dian,
	ma_factura_notas.file_name_nota AS file_name_fact,
	ma_factura_notas.file_name_zip,
	ma_factura_notas.track_id,
	ma_factura_notas.motivo_nota,
	ma_factura_notas.fecha_nota

FROM ((((ma_tercero 
	LEFT JOIN pm_gral_ciudad AS ciudad_cliente ON (ma_tercero.cod_pais = ciudad_cliente.cod_pais) AND (ma_tercero.cod_depto = ciudad_cliente.cod_depto) AND (ma_tercero.cod_ciudad = ciudad_cliente.cod_ciudad)) 
	LEFT JOIN pm_gral_depto AS depto_cliente ON (ma_tercero.cod_pais = depto_cliente.cod_pais) AND (ma_tercero.cod_depto = depto_cliente.cod_depto)) 
	INNER JOIN ((((pm_gral_lugar 
	LEFT JOIN pm_gral_depto AS depto_facturador ON (pm_gral_lugar.cod_pais = depto_facturador.cod_pais) AND (pm_gral_lugar.cod_depto = depto_facturador.cod_depto)) 
	LEFT JOIN pm_gral_ciudad AS ciudad_facturador ON (pm_gral_lugar.cod_pais = ciudad_facturador.cod_pais) AND (pm_gral_lugar.cod_depto = ciudad_facturador.cod_depto) AND (pm_gral_lugar.cod_ciudad = ciudad_facturador.cod_ciudad)) 
	INNER JOIN (ma_factura 
	LEFT JOIN pm_gral_documentos_opciones ON ma_factura.nro_resolucion = pm_gral_documentos_opciones.nro_resolucion) ON pm_gral_lugar.clugar = ma_factura.clug_fact) 
	INNER JOIN ((ma_factura_deta 
	LEFT JOIN ma_producto ON ma_factura_deta.cod_producto = ma_producto.cod_producto) 
	LEFT JOIN car_pm_concep_cobro ON ma_factura_deta.cod_concep_cobro = car_pm_concep_cobro.cod_concep) ON (ma_factura.nro_factura = ma_factura_deta.nro_factura) AND (ma_factura.clug_fact = ma_factura_deta.clug_fact)) 
		ON (ma_tercero.documento = ma_factura.documento_ter) AND (ma_tercero.tipo_doc = ma_factura.tipo_doc_ter)) 
	INNER JOIN ma_factura_notas ON (ma_factura.nro_factura = ma_factura_notas.nro_factura) AND (ma_factura.clug_fact = ma_factura_notas.clug_fact)) 
	INNER JOIN ma_factura_notas_deta ON (ma_factura_notas.nro_nota = ma_factura_notas_deta.nro_nota) AND (ma_factura_notas.tipo_nota = ma_factura_notas_deta.tipo_nota) AND (ma_factura_deta.item = ma_factura_notas_deta.item) 
		AND (ma_factura_deta.nro_factura = ma_factura_notas_deta.nro_factura) AND (ma_factura_deta.clug_fact = ma_factura_notas_deta.clug_fact)
WHERE ma_factura_notas.tipo_nota=:asTipoNota AND ma_factura_notas.nro_nota=:anNroNota AND ma_factura_notas_deta.cantidad>0
ORDER BY ma_factura_deta.item
