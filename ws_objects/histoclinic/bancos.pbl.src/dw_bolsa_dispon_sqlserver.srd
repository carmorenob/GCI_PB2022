table(column=(type=long updatewhereclause=yes name=esco dbname="esco" initial="0" values="	1/	0" validationmsg="'Cantidad excede a lo solicitado'" )
 column=(type=char(2) updatewhereclause=yes key=yes name=clugar dbname="clugar" )
 column=(type=long updatewhereclause=yes key=yes name=nro_donacion dbname="nro_donacion" )
 column=(type=long updatewhereclause=yes key=yes name=nro_bolsa dbname="nro_bolsa" )
 column=(type=datetime updatewhereclause=yes name=fecha_aprobacion dbname="fecha_aprobacion" )
