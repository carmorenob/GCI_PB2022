 retrieve="SELECT 
	0 AS esco, Banco_bolsa.clugar, Banco_bolsa.nro_donacion, Banco_bolsa.nro_bolsa, 
	Banco_bolsa.fecha_aprobacion, Banco_bolsa.nro_sello, Banco_bolsa.fecha_vencimiento, 
	Banco_bolsa.estado, Banco_bolsa.codproced, Banco_donacion.anyo, Banco_donacion.consec_donacion, 
	banco_subempaque.descrip_empaque, banco_subempaque.capacidad,CARAC.GRUPO,CARAC.factor,
	 (select  n_color 
		from medica_triage
		where 
		(datediff(dd,getdate(), Banco_bolsa.fecha_vencimiento))
		between inicial and final) as n_color
FROM 
	((((Banco_donacion INNER JOIN Banco_bolsa ON (Banco_donacion.NRO_DONACION = Banco_bolsa.NRO_DONACION) 
	AND (Banco_donacion.CLUGAR = Banco_bolsa.CLUGAR)) 
	INNER JOIN banco_subempaque ON (Banco_bolsa.NRO = banco_subempaque.NRO) 
	AND (Banco_bolsa.COD_EMPAQUE = banco_subempaque.COD_EMPAQUE) 
	AND (Banco_bolsa.COD_BANCO = banco_subempaque.COD_BANCO)) 
	INNER JOIN banco_encuesta ON (Banco_bolsa.NRO_DONACION = banco_encuesta.NRO_DONACION) 
	AND (Banco_bolsa.CLUGAR = banco_encuesta.CLUGAR)) 
	INNER JOIN banco_paciente_caract ON (banco_encuesta.SUBITEM = banco_paciente_caract.SUBITEM) 
	AND (banco_encuesta.ITEM_ENC = banco_paciente_caract.ITEM) 
	AND (banco_encuesta.COD_BANCO = banco_paciente_caract.COD_BANCO) 
	AND (banco_encuesta.ITEM = banco_paciente_caract.ITEM)) 
	INNER JOIN 
	(
		SELECT 
			TIPODOC, DOCUMENTO, MAX(FACTOR) AS FACTOR,
			MAX(GRUPO) AS GRUPO
		FROM
		(
		SELECT 
			BANCO_PACIENTE_CARACT.TIPODOC, BANCO_PACIENTE_CARACT.DOCUMENTO, 
			UPPER(BANCO_ITEM_ENCUESTA.DESCRIP_ITEM) AS DESCRIP_ITEM, 
			case when BANCO_ITEM_LISTA.DESCRIP_LISTA IN ('POSITIVO','NEGATIVO') then BANCO_ITEM_LISTA.DESCRIP_LISTA end as FACTOR ,
			case when BANCO_ITEM_LISTA.DESCRIP_LISTA NOT IN ('POSITIVO','NEGATIVO') then BANCO_ITEM_LISTA.DESCRIP_LISTA end AS GRUPO
		FROM 
			(BANCO_PACIENTE_CARACT INNER JOIN BANCO_ITEM_ENCUESTA ON (BANCO_PACIENTE_CARACT.ITEM = BANCO_ITEM_ENCUESTA.ITEM) 
			AND (BANCO_PACIENTE_CARACT.COD_BANCO = BANCO_ITEM_ENCUESTA.COD_BANCO)) INNER JOIN BANCO_ITEM_LISTA 
			ON (BANCO_PACIENTE_CARACT.SUBITEM = BANCO_ITEM_LISTA.SUBITEM) AND (BANCO_PACIENTE_CARACT.ITEM = BANCO_ITEM_LISTA.ITEM) 
			AND (BANCO_PACIENTE_CARACT.COD_BANCO = BANCO_ITEM_LISTA.COD_BANCO)
		) as TODOS
		GROUP BY 
			TIPODOC, DOCUMENTO) AS CARAC ON (Banco_donacion.DOCUMENTO = CARAC.DOCUMENTO) AND (Banco_donacion.TIPODOC = CARAC.TIPODOC)
WHERE 
	(((banco_paciente_caract.tipodoc)=:tdoc) AND ((banco_paciente_caract.documento)=:docu) AND ((banco_paciente_caract.cod_banco)=:banco) AND ((banco_subempaque.cod_tsubempaq)=:tipo)) 
GROUP BY 
	Banco_bolsa.clugar, Banco_bolsa.nro_donacion, Banco_bolsa.nro_bolsa, Banco_bolsa.fecha_aprobacion, 
	Banco_bolsa.nro_sello, Banco_bolsa.fecha_vencimiento, Banco_bolsa.estado, Banco_bolsa.codproced, 
	Banco_donacion.anyo, Banco_donacion.consec_donacion, 
	banco_subempaque.descrip_empaque, banco_subempaque.capacidad,CARAC.GRUPO,CARAC.factor
HAVING 
	(((Banco_bolsa.estado) In ('D','P')) AND ((Banco_bolsa.codproced)=:cod) AND ((Count(banco_encuesta.item))=:cuantos))
	
UNION

SELECT 
	0 AS esco, Banco_bolsa.clugar, Banco_bolsa.nro_donacion, Banco_bolsa.nro_bolsa, 
	Banco_bolsa.fecha_aprobacion, Banco_bolsa.nro_sello, Banco_bolsa.fecha_vencimiento, 
	Banco_bolsa.estado, Banco_bolsa.codproced, Banco_donacion.anyo, Banco_donacion.consec_donacion, 
	banco_subempaque.descrip_empaque, banco_subempaque.capacidad,CARAC.GRUPO,CARAC.factor,
	 (select  n_color 
		from medica_triage
		where 
		(datediff(dd,getdate(), Banco_bolsa.fecha_vencimiento))
		between inicial and final) as n_color
FROM 
	((((Banco_donacion INNER JOIN Banco_bolsa ON (Banco_donacion.NRO_DONACION = Banco_bolsa.NRO_DONACION) 
	AND (Banco_donacion.CLUGAR = Banco_bolsa.CLUGAR)) 
	INNER JOIN banco_subempaque ON (Banco_bolsa.NRO = banco_subempaque.NRO) 
	AND (Banco_bolsa.COD_EMPAQUE = banco_subempaque.COD_EMPAQUE) 
	AND (Banco_bolsa.COD_BANCO = banco_subempaque.COD_BANCO)) 
	INNER JOIN banco_encuesta ON (Banco_bolsa.NRO_DONACION = banco_encuesta.NRO_DONACION) 
	AND (Banco_bolsa.CLUGAR = banco_encuesta.CLUGAR)) 
	INNER JOIN banco_paciente_caract ON (banco_encuesta.SUBITEM = banco_paciente_caract.SUBITEM) 
	AND (banco_encuesta.ITEM_ENC = banco_paciente_caract.ITEM) 
	AND (banco_encuesta.COD_BANCO = banco_paciente_caract.COD_BANCO) 
	AND (banco_encuesta.ITEM = banco_paciente_caract.ITEM)) 
	INNER JOIN 
	(
		SELECT 
			TIPODOC, DOCUMENTO, MAX(FACTOR) AS FACTOR,
			MAX(GRUPO) AS GRUPO
		FROM
		(
		SELECT 
			BANCO_PACIENTE_CARACT.TIPODOC, BANCO_PACIENTE_CARACT.DOCUMENTO, 
			UPPER(BANCO_ITEM_ENCUESTA.DESCRIP_ITEM) AS DESCRIP_ITEM, 
			case when BANCO_ITEM_LISTA.DESCRIP_LISTA IN ('POSITIVO','NEGATIVO') then BANCO_ITEM_LISTA.DESCRIP_LISTA end as FACTOR ,
			case when BANCO_ITEM_LISTA.DESCRIP_LISTA NOT IN ('POSITIVO','NEGATIVO') then BANCO_ITEM_LISTA.DESCRIP_LISTA end AS GRUPO
		FROM 
			(BANCO_PACIENTE_CARACT INNER JOIN BANCO_ITEM_ENCUESTA ON (BANCO_PACIENTE_CARACT.ITEM = BANCO_ITEM_ENCUESTA.ITEM) 
			AND (BANCO_PACIENTE_CARACT.COD_BANCO = BANCO_ITEM_ENCUESTA.COD_BANCO)) INNER JOIN BANCO_ITEM_LISTA 
			ON (BANCO_PACIENTE_CARACT.SUBITEM = BANCO_ITEM_LISTA.SUBITEM) AND (BANCO_PACIENTE_CARACT.ITEM = BANCO_ITEM_LISTA.ITEM) 
			AND (BANCO_PACIENTE_CARACT.COD_BANCO = BANCO_ITEM_LISTA.COD_BANCO)
		) as TODOS
		GROUP BY 
			TIPODOC, DOCUMENTO) AS CARAC ON (Banco_donacion.DOCUMENTO = CARAC.DOCUMENTO) AND (Banco_donacion.TIPODOC = CARAC.TIPODOC)
WHERE 
	(((banco_paciente_caract.tipodoc)='NI') AND ((banco_paciente_caract.documento)='800193987') AND ((banco_paciente_caract.cod_banco)=:banco) AND ((banco_subempaque.cod_tsubempaq)=:tipo)) 
GROUP BY 
	Banco_bolsa.clugar, Banco_bolsa.nro_donacion, Banco_bolsa.nro_bolsa, Banco_bolsa.fecha_aprobacion, 
	Banco_bolsa.nro_sello, Banco_bolsa.fecha_vencimiento, Banco_bolsa.estado, Banco_bolsa.codproced, 
	Banco_donacion.anyo, Banco_donacion.consec_donacion, 
	banco_subempaque.descrip_empaque, banco_subempaque.capacidad,CARAC.GRUPO,CARAC.factor
HAVING 
	(((Banco_bolsa.estado) In ('D','P')) AND ((Banco_bolsa.codproced)=:cod) AND ((Count(banco_encuesta.item))=:cuantos))
" update="banco_bolsa" updatewhere=0 updatekeyinplace=yes arguments=(("tdoc", string),("docu", string),("banco", string),("cod", string),("cuantos", number),("tipo", string))  sort="fecha_vencimiento A nro_sello A " )
