table(column=(type=char(15) updatewhereclause=yes key=yes name=codgenerico dbname="sumgenerico.codgenerico" dbalias=".codgenerico" )
 column=(type=char(200) updatewhereclause=yes name=medicamento dbname="sumgenerico.medicamento" dbalias=".medicamento" )
 column=(type=char(15) updatewhereclause=yes name=codalmacen dbname="sumgenerico.codalmacen" dbalias=".codalmacen" )
 column=(type=char(10) updatewhereclause=yes name=codarticulo dbname="sumgenerico.codarticulo" dbalias=".codarticulo" )
 column=(type=char(20) updatewhereclause=yes name=convenio dbname="sumgenerico.convenio" dbalias=".convenio" )
 column=(type=char(250) updatewhereclause=yes name=descripcion dbname="sumgenerico.descripcion" dbalias=".descripcion" )
 column=(type=char(200) updatewhereclause=yes name=fabricante dbname="sumgenerico.fabricante" dbalias=".fabricante" )
 column=(type=datetime updatewhereclause=yes name=fecha_venc dbname="sumgenerico.fecha_venc" dbalias=".fecha_venc" )
 column=(type=char(30) updatewhereclause=yes name=lote dbname="sumgenerico.lote" dbalias=".lote" )
 column=(type=decimal(2) updatewhereclause=yes name=cantidad dbname="sumgenerico.cantidad" dbalias=".cantidad" )
 column=(type=long updatewhereclause=yes name=llevar dbname="llevar" validation="long(gettext())>=0 and long(gettext())<=cantidad" validationmsg="'Cantidad no valida'" )
 column=(type=decimal(2) updatewhereclause=yes name=costoprom dbname="sumgenerico.costoprom" dbalias=".costoprom" )
 retrieve="SELECT SUMGENERICO.c_medica as codgenerico, SUMGENERICO.descripcion AS medicamento, SUM_LOTE.CODALMACEN, 
SUM_LOTE.CODARTICULO,SUM_LOTE.convenio, SUMARTICULO.DESCRIPCION, SUMARTICULO.FABRICANTE, SUM_LOTE.Fecha_Venc, 
SUM_LOTE.Lote, SUM_LOTE.Cantidad, 0 AS llevar, SUM_KARDEX.CostoProm
FROM SUMGENERICO INNER JOIN ((SUMARTICULO LEFT JOIN SUM_KARDEX ON SUMARTICULO.codArticulo = SUM_KARDEX.CODARTICULO) LEFT JOIN SUM_LOTE ON (SUM_KARDEX.CODARTICULO = SUM_LOTE.CODARTICULO) AND (SUM_KARDEX.CODALMACEN = SUM_LOTE.CODALMACEN)) ON SUMGENERICO.codgenerico = SUMARTICULO.codgenerico
WHERE 
	(SUM_LOTE.CODALMACEN=:alm) AND (SUM_LOTE.Cantidad>0 And SUM_LOTE.Cantidad>=:solic) AND (SUMGENERICO.codgenerico=:cgener) AND 
	(SUMARTICULO.vence='1') AND (SUM_KARDEX.saldoactual>0)
UNION ALL
SELECT SUMGENERICO.c_medica, SUMGENERICO.descripcion, SUM_KARDEX.CODALMACEN, SUM_KARDEX.CODARTICULO, null as convenio,
SUMARTICULO.DESCRIPCION, SUMARTICULO.FABRICANTE, Null AS vence, Null AS lote, SUM_KARDEX.saldoactual, 0 AS llevar, SUM_KARDEX.CostoProm
FROM SUMGENERICO INNER JOIN (SUMARTICULO INNER JOIN SUM_KARDEX ON SUMARTICULO.codArticulo = SUM_KARDEX.CODARTICULO) ON SUMGENERICO.codgenerico = SUMARTICULO.codgenerico
WHERE (((SUMGENERICO.codgenerico)=:cgener) AND ((SUM_KARDEX.CODALMACEN)=:alm) AND ((SUM_KARDEX.saldoactual)>0 and (SUM_KARDEX.saldoactual)>=:solic) 
AND ((SUMARTICULO.vence)='0' Or (SUMARTICULO.vence) Is Null))
" update="SUMGENERICO" updatewhere=0 updatekeyinplace=no arguments=(("cgener", string),("alm", string),("solic", number))  sort="fecha_venc A " )
