 retrieve="SELECT hclin_reg_valor.nregistro, hclin_reg_valor.item, hclin_registro.desplantilla, hclin_registro.tipo as hctipo, HISTORIAL.INDAPDX as codtingre, hclin_registro.ingreso, 
hclin_registro.fecha AS fecha_reg, hclin_reg_valor.orden, hclin_reg_valor.descampo, 
hclin_reg_valor.tipo , hclin_reg_valor.texto, hclin_reg_valor.sino, hclin_reg_valor.numero, hclin_reg_valor.long_texto, hclin_reg_valor.fecha, 
 profe.nombre1|| case when profe.nombre2  is null then ' ' else  '  '||profe.nombre2 end ||' '|| 
 profe.apellido1|| case when profe.apellido2 is null then ' ' else  '  '||profe.apellido2 end as desprof
, hclin_reg_valor.tipo_memo, Especialidades.DesEsp, Profe.registro
FROM ((hclin_registro INNER JOIN Profe ON hclin_registro.CodProf = Profe.CodProf) INNER JOIN Especialidades ON hclin_registro.CEsp = Especialidades.CodEsp) INNER JOIN hclin_reg_valor ON (hclin_registro.nregistro = hclin_reg_valor.nregistro) AND (hclin_registro.clugar = hclin_reg_valor.clugar) AND (hclin_registro.Contador = hclin_reg_valor.Contador)
WHERE (((hclin_registro.Contador)=1398) AND ((hclin_reg_valor.clugar)='01') AND ((hclin_registro.codplantilla)='U01'))
order by 5,case when hclin_registro.tipo ='H' then case when ingreso='I' then 0 else 0 end  else 8 end ,7,1,2 "  sort="codtingre A case(  hctipo  
when 'H' then if (  ingreso ='I' , 0 , 10) 
else 8) A fecha_reg A nregistro A orden A " )
