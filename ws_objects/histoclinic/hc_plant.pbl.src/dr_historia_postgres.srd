, if ( tipo [-1] ='E'  or tipo [-1] ='M', '',  '	' )  ,  '~~r~~n'  ) " )
compute(name=valor_texto visible="1" lineremove=no multiline=no width.autosize=yes width="0" alignment="0" format="[general]" expression="if( tipo ='T' or tipo ='L' or tipo ='A', texto ,' ')" )
