 retrieve="  SELECT  
	hclin_registro.fecha ,            profe.nombre1+ case when profe.nombre2  is null then ' ' else  '  '+profe.nombre2 end +' '+ 
 profe.apellido1+ case when profe.apellido2 is null then ' ' else  '  '+profe.apellido2 end as desprof,           hclin_registro.desplantilla ,           hclin_registro.nregistro ,           
	hclin_reg_valor.orden ,           hclin_reg_valor.descampo ,           hclin_reg_valor.tipo ,           
	hclin_reg_valor.tipo_memo ,           hclin_reg_valor.texto ,           hclin_reg_valor.sino ,           
	hclin_reg_valor.numero ,           hclin_reg_valor.long_texto ,           hclin_reg_valor.fecha ,           
	hclin_reg_valor.item ,           hclin_registro.fecha_reg     
FROM 
	(hclin_registro INNER JOIN hclin_reg_valor ON (hclin_registro.nregistro = hclin_reg_valor.nregistro) AND 
	(hclin_registro.clugar = hclin_reg_valor.clugar) AND (hclin_registro.contador = hclin_reg_valor.CONTADOR)) INNER JOIN 
	profe ON hclin_registro.codprof = profe.CodProf
WHERE ( hclin_registro.contador = hclin_reg_valor.contador ) and          ( hclin_registro.clugar = hclin_reg_valor.clugar ) and          
	( hclin_registro.nregistro = hclin_reg_valor.nregistro ) and          ( hclin_registro.codprof = profe.codprof ) and          
((hclin_registro.contador = :contador And          ( hclin_registro.clugar = :clug ) And          ( hclin_registro.tipo = :tipo) ) or          
	(hclin_registro.tipo = 'R' And          ( hclin_registro.tipo = 'H' ) And          ( hclin_registro.ingreso = 'I' ) And          ( hclin_reg_valor.tipo = 'M' ) And          
( hclin_reg_valor.tipo_memo = 'C' ) And          ( hclin_registro.contador = :contador ) And          ( hclin_registro.clugar = :clug)) )
" arguments=(("contador", number),("clug", string),("tipo", string))  sort="hclin_registro_nregistro D hclin_reg_valor_orden A " )
