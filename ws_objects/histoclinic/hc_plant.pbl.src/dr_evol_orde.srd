 column=(type=datetime updatewhereclause=yes name=fecha dbname="especialidades.fecha" )
 column=(type=long updatewhereclause=yes name=item dbname="especialidades.item" )
 column=(type=datetime updatewhereclause=yes name=fecha_reg dbname="especialidades.fecha_reg" )
 column=(type=char(15) updatewhereclause=yes name=registro dbname="especialidades.registro" )
 column=(type=char(100) updatewhereclause=yes name=desesp dbname="especialidades.desesp" )
 retrieve="  SELECT  
	hclin_registro.fecha as fechag,
	profe.nombre1+ case when profe.nombre2  is null then ' ' else  '  '+profe.nombre2 end+' '+profe.apellido1+case when profe.apellido2 is null then ' ' else  '  '+profe.apellido2 end as desprof,
	hclin_registro.desplantilla,hclin_registro.nregistro,
	hclin_reg_valor.orden,hclin_reg_valor.descampo,hclin_reg_valor.tipo,
	hclin_reg_valor.tipo_memo,hclin_reg_valor.texto,
	case when  hclin_reg_valor.sino ='1' then 'SI' ELSE case when  hclin_reg_valor.sino='0'Then  'NO'else '' END END as sinob,           
	hclin_reg_valor.numero,hclin_reg_valor.long_texto,hclin_reg_valor.fecha ,
	hclin_reg_valor.item,hclin_registro.fecha_reg, 
	profe.REGISTRO, ESPECIALIDADES.DESESP  
FROM 
	((hclin_registro INNER JOIN hclin_reg_valor ON (hclin_registro.nregistro = hclin_reg_valor.nregistro) AND 
	(hclin_registro.clugar = hclin_reg_valor.clugar) AND (hclin_registro.contador = hclin_reg_valor.CONTADOR)) INNER JOIN 
	profe ON hclin_registro.codprof = profe.CodProf) INNER JOIN ESPECIALIDADES ON hclin_registro.CESP = ESPECIALIDADES.CODESP
WHERE 
	(((hclin_registro.fecha) Between :fecha1 And :fecha2) 
	AND ((hclin_registro.contador)=:contador) AND ((hclin_registro.clugar)=:clug) AND ((hclin_registro.tipo)=:tipo)) OR 
	( ((hclin_reg_valor.tipo)='M') AND ((hclin_reg_valor.tipo_memo)='C') 
	AND ((hclin_registro.contador)=:contador) AND ((hclin_registro.clugar)=:clug) 
	AND ((hclin_registro.tipo)='R' And (hclin_registro.tipo)='H') AND ((hclin_registro.ingreso)='I'));
" arguments=(("contador", number),("clug", string),("tipo", string),("fecha1", datetime),("fecha2", datetime))  sort="fechag A nregistro A orden A " )
