 retrieve="SELECT DISTINCT
 	hclin_plant.codplantilla, hclin_plant.desplantilla
FROM 
	(hclin_plant INNER JOIN HCLIN_PLANT_EDAD ON hclin_plant.CODPLANTILLA = HCLIN_PLANT_EDAD.CODPLANTILLA) 
	LEFT JOIN hclin_plant_finalidad ON hclin_plant.codplantilla = hclin_plant_finalidad.codplantilla
WHERE 
	(((hclin_plant.tipo)=:tipo And (hclin_plant.tipo) In ('C','P','O','Q','R','F','S')) AND ((hclin_plant.estado)='1') AND ((hclin_plant.por_espe)='0') AND ((hclin_plant.dxs)='0') AND ((hclin_plant.sexo) In ('0',:sexo)) AND ((:edad) Between HCLIN_PLANT_EDAD.EDADINI And HCLIN_PLANT_EDAD.EDADFIN)) 
	OR 
	(((hclin_plant.tipo)=:tipo And (hclin_plant.tipo) In ('C','P','O','Q','R','F','S')) AND ((hclin_plant.estado)='1') AND ((hclin_plant.por_espe)='0') AND ((hclin_plant.dxs)='1') AND ((hclin_plant.sexo) In ('0',:sexo)) AND ((:edad) Between HCLIN_PLANT_EDAD.EDADINI And HCLIN_PLANT_EDAD.EDADFIN) AND ((hclin_plant_finalidad.finconsulta) like :fina Or (hclin_plant_finalidad.finconsulta) Is Null)) 
	OR 
	(((hclin_plant.tipo)=:tipo And (hclin_plant.tipo) In ('C','P','O','Q','R','F','S')) AND ((hclin_plant.estado)='1') AND ((hclin_plant.por_espe)='1') AND ((hclin_plant.dxs)='0') AND ((hclin_plant.sexo) In ('0',:sexo)) AND ((:edad) Between HCLIN_PLANT_EDAD.EDADINI And HCLIN_PLANT_EDAD.EDADFIN) AND ((hclin_plant.codesp)=:espec)) 
	OR 
	(((hclin_plant.tipo)=:tipo And (hclin_plant.tipo) In ('C','P','O','Q','R','F','S')) AND ((hclin_plant.estado)='1') AND ((hclin_plant.por_espe)='1') AND ((hclin_plant.dxs)='1') AND ((hclin_plant.sexo) In ('0',:sexo)) AND ((:edad) Between HCLIN_PLANT_EDAD.EDADINI And HCLIN_PLANT_EDAD.EDADFIN) AND ((hclin_plant_finalidad.finconsulta) like :fina Or (hclin_plant_finalidad.finconsulta) Is Null) AND ((hclin_plant.codesp)=:espec)) 
	OR 
	(((hclin_plant.tipo)=:tipo And (hclin_plant.tipo) In ('H','E')) AND ((hclin_plant.estado)='1') AND ((hclin_plant.por_espe)='0') AND ((hclin_plant.dxs)='0') AND ((hclin_plant.sexo) In ('0',:sexo)) AND ((:edad) Between HCLIN_PLANT_EDAD.EDADINI And HCLIN_PLANT_EDAD.EDADFIN) AND ((hclin_plant.codtingre)='T' Or (hclin_plant.codtingre)=:tingre) AND ((hclin_plant.ingreso)=:ingr Or (hclin_plant.ingreso) Is Null)) 
	OR 
	(((hclin_plant.tipo)=:tipo And (hclin_plant.tipo) In ('H','E')) AND ((hclin_plant.estado)='1') AND ((hclin_plant.por_espe)='0') AND ((hclin_plant.dxs)='1') AND ((hclin_plant.sexo) In ('0',:sexo)) AND ((:edad) Between HCLIN_PLANT_EDAD.EDADINI And HCLIN_PLANT_EDAD.EDADFIN) AND ((hclin_plant_finalidad.finconsulta) like:fina Or (hclin_plant_finalidad.finconsulta) Is Null) AND ((hclin_plant.codtingre)='T' Or (hclin_plant.codtingre)=:tingre) AND ((hclin_plant.ingreso)=:ingr Or (hclin_plant.ingreso) Is Null)) 
	OR 
	(((hclin_plant.tipo)=:tipo And (hclin_plant.tipo) In ('H','E')) AND ((hclin_plant.estado)='1') AND ((hclin_plant.por_espe)='1') AND ((hclin_plant.dxs)='0') AND ((hclin_plant.sexo) In ('0',:sexo)) AND ((:edad) Between HCLIN_PLANT_EDAD.EDADINI And HCLIN_PLANT_EDAD.EDADFIN) AND ((hclin_plant.codesp)=:espec) AND ((hclin_plant.codtingre)='T' Or (hclin_plant.codtingre)=:tingre) AND ((hclin_plant.ingreso)=:ingr Or (hclin_plant.ingreso) Is Null)) 
	OR 
	(((hclin_plant.tipo)=:tipo And (hclin_plant.tipo) In ('H','E')) AND ((hclin_plant.estado)='1') AND ((hclin_plant.por_espe)='1') AND ((hclin_plant.dxs)='1') AND ((hclin_plant.sexo) In ('0',:sexo)) AND ((:edad) Between HCLIN_PLANT_EDAD.EDADINI And HCLIN_PLANT_EDAD.EDADFIN) AND ((hclin_plant_finalidad.finconsulta) like :fina Or (hclin_plant_finalidad.finconsulta) Is Null) AND ((hclin_plant.codesp)=:espec) AND ((hclin_plant.codtingre)='T' Or (hclin_plant.codtingre)=:tingre) AND ((hclin_plant.ingreso)=:ingr Or (hclin_plant.ingreso) Is Null));
