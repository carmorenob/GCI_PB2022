 retrieve="SELECT 
	lugar.descripcion AS lugar_atencion, 
	lugar.direccion as dir_centro,
	hclin_registro.contador, 
	hclin_registro.clugar, 
	0 as nregistro, 
	emp_hosp.desemp, 
	'RESUMEN DE EVOLUCIÓN' desplantilla, 
	historial.fecha as fecha_reg, 
	historial.fecha AS fecha_atención, 
	profe.nombre1 AS nom1_profe, 
	profe.nombre2 AS nom2_profe, 
	profe.apellido1 AS ape1_profe, 
	profe.apellido2 AS ape2_profe, 
	especialidades.desesp, 
	profe.registro, 
	'RESUMEN' descampo, 
	'M' tipo, 
	'' tipo_memo, 
	'' texto, 
	'' sino, 
	0 numero, 
	evolucionhc.resumeningreso long_texto, 
	NULL AS fecha,
	pacientes.tipodoc, 
	pacientes.documento, 
	pacientes.nombre1, 
	pacientes.nombre2, 
	pacientes.apellido1, 
	pacientes.apellido2, 
	pacientes.sexo, 
	pacientes.fnacimiento, 
	pacientes.tel, 
	pacientes.direccion,
	pacientes.historia,
	(select des_ocup from ocupaciones where todo=pacientes.codocup) as ocupacion,
	(select nombre from departamentos where coddepar=pacientes.cdepto) as depto,
	(select desciudad from ciudades where cdepto=pacientes.cdepto and codciudad=pacientes.cciudad) as munic ,
	emp_hosp.desemp as emp_responsable
FROM 
	(((((
	historial 
	INNER JOIN pacientes 
		ON (historial.documento = pacientes.documento) AND (historial.tipodoc = pacientes.tipodoc)) 
	LEFT JOIN (

		SELECT contador, clugar_his, Min(cemp) AS cemp
		FROM hospadmi
		GROUP BY contador, clugar_his
		HAVING contador=:an_conta AND clugar_his=:as_clug

	) as vw_respon_ingreso_hosp 
		ON (historial.clugar = vw_respon_ingreso_hosp.clugar_his) AND (historial.contador = vw_respon_ingreso_hosp.contador)) 
	LEFT JOIN empresa AS emp_hosp 
		ON vw_respon_ingreso_hosp.cemp = emp_hosp.codemp) 
	INNER JOIN evolucionhc 
		ON (historial.clugar = evolucionhc.clugar) AND (historial.contador = evolucionhc.contador)) 
	INNER JOIN lugar 
		ON evolucionhc.clugar = lugar.codlugar) 
	LEFT JOIN ( 
		(

			SELECT hclin_registro.contador, hclin_registro.clugar, Min(hclin_registro.nregistro) AS nregistro
			FROM hclin_registro
			GROUP BY hclin_registro.contador, hclin_registro.clugar
			HAVING hclin_registro.contador=:an_conta AND hclin_registro.clugar=:as_clug

		) as vw_min_hclin_reg 
	LEFT JOIN ((hclin_registro 
	LEFT JOIN especialidades 
		ON hclin_registro.cesp = especialidades.codesp) 
	LEFT JOIN profe 
		ON hclin_registro.codprof = profe.codprof) 
		ON (vw_min_hclin_reg.contador = hclin_registro.contador) AND (vw_min_hclin_reg.clugar = hclin_registro.clugar) AND (vw_min_hclin_reg.nregistro = hclin_registro.nregistro)) 
		ON (evolucionhc.clugar = vw_min_hclin_reg.clugar) AND (evolucionhc.contador = vw_min_hclin_reg.contador)
WHERE historial.contador=:an_conta AND historial.clugar=:as_clug" arguments=(("as_clug", string),("an_conta", number),("an_reg", number),("as_tipo_impr", string)) )
