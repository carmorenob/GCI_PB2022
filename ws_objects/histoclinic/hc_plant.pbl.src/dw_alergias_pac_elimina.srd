 retrieve="SELECT 
	TIPO_ALERGIA.COD_TIPO AS TA_COD_TIPO, TIPO_ALERGIA.DESP_TIPO AS TA_DESP_TIPO, 
	PM_ALERGIAS.COD_ALERGIA AS PA_COD_ALERGIA, PM_ALERGIAS.DESP_ALERGIA AS PA_DESP_ALERGIA, 
	CASE  WHEN pacientes_alergias_elimina.TEXTO='1'  THEN 1 ELSE 0 END AS SELE,
	pacientes_alergias_elimina.TIPODOC, pacientes_alergias_elimina.DOCUMENTO, pacientes_alergias_elimina.COD_TIPOA, 
	pacientes_alergias_elimina.COD_TIPO, pacientes_alergias_elimina.COD_ALERGIA, pacientes_alergias_elimina.CPROF, 
	pacientes_alergias_elimina.TIPO, pacientes_alergias_elimina.FECHA, pacientes_alergias_elimina.TEXTO, pacientes_alergias_elimina.SINO, 
	pacientes_alergias_elimina.NUMERO, pacientes_alergias_elimina.FECHA_CAP,TIPO_ALERGIA.COD_TIPOA AS TA_COD_TIPOA,
	PM_ALERGIAS.TIPO AS TA_TIPO,TIPO_ALERGIA.ORDEN AS TA_ORDEN,PM_ALERGIAS.ORDEN AS PM_ORDEN,
	pacientes_alergias_elimina.parentesco, pacientes_alergias_elimina.cod_atc,
	PM_ALERGIAS.num_min,PM_ALERGIAS.num_max,
	pacientes_alergias_elimina.fecha_anula, pacientes_alergias_elimina.motiv_anula, pm_motivo_anula.des_motivo, usuarios.nombre,
	pacientes_alergias_elimina.item
FROM 
	((((TIPO_ALERGIA 
	INNER JOIN PM_ALERGIAS ON (TIPO_ALERGIA.COD_TIPO = PM_ALERGIAS.COD_TIPO) 
	AND (TIPO_ALERGIA.COD_TIPOA = PM_ALERGIAS.COD_TIPOA)) 
	INNER JOIN pacientes_alergias_elimina ON (PM_ALERGIAS.cod_alergia = pacientes_alergias_elimina.cod_alergia) AND (PM_ALERGIAS.cod_tipo = pacientes_alergias_elimina.cod_tipo) 
	AND (PM_ALERGIAS.cod_tipoa = pacientes_alergias_elimina.cod_tipoa)) 
	LEFT JOIN parentesco ON pacientes_alergias_elimina.parentesco = parentesco.codigo) 
	INNER JOIN usuarios ON pacientes_alergias_elimina.usu_anula = usuarios.usuario) 
	INNER JOIN pm_motivo_anula ON pacientes_alergias_elimina.cod_anula = pm_motivo_anula.cod_anula
WHERE 
	(((TIPO_ALERGIA.COD_TIPOA)=:TIPOA) AND ((TIPO_ALERGIA.ESTADO)='1') AND ((PM_ALERGIAS.ESTADO)='1') 
	AND ((TIPO_ALERGIA.MEDICA) In ('0','1')) 
	AND ((pacientes_alergias_elimina.tipodoc)=:TD) AND ((pacientes_alergias_elimina.documento)=:DOC));" arguments=(("TIPOA", string),("TD", string),("DOC", string))  sort="ta_cod_tipo A ta_orden A pm_orden A ta_desp_tipo A pa_desp_alergia A " )
