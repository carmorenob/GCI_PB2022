 retrieve="SELECT 
	lugar.descripcion AS lugar_atencion, 
	lugar.direccion as dir_centro,
	hclin_registro.contador, 
	hclin_registro.clugar, 
	hclin_registro.nregistro, 
	empresa.desemp, 
	hclin_registro.desplantilla, 
	historial.fecha AS fecha_atención, 
	profe.nombre1 AS nom1_profe, 
	profe.nombre2 AS nom2_profe, 
	profe.apellido1 AS ape1_profe, 
	profe.apellido2 AS ape2_profe, 
	especialidades.desesp, 
	profe.registro, 
	hclin_reg_valor.descampo, 
	hclin_reg_valor.tipo, 
	hclin_reg_valor.tipo_memo, 
	hclin_reg_valor.texto, 
	hclin_reg_valor.sino, 
	hclin_reg_valor.numero, 
	hclin_reg_valor.long_texto, 
	hclin_reg_valor.fecha,
	pacientes.tipodoc, 
	pacientes.documento, 
	pacientes.nombre1, 
	pacientes.nombre2, 
	pacientes.apellido1, 
	pacientes.apellido2, 
	pacientes.sexo, 
	pacientes.fnacimiento, 
	pacientes.tel, 
	pacientes.direccion,
	pacientes.historia,
	(select des_ocup from ocupaciones where todo=pacientes.codocup) as ocupacion,
	(select nombre from departamentos where coddepar=pacientes.cdepto) as depto,
	(select desciudad from ciudades where cdepto=pacientes.cdepto and codciudad=pacientes.cciudad) as munic ,
	CASE WHEN vw_respon_x_ingreso.cemp is not null then empresa.desemp else
	CASE WHEN vw_respon_ingreso_hosp.cemp is not null then emp_hosp.desemp else
	CASE WHEN vw_respon_ingre_odont.cemp is not null then emp_odont.desemp end end end as emp_responsable
FROM (((((((((
	historial INNER JOIN ((hclin_registro 
	INNER JOIN hclin_reg_valor 
		ON (hclin_registro.nregistro = hclin_reg_valor.nregistro) AND (hclin_registro.clugar = hclin_reg_valor.clugar) AND (hclin_registro.contador = hclin_reg_valor.contador)) 
	INNER JOIN lugar 
		ON hclin_registro.clugar = lugar.codlugar) ON (historial.clugar = hclin_registro.clugar) AND (historial.contador = hclin_registro.contador)) 
	INNER JOIN pacientes 
		ON (historial.documento = pacientes.documento) AND (historial.tipodoc = pacientes.tipodoc)) 
	LEFT JOIN profe 
		ON hclin_registro.codprof = profe.codprof) 
	LEFT JOIN especialidades 
		ON hclin_registro.cesp = especialidades.codesp) 
	LEFT JOIN (

		SELECT contador, clugar, Min(cemp) AS cemp
		FROM serviciosingreso
		GROUP BY contador, clugar
		HAVING contador=:an_conta AND clugar=:as_clug

	) as vw_respon_x_ingreso 
		ON (historial.clugar = vw_respon_x_ingreso.clugar) AND (historial.contador = vw_respon_x_ingreso.contador)) 
	LEFT JOIN empresa 
		ON vw_respon_x_ingreso.cemp = empresa.codemp) 
	LEFT JOIN (

		SELECT contador, clugar_his, Min(cemp) AS cemp
		FROM hospadmi
		GROUP BY contador, clugar_his
		HAVING contador=:an_conta AND clugar_his=:as_clug

	) as vw_respon_ingreso_hosp 
		ON (historial.clugar = vw_respon_ingreso_hosp.clugar_his) AND (historial.contador = vw_respon_ingreso_hosp.contador)) 
	LEFT JOIN empresa AS emp_hosp 
		ON vw_respon_ingreso_hosp.cemp = emp_hosp.codemp) 
	LEFT JOIN (

		SELECT contador, clugar_his, Min(cemp) AS cemp
		FROM odontratacab
		GROUP BY contador, clugar_his
		HAVING contador=:an_conta AND clugar_his=:as_clug

	) as vw_respon_ingre_odont 
		ON (historial.clugar = vw_respon_ingre_odont.clugar_his) AND (historial.contador = vw_respon_ingre_odont.contador)) 
	LEFT JOIN empresa AS emp_odont 
		ON vw_respon_ingre_odont.cemp = emp_odont.codemp
WHERE hclin_registro.contador=:an_conta AND hclin_registro.clugar=:as_clug AND hclin_registro.nregistro=:an_reg
ORDER BY hclin_reg_valor.orden
