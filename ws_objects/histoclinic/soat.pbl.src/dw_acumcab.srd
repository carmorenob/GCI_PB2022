 retrieve="SELECT  
	acumcab.consecutivo,acumcab.clugar,acumcab.npoliza,acumcab.cemp,
	acumcab.ccontrato,acumcab.fecha,acumcab.valor,acumcab.vpendiente,
	acumcab.estado,acumcab.tipodoc,acumcab.documento
FROM 
	acumcab      
WHERE 
	( acumcab.tipodoc = :tdoc ) 
	and ( acumcab.documento = :docu ) 
	and ( acumcab.cemp = :emp ) 
	and ( acumcab.ccontrato = :cont )   " update="acumcab" updatewhere=0 updatekeyinplace=yes arguments=(("tdoc", string),("docu", string),("emp", string),("cont", string))  sort="fecha D " )
