 retrieve="SELECT 
	PACIENTES.TIPODOC, PACIENTES.DOCUMENTO, PACIENTES.NOMBRE1, PACIENTES.NOMBRE2, PACIENTES.APELLIDO1, 
	PACIENTES.APELLIDO2, EMPRESA.DESEMP, HOSPADMI.FECHAINGRESO, HOSPADMI.HORAINGRESO,HOSPADMI.FECHA_TRIAGE,
    datediff('mi',HOSPADMI.FECHA_TRIAGE,ahora()) as espera,
	HOSP_TRIAGE.COLOR, HOSP_TRIAGE.N_COLOR,HOSP_TRIAGE.orden,HOSPADMI.NH,HOSPADMI.CLUGAR
FROM 
    (((HOSPADMI INNER JOIN PACIENTES ON (HOSPADMI.DOCUMENTO = PACIENTES.DOCUMENTO) AND (HOSPADMI.TIPODOC = PACIENTES.TIPODOC)) 
    INNER JOIN EMPRESA ON HOSPADMI.CEMP = EMPRESA.CODEMP) INNER JOIN HOSP_TRIAGE ON HOSPADMI.CLASIF = HOSP_TRIAGE.COD_TRIAGE) 
    LEFT JOIN HOSPRESPON ON (HOSPADMI.NH = HOSPRESPON.NH) AND (HOSPADMI.CLUGAR = HOSPRESPON.CLUGAR) AND (HOSPADMI.CODTINGRE = HOSPRESPON.CODTINGRE)
WHERE 
    ((((HOSPADMI.FECHAINGRESO) Between :FechaI And :FechaF) 
	 AND ((HOSPADMI.CLUGAR) like :Lugar) 
	AND ((HOSPRESPON.NITEM) Is Null) 
	AND ((HOSPADMI.CODTINGRE)='2'))
	AND ((HOSP_TRIAGE.CIERRE)='0')
	AND ((HOSPADMI.estado) Not In ('7','5')));

" arguments=(("FechaI", date),("FechaF", date),("Lugar", string))  sort="orden A fechaingreso A horaingreso A " )
