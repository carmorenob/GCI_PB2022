 column=(type=char(30) updatewhereclause=yes name=camas_descama dbname="CAMAS.descama" )
 column=(type=datetime update=yes updatewhereclause=yes name=fecha_sale dbname="HOSPTRASL.fecha_sale" )
 column=(type=char(50) updatewhereclause=yes name=lugar_descripcion dbname="LUGAR.descripcion" )
 retrieve=" SELECT  
	hosptrasl.nh ,hosptrasl.clugar ,hosptrasl.codtingre ,hosptrasl.ntraslado ,hosptrasl.codser ,hosptrasl.clugar_cam ,
	hosptrasl.codhab ,hosptrasl.codcama ,hosptrasl.fecha ,hosptrasl.usuario ,pabellon.desser ,habitacion.deshab ,
	camas.descama ,hosptrasl.fecha_sale ,lugar.descripcion    
FROM 
	(((hosptrasl 
	INNER JOIN camas ON (hosptrasl.CLUGAR_CAM = camas.CLUGAR) AND (hosptrasl.CODCAMA = camas.CODIGOCAMA) 
	AND (hosptrasl.CODHAB = camas.CODHAB) AND (hosptrasl.CODSER = camas.CODSER)) 
	INNER JOIN habitacion ON (camas.CLUGAR = habitacion.CLUGAR) AND (camas.CODHAB = habitacion.CODIGOHAB) 
	AND (camas.CODSER = habitacion.CODIGOSER)) 
	INNER JOIN pabellon ON (habitacion.CLUGAR = pabellon.CLUGAR) AND (habitacion.CODIGOSER = pabellon.CODIGOSER)) 
	INNER JOIN lugar ON pabellon.CLUGAR = lugar.CODLUGAR
WHERE 
	(((hosptrasl.nh)=:Nhc) AND ((hosptrasl.clugar)=:clugar));
" update="hosptrasl" updatewhere=0 updatekeyinplace=yes arguments=(("Nhc", number),("clugar", string))  sort="fecha D " )
