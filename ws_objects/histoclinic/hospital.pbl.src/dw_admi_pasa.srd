 retrieve="SELECT  
	hospadmi.nh ,hospadmi.clugar ,hospadmi.fechaingreso ,hospadmi.horaingreso ,hospadmi.estado ,
	hospadmi.contador ,hospadmi.clugar_his ,hospadmi.cemp ,hospadmi.ccontrato,hospadmi.codtingre     
FROM 
	hospadmi      
WHERE 
	( hospadmi.codtingre = :tingres ) 
	and ( hospadmi.tipodoc = :tdoc ) 
	And ( hospadmi.documento = :DOCU )   
	AND (hospadmi.clugar =:lugar)" update="hospadmi" updatewhere=0 updatekeyinplace=no arguments=(("tdoc", string),("DOCU", string),("tingres", string),("lugar", string))  sort="fechaingreso D horaingreso D " )
