table(column=(type=char(120) updatewhereclause=yes name=desemp dbname="desemp" )
 column=(type=char(35) updatewhereclause=yes name=descontrato dbname="descontrato" )
 column=(type=long updatewhereclause=yes name=trasl dbname="trasl" )
 column=(type=char(30) updatewhereclause=yes name=desser dbname="desser" )
 column=(type=char(50) updatewhereclause=yes name=deshab dbname="deshab" )
 column=(type=char(30) updatewhereclause=yes name=descama dbname="descama" )
 column=(type=char(5) updatewhereclause=yes name=codigocama dbname="codigocama" )
 column=(type=char(50) updatewhereclause=yes name=color dbname="color" )
 column=(type=long updatewhereclause=yes name=n_color dbname="n_color" )
 retrieve="SELECT 
 	Empresa.DesEmp, Contratos.DesContrato, Max(HospTrasl.NTraslado) AS trasl, 
	Pabellon.DesSer, Habitacion.DesHab, Camas.DesCama, 
	Camas.CodigoCama, hosp_triage.color, hosp_triage.n_color
FROM 
	((((((HospAdmi 
	INNER JOIN Contratos ON (HospAdmi.cemp = Contratos.codemp) AND (HospAdmi.ccontrato = Contratos.codcontrato)) 
	INNER JOIN Empresa ON Contratos.codemp = Empresa.codemp) 
	LEFT JOIN hosp_triage ON HospAdmi.clasif = hosp_triage.cod_triage) 
	LEFT JOIN HospTrasl ON (HospAdmi.nh = HospTrasl.nh) AND (HospAdmi.clugar = HospTrasl.clugar) AND (HospAdmi.codtingre = HospTrasl.codtingre)) 
	LEFT JOIN Camas ON (HospTrasl.codser = Camas.codser) AND (HospTrasl.codhab = Camas.codhab) AND (HospTrasl.codcama = Camas.codigocama) 
	AND (HospTrasl.clugar_cam = Camas.clugar)) 
	LEFT JOIN Habitacion ON (Camas.codser = Habitacion.codigoser) AND (Camas.codhab = Habitacion.codigohab) AND (Camas.clugar = Habitacion.clugar)) 
	LEFT JOIN Pabellon ON (Habitacion.codigoser = Pabellon.codigoser) AND (Habitacion.clugar = Pabellon.clugar)
WHERE 
	(((HospAdmi.NH)=:nh) AND ((HospAdmi.clugar)=:clug_hadm) AND ((HospAdmi.codtingre)=:tingre))
GROUP BY 
	Empresa.DesEmp, Contratos.DesContrato, Pabellon.DesSer, Habitacion.DesHab, Camas.DesCama, 
	Camas.CodigoCama, hosp_triage.color, hosp_triage.n_color
" arguments=(("nh", number),("clug_hadm", string),("tingre", string))  sort="trasl D " )
