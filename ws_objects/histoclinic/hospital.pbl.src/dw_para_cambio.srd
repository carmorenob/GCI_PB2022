 column=(type=long updatewhereclause=yes name=este dbname="hospadmi.este" )
 column=(type=char(2) updatewhereclause=yes name=clugar_his dbname="hospadmi.clugar_his" )
 column=(type=char(50) updatewhereclause=yes name=regimen dbname="tipoingreso.regimen" )
 retrieve="SELECT
	hospadmi.NH, hospadmi.clugar, hospadmi.CodTIngre, hospadmi.FechaIngreso, 
	hospadmi.CEmp, hospadmi.CContrato, tipoingreso.Descripcion, evolucionhc.Contador, 
	Empresa.DesEmp, Contratos.DesContrato, 
	hospadmi.Contador AS este,hospadmi.clugar_his, tiporegimen.descripcion as regimen
FROM 
	(((((hospadmi INNER JOIN pacientes ON (hospadmi.TipoDoc = pacientes.TipoDoc) AND (hospadmi.Documento = pacientes.Documento)) 
	LEFT JOIN evolucionhc ON (hospadmi.Contador = evolucionhc.Contador) AND (hospadmi.clugar_his = evolucionhc.clugar)) 
	INNER JOIN Empresa ON hospadmi.CEmp = Empresa.CodEmp) 
	INNER JOIN Contratos ON (hospadmi.CContrato = Contratos.CodContrato) AND (hospadmi.CEmp = Contratos.CodEmp)) 
	INNER JOIN tiporegimen ON Contratos.ctreg = tiporegimen.tipo) 
	INNER JOIN tipoingreso ON hospadmi.codtingre = tipoingreso.codtingre
WHERE 
	(((hospadmi.CodTIngre)=:tingres) AND (hospadmi.Estado='1' or hospadmi.Estado='2') 
	AND ((hospadmi.TipoDoc)=:tdocu) AND ((hospadmi.Documento)=:docu))
ORDER BY 
	hospadmi.FechaIngreso DESC
" update="hospadmi" updatewhere=0 updatekeyinplace=yes arguments=(("tingres", string),("tdocu", string),("docu", string)) )
