 retrieve="SELECT 
	pabellon.codigoser, pabellon.desser, habitacion.codigohab, habitacion.deshab, camas.codigocama, 
	camas.descama, camas.clugar
FROM 
	(pabellon INNER JOIN habitacion ON (pabellon.CODIGOSER = habitacion.CODIGOSER) AND 
	(pabellon.CLUGAR = habitacion.CLUGAR)) INNER JOIN camas ON (habitacion.CODIGOSER = camas.CODSER) AND 
	(habitacion.CODIGOHAB = camas.CODHAB) AND (habitacion.CLUGAR = camas.CLUGAR)
WHERE 
	(((:tingres)='2') AND ((camas.xa_urge)='1') AND ((camas.documento) Is Null) AND ((pabellon.clugar)=:lugar) AND ((pabellon.ESTADO)='1') AND ((habitacion.ESTADO)='1') AND ((camas.ESTADO)='1')) OR 
	(((:tingres)='3') AND ((camas.documento) Is Null) AND ((pabellon.clugar)=:lugar) AND ((camas.xa_hosp)='1') AND ((pabellon.ESTADO)='1') AND ((habitacion.ESTADO)='1') AND ((camas.ESTADO)='1')) OR 
	(((:tingres)='4') AND ((camas.documento) Is Null) AND ((pabellon.clugar)=:lugar) AND ((camas.xa_ambu)='1') AND ((pabellon.ESTADO)='1') AND ((habitacion.ESTADO)='1') AND ((camas.ESTADO)='1')) OR 
	(((:tingres)='7') AND ((camas.documento) Is Null) AND ((pabellon.clugar)=:lugar) AND ((camas.XA_UCI)='1') AND ((pabellon.ESTADO)='1') AND ((habitacion.ESTADO)='1') AND ((camas.ESTADO)='1'));" arguments=(("tingres", string),("lugar", string))  sort="desser A deshab A descama A " )
