 retrieve="  SELECT  
	agendaprof.codgc ,           agendaprof.consult ,           agendaprof.cesp ,           
	agendaprof.prof ,           agendaprof.fecha ,           agendaprof.hora ,           
	agendaprof.duracion ,           agendaprof.estcita ,           grupocita.descripcion ,           
	nombre1,nombre2,apellido1,apellido2,
	agendaprof.usuario ,agendaprof.fecha_crea     
FROM agendaprof ,           grupocita ,           profe     
WHERE ( grupocita.codgc = agendaprof.codgc ) and          ( agendaprof.prof = profe.codprof ) and          
	( ( agendaprof.consult = :consul ) and          ( agendaprof.fecha between :fecha1 and :fecha2 ) )  " update="agendaprof" updatewhere=0 updatekeyinplace=no arguments=(("consul", string),("fecha1", datetime),("fecha2", datetime))  sort="hora A " )
