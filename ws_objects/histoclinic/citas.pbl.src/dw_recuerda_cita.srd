table(column=(type=char(3) updatewhereclause=yes name=tdoc dbname="grupocita.tdoc" )
 column=(type=char(15) updatewhereclause=yes name=docu dbname="grupocita.docu" )
 column=(type=char(93) updatewhereclause=yes name=nombre_completo dbname="nombre_completo" )
 column=(type=char(10) updatewhereclause=yes name=fecha_cita dbname="fecha_cita" )
 column=(type=char(8) updatewhereclause=yes name=hora_cita dbname="hora_cita" )
 column=(type=char(50) updatewhereclause=yes name=consultorio dbname="grupocita.consultorio" )
 column=(type=char(83) updatewhereclause=yes name=profesional dbname="profesional" )
 column=(type=char(50) updatewhereclause=yes name=ubica dbname="grupocita.ubica" )
 column=(type=char(50) updatewhereclause=yes name=grupo dbname="grupocita.grupo" )
 column=(type=number updatewhereclause=yes name=cita dbname="cita" )
 column=(type=long updatewhereclause=yes name=mes dbname="mes" )
 column=(type=long updatewhereclause=yes name=dia dbname="dia" )
 retrieve="SELECT 
   pacientes.tipodoc as Tdoc, pacientes.documento as Docu, 
   pacientes.nombre1+ case when pacientes.nombre2 is null then ' ' else ' ' +pacientes.nombre2+ ' ' end + pacientes.apellido1 + case when pacientes.apellido2  is null then ' ' else ' ' + pacientes.apellido2 end as Nombre_completo, 
  convert( varchar(10),serciosturno.fecha ,101) as Fecha_Cita, 
  convert(varchar(8),serciosturno.hora,108) as Hora_Cita, consultorio.descripcion AS Consultorio, 
   profe.nombre1 + case when profe.nombre2 is null then ' ' else ' ' + profe.nombre2+ ' ' end +  profe.apellido1 + case when profe.apellido2 is null then ' ' else ' ' + profe.apellido2 end as Profesional,
   lugar.direccion as Ubica, grupocita.descripcion AS Grupo, convert(varchar(10),citasasig.ncita)+citasasig.clugar as Cita,
   month(serciosturno.fecha) as Mes,
   DATEPART(dd, serciosturno.fecha) as Dia
FROM 
		((((((citasasig INNER JOIN lugar ON citasasig.clugar = lugar.codlugar) 
		 INNER JOIN pacientes ON (citasasig.documento = pacientes.documento) AND (citasasig.tipodoc = pacientes.tipodoc)) 
		 INNER JOIN sercioscita ON (citasasig.clugar = sercioscita.clugar) AND (citasasig.ncita = sercioscita.ncita)) 
		 INNER JOIN serciosturno ON (sercioscita.clugar = serciosturno.clugar) AND (sercioscita.nservicio = serciosturno.nservicio) 
		 AND    (sercioscita.ncita = serciosturno.ncita)) 
		 INNER JOIN profe ON serciosturno.prof = profe.codprof) 
		 INNER JOIN consultorio ON serciosturno.consult = consultorio.codconsultorio) 
		 INNER JOIN grupocita ON citasasig.codgc = grupocita.codgc
WHERE 
	(((citasasig.ncita)=:cita) AND ((citasasig.clugar)=:lugar))
" arguments=(("cita", number),("lugar", string)) )
