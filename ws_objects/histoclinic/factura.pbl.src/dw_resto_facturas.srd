 column=(type=char(2) updatewhereclause=yes name=clugar_rad dbname="factcab.clugar_rad" )
 column=(type=char(1) updatewhereclause=yes name=tipo_rad dbname="factcab.tipo_rad" )
 column=(type=number updatewhereclause=yes name=nfact dbname="factcab.nfact" )
 column=(type=char(2) updatewhereclause=yes name=clugar dbname="factcab.clugar" )
 column=(type=char(1) updatewhereclause=yes name=tipo dbname="factcab.tipo" )
 column=(type=datetime updatewhereclause=yes name=fecha dbname="factcab.fecha" )
 column=(type=decimal(2) updatewhereclause=yes name=vtemp dbname="factcab.vtemp" )
 column=(type=char(10) updatewhereclause=yes name=estado dbname="factcab.estado" )
 column=(type=char(1) updatewhereclause=yes name=selecc dbname="selecc" values="	1/	0" )
 retrieve="SELECT 
	factcab.nradica, factcab.clugar_rad, factcab.TIPO_RAD, factcab.nfact, factcab.clugar, 
	factcab.TIPO, factcab.fecha, factcab.vtemp, factcab.estado, '' AS Selecc
FROM 
	factcab LEFT JOIN ripsradica_resta ON (factcab.TIPO_RAD = ripsradica_resta.TIPO) AND (factcab.TIPO = ripsradica_resta.TIPO_FACT) 
	AND (factcab.clugar = ripsradica_resta.clugar_fact) AND (factcab.nfact = ripsradica_resta.nfact) AND (factcab.clugar_rad = ripsradica_resta.clugar) 
	AND (factcab.nradica = ripsradica_resta.num_radicacion)
WHERE
	(((factcab.nradica)=:nrad) AND ((factcab.clugar_rad)=:clug_rad) AND ((factcab.TIPO_RAD)=:tipo_rad) 
	AND ((ripsradica_resta.nfact) Is Null));
" arguments=(("nrad", number),("clug_rad", string),("tipo_rad", string))  sort="nfact A clugar A fecha A " )
