 retrieve="SELECT
	0 AS esco, 0 AS al_segun, hospadmi.NH, hospadmi.clugar, hospadmi.CodTIngre, hospadmi.Estado, 
	hospadmi.Contador, hospadmi.clugar_his, hospadmi.FechaIngreso, hospadmi.HoraIngreso, hospadmi.en_observa, 
	historial.consec_soat, historial.clugar_soat, hospadmi.CEmp, hospadmi.CContrato, contratos_a.SegRespon, 
	contratos_a.ContSegRespon, contratos_b.DesContrato as DesContSeg,contratos_a.descontrato as Descont, 
	TipoIngreso.factu_sinsal, contratos_a.SOAT,hospadmi.autoriza, contratos_a.Estado AS estado_cont, 
	contratos_b.Estado AS estado_cont_seg, hospadmi.DiagIngre,TIPOCONTRATO.TIPO, TIPOCONTRATO_1.TIPO AS tipo_fac_seg
FROM
	(((((hospadmi INNER JOIN TipoIngreso ON hospadmi.CODTINGRE = TipoIngreso.CODTINGRE) 
	INNER JOIN historial ON (hospadmi.CLUGAR_HIS = historial.CLUGAR) AND (hospadmi.CONTADOR = historial.CONTADOR)) 
	INNER JOIN contratos AS contratos_a ON (hospadmi.CCONTRATO = contratos_a.CODCONTRATO) AND (hospadmi.CEMP = contratos_a.CODEMP)) 
	INNER JOIN TIPOCONTRATO ON contratos_a.CTCONTRA = TIPOCONTRATO.CTCONTRA) 
	LEFT JOIN contratos AS contratos_b ON (contratos_a.CONTSEGRESPON = contratos_b.CODCONTRATO) AND (contratos_a.SEGRESPON = contratos_b.CODEMP)) 
	LEFT JOIN TIPOCONTRATO AS TIPOCONTRATO_1 ON contratos_b.CTCONTRA = TIPOCONTRATO_1.CTCONTRA	
WHERE 
	(((hospadmi.Estado)<='2') AND ((hospadmi.TipoDoc)=:tdoc) AND ((hospadmi.Documento)=:docu))
" arguments=(("tdoc", string),("docu", string))  sort="nh D " )
