 retrieve="SELECT 
	empresa.DesEmp, contratos.DesContrato, emppac.Carnet, tipoafil.DesTA, emppac.TipoDoc, emppac.Documento, emppac.CodEmp, 
	emppac.CodContrato, emppac.CodTa, contratos.SegRespon, contratos.ContSegRespon, contratos.TAfil, contratos.Estado, 
	estratos.DesEstrato, pacientes.Estrato, contratos.TopeCopago, contratos.TopeCoutaRecu, contratos.VtopeFijo, contratos.SOAT, 
	contratos.catastrofe, contratos.iva, contratos.monto_hasta, contratos.Acumulado, contratos.chequea_base, pacientes.FNacimiento, 
	pacientes.Sexo, TIPOCONTRATO.TIPO
FROM 
((((((
(
	SELECT DISTINCT 
		FactCpo.TipoDoc, FactCpo.Documento
	FROM 
		FactCpo
	WHERE 
		(((FactCpo.NFact)=:fact) AND ((FactCpo.clugar)=:clug_fact) AND ((FactCpo.TIPO)=:tipo_fact))) as
dist_pac INNER JOIN pacientes ON (dist_pac.Documento = pacientes.DOCUMENTO) AND (dist_pac.TipoDoc = pacientes.TIPODOC)) 
INNER JOIN estratos ON pacientes.ESTRATO = estratos.CODESTRATO) 
INNER JOIN emppac ON (pacientes.DOCUMENTO = emppac.DOCUMENTO) AND (pacientes.TIPODOC = emppac.TIPODOC)) 
INNER JOIN contratos ON (emppac.CODCONTRATO = contratos.CODCONTRATO) AND (emppac.CODEMP = contratos.CODEMP)) 
INNER JOIN empresa ON contratos.CODEMP = empresa.CODEMP) 
LEFT JOIN tipoafil ON emppac.CODTA = tipoafil.CODTA) 
INNER JOIN TIPOCONTRATO ON contratos.CTCONTRA = TIPOCONTRATO.CTCONTRA;



" arguments=(("fact", number),("clug_fact", string),("tipo_fact", string))  sort="estado D desemp A descontrato A " )
