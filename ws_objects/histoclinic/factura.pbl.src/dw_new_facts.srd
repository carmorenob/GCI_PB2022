 retrieve="  SELECT ripsrad_resta_det.num_radicacion,   
         ripsrad_resta_det.tipo,   
         ripsrad_resta_det.clugar,   
         ripsrad_resta_det.item,   
         ripsrad_resta_det.subitem,   
         ripsrad_resta_det.nfact,   
         ripsrad_resta_det.clugar_fact,   
         ripsrad_resta_det.tipo_fact  ,
         factcab.fecha,   
         factcab.estado
FROM 
	ripsrad_resta_det INNER JOIN factcab ON (ripsrad_resta_det.TIPO_FACT = factcab.TIPO) 
	AND (ripsrad_resta_det.CLUGAR_FACT = factcab.CLUGAR) 
	AND (ripsrad_resta_det.NFACT = factcab.NFACT)
WHERE 
	(((ripsrad_resta_det.num_radicacion)=:num_rad) AND ((ripsrad_resta_det.tipo)=:tipo_rad) 
	AND ((ripsrad_resta_det.clugar)=:clug_rad) AND ((ripsrad_resta_det.item)=:item));" update="ripsrad_resta_det" updatewhere=0 updatekeyinplace=no arguments=(("num_rad", number),("clug_rad", string),("tipo_rad", string),("item", number)) )
