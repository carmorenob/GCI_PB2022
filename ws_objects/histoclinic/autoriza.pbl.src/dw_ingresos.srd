 retrieve="SELECT 
	HospAdmi.NH, HospAdmi.clugar, HospAdmi.CodTIngre, HospAdmi.TipoDoc, HospAdmi.Documento, Pacientes.Nombre1, Pacientes.Nombre2, Pacientes.Apellido1, Pacientes.Apellido2, 
	HospAdmi.FechaIngreso, HospAdmi.HoraIngreso, HospAdmi.Estado, HospAdmi.estad_evol, hosp_triage.color, hosp_triage.n_color, 
	HospAdmi.Contador, HospAdmi.clugar_his, HospAdmi.cemp, Empresa.DesEmp, Empresa.fax, 
	Empresa.correo_urg, correo_cext, correo_otro,'' as proced,hospadmi.usuario,HospAdmi.CEmp 
FROM 
	(((HospAdmi INNER JOIN Pacientes ON (HospAdmi.Documento = Pacientes.Documento) AND 
	(HospAdmi.TipoDoc = Pacientes.TipoDoc)) INNER JOIN Empresa ON HospAdmi.CEmp = Empresa.CodEmp) LEFT JOIN 
	hosp_triage ON HospAdmi.Clasif = hosp_triage.cod_triage) left JOIN hosp_autoriza ON (HospAdmi.CLUGAR = hosp_autoriza.CLUGAR_ADM) 
	AND (HospAdmi.NH = hosp_autoriza.nh) AND (HospAdmi.CodTIngre = hosp_autoriza.codtingre)
WHERE 
	(((HospAdmi.CodTIngre)=:tipoIngreso) AND (HospAdmi.clugar=:lugar) AND ((HospAdmi.Estado)='1') AND ((HospAdmi.estad_evol)='1') AND ((hosp_autoriza.id_solicitud) Is Null)
	and  (hosp_triage.n_color is not null) AND (FechaIngreso between :FechaI and :fechaF	))" arguments=(("tipoIngreso", string),("lugar", string),("FechaI", datetime),("FechaF", datetime))  sort="fechaingreso A horaingreso A " )
