 retrieve="SELECT 
	0 AS selec, OSCuerpo.Contador, OSCuerpo.clugar, OSCuerpo.NSolicitud, OSCuerpo.Item, OSCuerpo.CodProced, 
	OSCuerpo.c_medica, OSCuerpo.solicitada, OSCuerpo.entregada, OSCuerpo.suministrada, 
	Proced.Descripcion, Medicamentos.Medicamento,0 as NUMERO_Qx, null CLUGAR_qx,0 as CONSECUTIVO_Qx,1 as tipo,
	proced.cod_cups,proced.cod_version
FROM 
	(((OSCuerpo LEFT JOIN Proced ON OSCuerpo.CodProced = Proced.CodProced) 
	LEFT JOIN Medicamentos ON OSCuerpo.c_medica = Medicamentos.c_medica) 
	LEFT JOIN hosp_autoriza_cpo ON (OSCuerpo.CLUGAR = hosp_autoriza_cpo.CLUGAR_OS) AND (OSCuerpo.Contador = hosp_autoriza_cpo.contador) 
	AND (OSCuerpo.NSolicitud = hosp_autoriza_cpo.nsolicitud) AND (OSCuerpo.Item = hosp_autoriza_cpo.item_os)) 
	INNER JOIN OSCABEZA ON (OSCuerpo.NSOLICITUD = OSCABEZA.NSOLICITUD) AND (OSCuerpo.CLUGAR = OSCABEZA.CLUGAR) 
	AND (OSCuerpo.CONTADOR = OSCABEZA.CONTADOR)
WHERE 
	(((OSCuerpo.Contador)=:contador) AND ((OSCuerpo.clugar)=:lugar) AND ((hosp_autoriza_cpo.contador) Is Null) 
	AND ((OSCuerpo.estado)<>'4') AND ((OSCABEZA.TIPO_ORDEN) Not In ('S','I')))
UNION ALL
SELECT 
	0 AS selec, QXCABACTO.CONTADOR, QXCABACTO.CLUGAR_HIS, 0 AS NSolicitud, 0 AS Item, 
	QXCPACTO.PROCCUPS,  null AS c_medica, 1 AS solicitada, 0 AS entregada, 0 AS suministrada, 
	proced.Descripcion,null AS Medicamento, QXCPACTO.NUMERO, QXCPACTO.CLUGAR, QXCPACTO.CONSECUTIVO,2 as tipo,
	proced.cod_cups,proced.cod_version
FROM 
	((((QXCABACTO INNER JOIN QXCPACTO ON (QXCABACTO.NUMERO_INGRE = QXCPACTO.NUMERO) AND (QXCABACTO.CLUGAR = QXCPACTO.CLUGAR))
	LEFT JOIN HOSP_AUTORIZA_CPO ON (QXCPACTO.NUMERO = HOSP_AUTORIZA_CPO.NUMERO_QX) AND (QXCPACTO.CLUGAR = HOSP_AUTORIZA_CPO.CLUGAR_QX) 
	AND (QXCPACTO.CONSECUTIVO = HOSP_AUTORIZA_CPO.CONSECUTIVO_QX)) INNER JOIN PROCED ON QXCPACTO.PROCCUPS = PROCED.CODPROCED) 
	LEFT JOIN OSCUERPO ON (QXCPACTO.NUMERO = OSCUERPO.NRO_QX) AND (QXCPACTO.CLUGAR = OSCUERPO.CLUGAR_QX) 
	AND (QXCPACTO.CONSECUTIVO = OSCUERPO.CONSEC_QX)) LEFT JOIN HOSP_AUTORIZA_CPO AS HOSP_AUTORIZA_CPO_1 
	ON (OSCUERPO.CONTADOR = HOSP_AUTORIZA_CPO_1.CONTADOR) AND (OSCUERPO.CLUGAR = HOSP_AUTORIZA_CPO_1.CLUGAR_OS) 
	AND (OSCUERPO.NSOLICITUD = HOSP_AUTORIZA_CPO_1.NSOLICITUD) AND (OSCUERPO.ITEM = HOSP_AUTORIZA_CPO_1.ITEM_OS)
WHERE
	(((QXCABACTO.CONTADOR)=:contador) AND ((QXCABACTO.CLUGAR_HIS)=:lugar) AND ((HOSP_AUTORIZA_CPO.NUMERO_QX) Is Null) 
	AND ((HOSP_AUTORIZA_CPO_1.AUTORIZACION) Is Null));
