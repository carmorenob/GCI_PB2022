table(column=(type=char(3) updatewhereclause=yes name=tipodoc dbname="tipodoc" )
 column=(type=char(15) updatewhereclause=yes name=documento dbname="documento" )
 column=(type=char(60) updatewhereclause=yes name=nombre1 dbname="nombre1" )
 column=(type=char(60) updatewhereclause=yes name=nombre2 dbname="nombre2" )
 column=(type=char(60) updatewhereclause=yes name=apellido1 dbname="apellido1" )
 column=(type=char(60) updatewhereclause=yes name=apellido2 dbname="apellido2" )
 column=(type=long updatewhereclause=yes name=edad dbname="edad" )
 column=(type=char(1) updatewhereclause=yes name=sexo dbname="sexo" )
 column=(type=decimal(6) updatewhereclause=yes name=hgbl dbname="hgbl" )
 column=(type=char(8190) updatewhereclause=no name=alerta dbname="alerta" )
 retrieve="SELECT
	tipodoc, documento,nombre1,nombre2,apellido1,apellido2,
	edad,sexo,hgbl,
	case when 
		hgbl=999 or hgbl=98
	then
		'Sin dato'
	else
		case when 
			hgbl<12 and sexo='F'
		then
			'Anemia'
		else
			case when 
				hgbl>=12 and sexo='F'
			then
				'No Anemia'
			else
				case when 
					hgbl<13 and sexo='M'
				then
					'Anemia'
				else
					case when 
						hgbl>=13 and sexo='M'
					then
						'No Anemia'
					else
						'Sin dato'
					end
				end
			end
		end
	end as Alerta
FROM
 (	
	SELECT 
		pacientes_cohortes.tipodoc, pacientes_cohortes.documento, 
		pacientes.nombre1, pacientes.nombre2, 
		pacientes.apellido1, pacientes.apellido2,
		f_edad('yy',pacientes.fnacimiento,ahora()) as edad,
		pacientes.sexo,
		case when 
		  f_trae_ultimo_result2_apdx(pacientes.tipodoc,pacientes.documento,'902213',1)= 0 
		then
			999 
		else 
			f_trae_ultimo_result2_apdx(pacientes.tipodoc,pacientes.documento,'902213',1)
		end as hgbl

  FROM 
		(pacientes_cohortes 
		INNER JOIN pacientes_cohortes_historial ON pacientes_cohortes.id_cohorte_pac = pacientes_cohortes_historial.id_cohorte_pac) 
		INNER JOIN pacientes ON (pacientes_cohortes.documento = pacientes.documento) AND (pacientes_cohortes.tipodoc = pacientes.tipodoc)
 WHERE 
		(((pacientes_cohortes.id_grupoetareo)='5') 
		AND ((pacientes_cohortes.fecha_inicio) between :FechaI and :FechaF)
		AND ((pacientes_cohortes_historial.clugar) Like :lugar)	 
		AND ((pacientes_cohortes.estado)='1'))
) as tar_calc" arguments=(("FechaI", datetime),("FechaF", datetime),("lugar", string))  sort="alerta A " )
