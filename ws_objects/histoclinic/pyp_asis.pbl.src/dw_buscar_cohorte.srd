table(column=(type=char(3) updatewhereclause=yes name=codgetareo dbname="grupoetareo.codgetareo" dbalias=".codgetareo" )
 column=(type=char(50) updatewhereclause=yes name=desgetareo dbname="grupoetareo.desgetareo" dbalias=".desgetareo" )
 column=(type=long updatewhereclause=yes name=id_trigger dbname="pm_cohortes_triggers.id_trigger" dbalias=".id_trigger" )
 column=(type=char(1) updatewhereclause=yes name=tipo_pro dbname="pm_cohortes_triggers.tipo_pro" dbalias=".tipo_pro" )
 column=(type=long updatewhereclause=yes name=consecampo dbname="pm_cohortes_triggers.consecampo" dbalias=".consecampo" )
 column=(type=char(100) updatewhereclause=yes name=modat dbname="pm_cohortes_triggers.modat" dbalias=".modat" )
 column=(type=number updatewhereclause=yes name=modan dbname="pm_cohortes_triggers.modan" dbalias=".modan" )
 retrieve="SELECT DISTINCT 
	grupoetareo.codgetareo, grupoetareo.desgetareo , pm_cohortes_triggers.id_trigger,
	pm_cohortes_triggers.tipo_pro,pm_cohortes_triggers.consecampo,pm_cohortes_triggers.modat,
	pm_cohortes_triggers.modan
FROM 
	grupoetareo INNER JOIN pm_cohortes_triggers 
	ON grupoetareo.codgetareo = pm_cohortes_triggers.id_grupoetareo
WHERE 
	(((pm_cohortes_triggers.id_diag)=:as_iddiag) AND ((:as_iddiag)<>'' And (:as_iddiag) Is Not Null) 
	AND ((pm_cohortes_triggers.estado)='1') 
	AND ((grupoetareo.estado)='1') AND ((grupoetareo.sexo)='0' Or (grupoetareo.sexo)=:as_sexo) 
	AND ((:ai_edad_dias) Between grupoetareo.edmin And grupoetareo.edmax)) 
	OR 
	(((pm_cohortes_triggers.estado)='1') AND ((grupoetareo.estado)='1') AND ((grupoetareo.sexo)='0' Or (grupoetareo.sexo)=:as_sexo) 
	AND ((:ai_edad_dias) Between grupoetareo.edmin And grupoetareo.edmax) AND ((pm_cohortes_triggers.codproced)=:as_codproced) 
	AND ((:as_codproced)<>'' And (:as_codproced) Is Not Null));" arguments=(("as_sexo", string),("ai_edad_dias", number),("as_iddiag", string),("as_codproced", string)) )
