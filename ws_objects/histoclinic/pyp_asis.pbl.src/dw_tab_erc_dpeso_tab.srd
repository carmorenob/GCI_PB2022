table(column=(type=char(3) updatewhereclause=yes name=tipodoc dbname="tipodoc" )
 column=(type=char(15) updatewhereclause=yes name=documento dbname="documento" )
 column=(type=char(60) updatewhereclause=yes name=nombre1 dbname="nombre1" )
 column=(type=char(60) updatewhereclause=yes name=nombre2 dbname="nombre2" )
 column=(type=char(60) updatewhereclause=yes name=apellido1 dbname="apellido1" )
 column=(type=char(60) updatewhereclause=yes name=apellido2 dbname="apellido2" )
 column=(type=datetime updatewhereclause=yes name=fnacimiento dbname="fnacimiento" )
 column=(type=decimal(6) updatewhereclause=yes name=talla dbname="talla" )
 column=(type=decimal(6) updatewhereclause=yes name=peso dbname="peso" )
 column=(type=decimal(6) updatewhereclause=yes name=imc dbname="imc" )
 column=(type=char(8190) updatewhereclause=no name=descrip dbname="descrip" )
 retrieve="SELECT
	tipodoc, documento,nombre1,nombre2,apellido1,apellido2,
	fnacimiento,
	talla, peso,
	CASE WHEN 
		talla =0 or peso=0
	THEN 
		0
	else
		(peso/(talla/100 * talla/100))
	end as IMC ,
	CASE WHEN 
		talla =0 or peso=0
	THEN 
		'Sin dato'
	else
		CASE WHEN 
			(peso/(talla/100 * talla/100))< 20
		then
			'Bajo peso'
		else
			CASE WHEN 
				(peso/(talla/100 * talla/100))>= 20 and
				(peso/(talla/100 * talla/100))<25
			then
				'Peso normal'
			else
				CASE WHEN 
					(peso/(talla/100 * talla/100))>= 25 and
					(peso/(talla/100 * talla/100))<30
				then
					'Sobrepeso'
				else
					CASE WHEN 
						(peso/(talla/100 * talla/100))>= 30 and
						(peso/(talla/100 * talla/100))<35
					then
						'Obesidad I'
					else
						CASE WHEN 
							(peso/(talla/100 * talla/100))>= 35 and
							(peso/(talla/100 * talla/100))<40
						then
							'Obesidad II'
						else
							'Obesidad extrema'
						end
					end
				end 
			end
		end
	end as descrip
FROM
 (	
	SELECT 
		pacientes_cohortes.tipodoc, pacientes_cohortes.documento, 
		pacientes.nombre1, pacientes.nombre2, 
		pacientes.apellido1, pacientes.apellido2,
		pacientes.fnacimiento,
		f_trae_ultimo_hc_numero_per(pacientes_cohortes.tipodoc, pacientes_cohortes.documento,'30', :fechai,:fechaf) as peso,
		f_trae_ultimo_hc_numero_per(pacientes_cohortes.tipodoc, pacientes_cohortes.documento,'32',:fechai,:fechaf) as talla
	FROM 
		(pacientes_cohortes 
		LEFT JOIN pacientes_cohortes_historial ON pacientes_cohortes.id_cohorte_pac = pacientes_cohortes_historial.id_cohorte_pac) 
		INNER JOIN pacientes ON (pacientes_cohortes.documento = pacientes.documento) AND (pacientes_cohortes.tipodoc = pacientes.tipodoc)	
	WHERE 
		(((pacientes_cohortes.id_grupoetareo)='5') 
		AND ((pacientes_cohortes.fecha_inicio) between :fechai and :fechaf)
		AND ((pacientes_cohortes_historial.clugar) Like :lugar)	 
		AND ((pacientes_cohortes.estado)='1'))
) as IMC_calc" arguments=(("FechaI", datetime),("FechaF", datetime),("lugar", string)) )
