table(column=(type=char(120) updatewhereclause=yes name=desemp dbname="desemp" )
 column=(type=char(7) updatewhereclause=yes name=codsuper dbname="codsuper" )
 column=(type=decimal(0) updatewhereclause=yes name=ctos dbname="ctos" )
 column=(type=long updatewhereclause=yes name=esc dbname="esc" values="Si	1/Si	0" )
 column=(type=datetime updatewhereclause=yes name=fecha_cargue dbname="fecha_cargue" )
 column=(type=decimal(0) updatewhereclause=yes name=yas dbname="yas" values="Si	1/Si	0" )
 retrieve="SELECT
	totales.desemp,totales.codsuper,
	totales.ctos,totales.esc,totales.fecha_cargue,
	case when totales.ya<>0 then 1 else ya end as yas

FROM
(
	SELECT 
		empresa.desemp, empresa_rips.codsuper, 
		Count(maestro_afiliacion.tipodoc) AS ctos, 0 as esc,
		maestro_afiliacion.fecha_cargue,
		case when count(tt_trabajo_202.documento) is null then 0 else count(tt_trabajo_202.documento) end as ya
	FROM 
		(afiliados.pm_maestro_afiliacion AS maestro_afiliacion 
		INNER JOIN empresa_rips ON maestro_afiliacion.eapb = empresa_rips.codsuper) 
		INNER JOIN empresa ON empresa_rips.codemp = empresa.codemp
		LEFT JOIN afiliados.tt_trabajo_202 AS tt_trabajo_202 
		ON (tt_trabajo_202.documento = maestro_afiliacion.documento) 
		AND (tt_trabajo_202.tipodoc = maestro_afiliacion.tipodoc)
	GROUP BY 
		empresa.desemp, empresa_rips.codsuper,maestro_afiliacion.fecha_cargue
) as TOTALES"  sort="desemp A ctos A " )
