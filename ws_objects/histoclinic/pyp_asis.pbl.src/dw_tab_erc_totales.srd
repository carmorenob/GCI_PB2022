table(column=(type=decimal(0) updatewhereclause=yes name=total dbname="total" )
 column=(type=decimal(0) updatewhereclause=yes name=hta dbname="hta" )
 column=(type=decimal(0) updatewhereclause=yes name=dm dbname="dm" )
 column=(type=decimal(0) updatewhereclause=yes name=sindx dbname="sindx" )
 retrieve="SELECT 
	sum(1) as Total,
	sum(CASE WHEN capdiags.codcap='I10' then 1 else 0 end) as HTA,
	sum(CASE WHEN capdiags.codcap in ('E10','E11','E12','E13','E14') then 1 else 0 end) as DM,
	sum(CASE WHEN capdiags.codcap is null then 1 else 0 end) as SINDX
FROM 
	(((pacientes_cohortes 
	INNER JOIN pacientes_cohortes_historial ON pacientes_cohortes.id_cohorte_pac = pacientes_cohortes_historial.id_cohorte_pac) 
	LEFT JOIN pm_cohortes_triggers ON pacientes_cohortes_historial.id_trigger = pm_cohortes_triggers.id_trigger) 
	LEFT JOIN diags ON pm_cohortes_triggers.id_diag = diags.codgeral) 
	LEFT JOIN capdiags ON (diags.ccap = capdiags.codcap) AND (diags.c_version = capdiags.c_version)
WHERE 
	(((pacientes_cohortes.id_grupoetareo)='5') 
	AND ((pacientes_cohortes.fecha_inicio)<=:cohorte)
	AND ((pacientes_cohortes_historial.clugar) Like :lugar)	 
	AND ((pacientes_cohortes_historial.consecutivo)=(
		SELECT Min(pch.consecutivo) AS MínDeconsecutivo
		FROM pacientes_cohortes_historial AS pch
		WHERE (((pch.id_cohorte_pac)=pacientes_cohortes.id_cohorte_pac))
	))
	AND ((pacientes_cohortes.estado)='1'));
" arguments=(("cohorte", datetime),("lugar", string)) )
