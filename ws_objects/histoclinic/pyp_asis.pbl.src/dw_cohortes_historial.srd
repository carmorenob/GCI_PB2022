 retrieve="SELECT 
	pacientes_cohortes_historial.id_cohorte_pac, 
	pacientes_cohortes_historial.consecutivo, 
	pacientes_cohortes_historial.id_trigger, 
	pacientes_cohortes_historial.contador, 
	pacientes_cohortes_historial.clugar, 
	pacientes_cohortes_historial.fecha, 
	pacientes_cohortes_historial.observaciones,
	pm_cohortes_triggers.id_diag, 
	diags.desdiag, 
	pm_cohortes_triggers.codproced, 
	proced.descripcion,
	diags.cod_rips
FROM ((pm_cohortes_triggers 
	INNER JOIN pacientes_cohortes_historial 
		ON pm_cohortes_triggers.id_trigger = pacientes_cohortes_historial.id_trigger) 
	LEFT JOIN proced ON pm_cohortes_triggers.codproced = proced.codproced) 
	LEFT JOIN diags ON pm_cohortes_triggers.id_diag = diags.codgeral
WHERE pacientes_cohortes_historial.id_cohorte_pac=:id" update="pacientes_cohortes_historial" updatewhere=0 updatekeyinplace=yes arguments=(("id", number))  sort="id_cohorte_pac D " )
