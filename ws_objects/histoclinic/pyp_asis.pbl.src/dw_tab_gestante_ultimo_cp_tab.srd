table(column=(type=char(3) updatewhereclause=yes name=tipodoc dbname="hclin_registro.tipodoc" dbalias=".tipodoc" )
 column=(type=char(15) updatewhereclause=yes name=documento dbname="hclin_registro.documento" dbalias=".documento" )
 column=(type=char(60) updatewhereclause=yes name=nombre1 dbname="hclin_registro.nombre1" dbalias=".nombre1" )
 column=(type=char(60) updatewhereclause=yes name=nombre2 dbname="hclin_registro.nombre2" dbalias=".nombre2" )
 column=(type=char(60) updatewhereclause=yes name=apellido1 dbname="hclin_registro.apellido1" dbalias=".apellido1" )
 column=(type=char(60) updatewhereclause=yes name=apellido2 dbname="hclin_registro.apellido2" dbalias=".apellido2" )
 column=(type=char(8190) updatewhereclause=no name=ultimocp dbname="ultimocp" )
 retrieve="SELECT 
	pacientes.tipodoc, pacientes.documento, 
	pacientes.nombre1, pacientes.nombre2, 
	pacientes.apellido1, pacientes.apellido2,
	cast(year(hclin_registro.fecha_reg) as varchar)||
	case when month(hclin_registro.fecha_reg)<10 then
		'-0' || cast(month(hclin_registro.fecha_reg) as varchar)
	else
		'-' || cast(month(hclin_registro.fecha_reg) as varchar)
	end as ultimoCP
FROM 
	((pacientes_cohortes 
	 INNER JOIN historial ON (pacientes_cohortes.documento = historial.documento) 
	 AND (pacientes_cohortes.tipodoc = historial.tipodoc)) 
	 INNER JOIN pacientes ON (pacientes_cohortes.documento = pacientes.documento) 
	 AND (pacientes_cohortes.tipodoc = pacientes.tipodoc)) 
	 INNER JOIN hclin_registro ON (historial.clugar = hclin_registro.clugar) 
	 AND (historial.contador = hclin_registro.contador)
WHERE 
	( ((pacientes_cohortes.id_grupoetareo)='4') 
	AND ((hclin_registro.codplantilla)='C9' Or (hclin_registro.codplantilla)='C10') 
	AND ((historial.fecha) Between :fechaI And :fechaF)
	AND ((hclin_registro.fecha_reg)=
		  (	SELECT 
		   		Max(hcr.fecha_reg) 
			FROM 
		   		(pacientes_cohortes AS pc 
				INNER JOIN historial AS historialh ON (pc.documento = historialh.documento) 
				AND (pc.tipodoc = historialh.tipodoc)) 
		   		INNER JOIN hclin_registro AS hcr ON (historialh.clugar = hcr.clugar) 
		   		AND (historialh.contador = hcr.contador)

			WHERE 
		   		(((pc.id_grupoetareo)='4') 
				AND ((hcr.codplantilla)='C9' Or (hcr.codplantilla)='C10')
		   		AND (((pc.tipodoc)=pacientes.tipodoc) AND ((pc.documento)=pacientes.documento)))
			HAVING 
		   		(((Max(hcr.fecha_reg)) Between :fechaI And :fechaF))  
	)));
" arguments=(("FechaI", datetime),("FechaF", datetime))  sort="ultimocp A " )
