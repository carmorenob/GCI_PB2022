table(column=(type=number updatewhereclause=yes name=contador dbname="qxcita.contador" )
 column=(type=char(2) updatewhereclause=yes name=clugar dbname="qxcita.clugar" )
 column=(type=long updatewhereclause=yes name=nsolicitud dbname="qxcita.nsolicitud" )
 column=(type=char(255) updatewhereclause=yes name=diagnostico dbname="qxcita.diagnostico" )
 column=(type=datetime updatewhereclause=yes name=fecha dbname="qxcita.fecha" )
 column=(type=char(255) updatewhereclause=yes name=observaciones dbname="qxcita.observaciones" )
 column=(type=char(1) updatewhereclause=yes name=tipo_orden dbname="qxcita.tipo_orden" values="Estancia	H/Quirurgica	Q/" )
 column=(type=char(5) updatewhereclause=yes name=cemp dbname="qxcita.cemp" )
 retrieve="SELECT 
	oscabeza.contador, oscabeza.clugar, oscabeza.nsolicitud, 
	case when DIAGS.DESDIAG is null then oscabeza.diagnostico else DIAGS.DESDIAG end as diagnostico, 
	oscabeza.fecha, oscuerpo.observaciones,oscabeza.TIPO_ORDEN, HOSPADMI.CEMP
FROM 
	((((historial INNER JOIN oscabeza ON (historial.clugar = oscabeza.clugar) AND (historial.contador = oscabeza.contador)) 
	INNER JOIN oscuerpo ON (oscabeza.NSOLICITUD = oscuerpo.NSOLICITUD) AND (oscabeza.clugar = oscuerpo.clugar) 
	AND (oscabeza.contador = oscuerpo.contador)) INNER JOIN proced ON oscuerpo.codproced = proced.codproced) 
	LEFT JOIN (HOSPADMI LEFT JOIN DIAGS ON HOSPADMI.DIAGINGRE = DIAGS.CODGERAL) 
	ON (historial.CLUGAR = HOSPADMI.CLUGAR_HIS) AND (historial.CONTADOR = HOSPADMI.CONTADOR))
	 LEFT JOIN QXCITA ON (oscabeza.NSOLICITUD = QXCITA.NSOLICITUD) AND (oscabeza.CLUGAR = QXCITA.CLUGAR_OS) 
	AND (oscabeza.CONTADOR = QXCITA.CONTADOR)
WHERE 
	(((oscabeza.TIPO_ORDEN) In ('Q','H')) AND ((QXCITA.NCITA) Is Null) AND ((historial.tipodoc)=:tdoc) 
	AND ((historial.documento)=:docu) AND ((proced.tipoproc)='2') AND ((oscabeza.ESTADO)='1') AND ((oscuerpo.ESTADO)='1'));
" arguments=(("tdoc", string),("docu", string)) )
