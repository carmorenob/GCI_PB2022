table(column=(type=char(20) updatewhereclause=yes name=nombre1 dbname="profe.nombre1" )
 column=(type=char(20) updatewhereclause=yes name=nombre2 dbname="profe.nombre2" )
 column=(type=char(20) updatewhereclause=yes name=apellido1 dbname="profe.apellido1" )
 column=(type=char(20) updatewhereclause=yes name=apellido2 dbname="profe.apellido2" )
 column=(type=char(10) updatewhereclause=yes key=yes name=codprof dbname="profe.codprof" )
 column=(type=datetime updatewhereclause=yes name=fecha dbname="profe.fecha" )
 column=(type=datetime updatewhereclause=yes name=hora dbname="profe.hora" )
 retrieve="SELECT  
	 profe.nombre1,profe.nombre2,profe.apellido1,profe.apellido2, profe.CODPROF,agendaprof.fecha,agendaprof.hora
FROM 
	(((agendaprof INNER JOIN consultorio ON agendaprof.CONSULT = consultorio.CODCONSULTORIO) 
	INNER JOIN espprof ON (agendaprof.CESP = espprof.CESP) AND (agendaprof.PROF = espprof.CODPROF)) 
	INNER JOIN especialidades ON espprof.CESP = especialidades.CODESP) 
	INNER JOIN profe ON espprof.CODPROF = profe.CODPROF
WHERE
	 (((agendaprof.codgc)=:codgc) AND ((agendaprof.fecha) Between :fec1 And :fec2) AND ((agendaprof.estcita) Is Null Or (agendaprof.estcita)='1') 
	AND ((consultorio.codlugar)=:clug) AND ((espprof.estado)='1') AND ((especialidades.ANESTESIA)='1')) AND (((agendaprof.consult)=:consul))
GROUP BY 
 	profe.nombre1,profe.nombre2,profe.apellido1,profe.apellido2, profe.CODPROF,agendaprof.fecha,agendaprof.hora

	" update="profe" updatewhere=0 updatekeyinplace=no arguments=(("codgc", string),("fec1", datetime),("fec2", datetime),("clug", string),("consul", string))  sort="nombre1 A nombre2 A apellido1 A apellido2 A " )
