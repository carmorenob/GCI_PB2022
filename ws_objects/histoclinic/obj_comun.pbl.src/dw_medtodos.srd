 column=(type=char(255) updatewhereclause=yes name=unidadmed dbname="medumedida.unidadmed" )
 column=(type=char(255) updatewhereclause=yes name=desp_udmidi dbname="medumedida.desp_udmidi" )
 column=(type=decimal(6) updatewhereclause=yes name=saldo dbname="medumedida.saldo" )
 retrieve="SELECT 
	medicamentos.medicamento, medicamentos.codclase, medicamentos.codsubclase, medicamentos.codgrupo, 
	medicamentos.cum_pes, medicamentos.codigo_unif, medicamentos.c_medica, medicamentos.pos, 
	medformaf.formafarma, medicamentos.estado, medicamentos.via, medicamentos.sexo, medicamentos.peso, 
	MEDUMEDIDA.unidadmed, medicamentos.volumen, medumedida_1.unidadmed, pm_unidadmdispensa.desp_udmidi,
	 kardex.saldo
FROM 
	(((((medicamentos INNER JOIN SUMGENERICO ON medicamentos.c_medica = SUMGENERICO.c_medica) 
	LEFT JOIN 
	(	SELECT SUMARTICULO.codgenerico, Sum(SUM_KARDEX.saldoactual) AS saldo
		FROM SUM_KARDEX INNER JOIN SUMARTICULO ON SUM_KARDEX.CODARTICULO = SUMARTICULO.codArticulo
		WHERE (((SUM_KARDEX.CODALMACEN) = :alm) or ((SUM_KARDEX.CODALMACEN) is null))
		GROUP BY SUMARTICULO.codgenerico
	) as kardex ON SUMGENERICO.codgenerico = kardex.codgenerico) 
	LEFT JOIN pm_unidadmdispensa ON medicamentos.umm = pm_unidadmdispensa.cod_udmidi) 
	LEFT JOIN medformaf ON medicamentos.codformaf = medformaf.codformaf) 
	LEFT JOIN MEDUMEDIDA ON medicamentos.cum_pes = MEDUMEDIDA.codumeddca) 
	LEFT JOIN medumedida AS medumedida_1 ON medicamentos.cum_vol = medumedida_1.codumeddca
WHERE 
	(medicamentos.sexo in('0',:sexo)) and medicamentos.estado='1' 
UNION
SELECT 
	medicamentos.medicamento, medicamentos.codclase, medicamentos.codsubclase, medicamentos.codgrupo, 
	medicamentos.cum_pes, medicamentos.codigo_unif, medicamentos.c_medica, medicamentos.pos, 
	medformaf.formafarma, medicamentos.estado, medicamentos.via, medicamentos.sexo, medicamentos.peso, 
	medumedida.unidadmed, medicamentos.volumen, medumedida_1.unidadmed, pm_unidadmdispensa.desp_udmidi,
	0 as saldo
FROM 
	((((medicamentos 
	LEFT JOIN medformaf ON medicamentos.codformaf = medformaf.codformaf) 
	LEFT JOIN sumgenerico ON medicamentos.c_medica = sumgenerico.c_medica) 
	LEFT JOIN medumedida ON medicamentos.cum_pes = medumedida.codumeddca) 
	LEFT JOIN medumedida AS medumedida_1 ON medicamentos.cum_vol = medumedida_1.codumeddca) 
	LEFT JOIN pm_unidadmdispensa ON medicamentos.umm = pm_unidadmdispensa.cod_udmidi
WHERE 
	(medicamentos.sexo in('0',:sexo) and medicamentos.vital='1') and medicamentos.estado='1'
" arguments=(("alm", string),("sexo", string))  sort="medicamento A " )
