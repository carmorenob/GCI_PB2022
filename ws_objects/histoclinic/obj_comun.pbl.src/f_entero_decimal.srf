global function double f_entero_decimal (decimal numero);long ll_mide,pos_punto
long ll_decim
 
 ll_mide= len(string(numero))
 pos_punto=pos(STRING(numero), '.' )
 ll_decim= long(right(string(numero), (ll_mide - pos_punto)) )
 
 return ll_decim
end function

