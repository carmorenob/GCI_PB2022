table(column=(type=char(2) updatewhereclause=yes name=clugar dbname="lugar.clugar" )
 column=(type=char(1) updatewhereclause=yes name=tipo_ingres dbname="lugar.tipo_ingres" values="CONSULTA EXTERNA	1/URGENCIAS	2/HOSPITALIZACION	3/AMBULATORIO	4/" )
 column=(type=char(15) updatewhereclause=yes name=codproced dbname="lugar.codproced" )
 column=(type=char(255) updatewhereclause=yes name=descripcion dbname="lugar.descripcion" )
 column=(type=decimal(0) updatewhereclause=yes name=cuantos dbname="cuantos" )
 column=(type=char(255) updatewhereclause=yes name=des_lug dbname="lugar.des_lug" )
 retrieve="SELECT 
	ServiciosADX.clugar, IngresoADX.tipo_ingres, proced.cod_cups as CodProced, Proced.Descripcion, Sum(ServiciosADX.Cantidad) AS cuantos,
	case when lugar.descripcion is null then
		case when lugar_1.descripcion is null then
			case when lugar_2.descripcion is null then 
				case when lugar_3.descripcion is null then '' else lugar_3.descripcion end
			else  lugar_2.descripcion end
			else lugar_1.descripcion end
			else lugar.descripcion end
		as des_lug	
FROM
	(((( (IngresoADX INNER JOIN ServiciosADX ON (IngresoADX.NIngreso = ServiciosADX.NIngreso) AND (IngresoADX.CodAADX = ServiciosADX.CodAADX) 
	AND (IngresoADX.clugar = ServiciosADX.clugar)) 
	INNER JOIN Proced ON ServiciosADX.CodProced = Proced.CodProced) 
	LEFT JOIN lugar ON ServiciosADX.clugar_cita = lugar.codlugar) 
	LEFT JOIN lugar AS lugar_1 ON ServiciosADX.clugar_fac = lugar_1.codlugar) 
	LEFT JOIN lugar AS lugar_2 ON ServiciosADX.clugar_os = lugar_2.codlugar) 
	LEFT JOIN lugar AS lugar_3 ON ServiciosADX.clugar_acto = lugar_3.codlugar
WHERE 
	(((IngresoADX.Fecha) Between :fecha1 And :fecha2) AND ((ServiciosADX.CodAADX)=:area) AND ((ServiciosADX.CodGC)=:grupo)) 
	AND (ServiciosADX.Estado in('5','7')) and IngresoADX.clugar like :lug
GROUP BY 
	ServiciosADX.clugar, IngresoADX.tipo_ingres, proced.cod_cups, Proced.Descripcion,
	case when lugar.descripcion is null then
		case when lugar_1.descripcion is null then
			case when lugar_2.descripcion is null then 
				case when lugar_3.descripcion is null then '' else lugar_3.descripcion end
			else  lugar_2.descripcion end
			else lugar_1.descripcion end
			else lugar.descripcion end
HAVING 
	(((IngresoADX.tipo_ingres) Like :tingres))"  arguments=(("area", string),("grupo", number),("tingres", string),("fecha1", datetime),("fecha2", datetime),("lug", string))  sort="des_lug A tipo_ingres A codproced A " )
