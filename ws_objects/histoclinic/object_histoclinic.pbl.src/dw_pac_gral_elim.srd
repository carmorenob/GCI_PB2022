 retrieve="  SELECT pacientes.tipodoc,   
         pacientes.documento,   
         pacientes.historia,   
         pacientes.nombre1,   
         pacientes.nombre2,   
         pacientes.apellido1,   
         pacientes.apellido2,   
         pacientes.fnacimiento,   
         pacientes.sexo,   
         estratos.desestrato,   
         pacientes.estrato,   
         pacientes.direccion,   
         pacientes.tel,   
         pacientes.fecha_captura,   
         pacientes.email
    FROM pacientes LEFT OUTER JOIN estratos ON pacientes.estrato = estratos.codestrato
   WHERE ( pacientes.tipodoc = :tipodoc ) AND  
         ( pacientes.documento = :docu )    
" update="pacientes" updatewhere=0 updatekeyinplace=no arguments=(("tipodoc", string),("docu", string)) )
