 column=(type=char(1) updatewhereclause=yes name=persoatiende dbname="profe.persoatiende" )
 retrieve=" SELECT  
	consultorio.descripcion ,           
	 profe.nombre1,profe.nombre2,profe.apellido1,profe.apellido2 ,       
	especialidades.desesp ,           agendaprof.consult ,           
	agendaprof.cesp ,           agendaprof.prof ,           agendaprof.duracion ,           
	espprof.cufuncional ,           espprof.cccosto ,           agendaprof.estcita ,           
	count(agendaprof.prof) as turnos,           profe.persoatiende     
FROM 
	(((agendaprof INNER JOIN consultorio ON agendaprof.CONSULT = consultorio.CODCONSULTORIO) 
	INNER JOIN espprof ON (agendaprof.PROF = espprof.CODPROF) AND (agendaprof.CESP = espprof.CESP)) 
	INNER JOIN profe ON espprof.CODPROF = profe.CODPROF) INNER JOIN especialidades ON espprof.CESP = especialidades.CODESP
WHERE
	 (((agendaprof.codgc)=:codgc) AND ((agendaprof.fecha) Between :fec1 And :fec2) 
	AND ((agendaprof.estcita) Is Null Or (agendaprof.estcita)='1') AND ((consultorio.codlugar)=:clug) 
	AND ((espprof.estado)='1'))
GROUP BY 
	consultorio.descripcion , 	 profe.nombre1,profe.nombre2,profe.apellido1,profe.apellido2 ,   
	especialidades.desesp , agendaprof.consult ,           agendaprof.cesp ,           agendaprof.prof ,           agendaprof.duracion ,           
	espprof.cufuncional ,           espprof.cccosto ,           agendaprof.estcita ,           profe.persoatiende     " arguments=(("codgc", string),("fec1", datetime),("fec2", datetime),("clug", string))  sort="cesp A consult A nombre1 A apellido1 A " )
