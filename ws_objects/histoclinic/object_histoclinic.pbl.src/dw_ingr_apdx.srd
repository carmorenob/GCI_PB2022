 retrieve="SELECT DISTINCT 
  ingresoadx.fecha,   
   ingresoadx.muestra,   
   ingresoadx.codaadx,   
   ingresoadx.ningreso,   
   ingresoadx.tipodoc,   
   ingresoadx.documento,   
   ingresoadx.contador,   
    ingresoadx.tipo_ingres,   
    ingresoadx.clugar,   
    ingresoadx.clugar_his,   
    ingresoadx.usuario,   
    hospadmi.nh,   
     hospadmi.estado,
	hospadmi.DiagIngre,
	OSCabeza.CodGeral
FROM 
	((((ingresoadx LEFT JOIN hospadmi ON (ingresoadx.clugar_his = hospadmi.clugar_his) AND (ingresoadx.contador = hospadmi.contador)) 
	LEFT JOIN ServiciosADX ON (ingresoadx.NIngreso = ServiciosADX.NIngreso) AND (ingresoadx.CodAADX = ServiciosADX.CodAADX) 
	AND (ingresoadx.clugar = ServiciosADX.clugar)) LEFT JOIN FactCpo ON (ServiciosADX.tipo_fac = FactCpo.tipo) 
	AND (ServiciosADX.NItem = FactCpo.NItem) AND (ServiciosADX.clugar_fac = FactCpo.clugar) AND (ServiciosADX.NFact = FactCpo.NFact)) 
	LEFT JOIN OSCuerpo ON (FactCpo.tipo = OSCuerpo.tipo_fact) AND (FactCpo.NFact = OSCuerpo.NFact) AND (FactCpo.clugar = OSCuerpo.clugar_fact) 
	AND (FactCpo.NItem = OSCuerpo.nitem_fact)) LEFT JOIN OSCabeza ON (OSCuerpo.Contador = OSCabeza.Contador) 
	AND (OSCuerpo.clugar = OSCabeza.clugar) AND (OSCuerpo.NSolicitud = OSCabeza.NSolicitud)
WHERE 
	( ingresoadx.codaadx = :codaadx ) AND  
     ( ingresoadx.tipodoc = :tipodoc ) AND  
     ( ingresoadx.documento = :doc ) AND  
      ( ingresoadx.tipo_ingres = :tingres )   
" update="ingresoadx" updatewhere=0 updatekeyinplace=yes arguments=(("codaadx", string),("tipodoc", string),("doc", string),("tingres", string))  sort="fecha D " )
