 retrieve="  SELECT  
	oscuerpo.contador ,           oscuerpo.clugar ,           oscuerpo.nsolicitud ,           oscuerpo.item ,           oscuerpo.solicitada ,           
	oscuerpo.observaciones ,           oscuerpo.registrado ,           medicamentos.pos ,           oscuerpo.tipo ,           '0' as escog,           
	oscuerpo.entregada ,           oscuerpo.suministrada ,           oscuerpo.facturar ,           oscuerpo.estado ,           oscuerpo.enfermeria ,           
	medicamentos.medicamento ,           oscuerpo.c_medica ,           medicamentos.rips ,           oscuerpo.devuelta ,
	 oscuerpo.administracion, oscuerpo.tiempo, oscuerpo.via, oscuerpo.frecuen
FROM 
oscuerpo ,           medicamentos     
WHERE ( oscuerpo.c_medica = medicamentos.c_medica ) and          ( oscuerpo.contador = :conta ) and          
( oscuerpo.nsolicitud = :nsolici ) and          ( oscuerpo.clugar = :clugar )   " update="oscuerpo" updatewhere=0 updatekeyinplace=no arguments=(("conta", number),("clugar", string),("nsolici", number))  sort="contador A clugar A nsolicitud A item A " )
