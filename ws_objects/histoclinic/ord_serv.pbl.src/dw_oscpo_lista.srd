FROM oscuerpo INNER JOIN proced ON oscuerpo.CodProced = proced.CodProced
WHERE (((oscuerpo.Contador)=:contador) AND ((oscuerpo.clugar)=:clug) AND ((oscuerpo.NSolicitud)=:nord))
